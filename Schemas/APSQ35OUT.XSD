<!--
     Schema for the AP Released Payments business object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="APReleasedPaymentsReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="APReleasedPaymentsReport" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BranchFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_BranchFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BranchFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_SupplierClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierClassFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BankFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_BankFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BankFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OprAccessControl" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Message" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Payments">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Suppliers" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SupplierHeader" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="APSMST_Description" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Invoices" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="InvoiceDetails" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="APSBRN_Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Invoice" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="InvoiceDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DiscountDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DueDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="InvoiceBalance" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_InvoiceBalance" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DiscountAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_DiscountAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="APSBNK_Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="GrossAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_GrossAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PaymentDiscount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PaymentDiscount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PaymentTaxWithheld" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PaymentTaxWithheld" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PaymentNetAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PaymentNetAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ManualCheckNumber" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ForeignCurrency" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TBLCUR_Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WithholdingTaxRequired" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="SupplierTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="SupplierInvoiceTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_SupplierInvoiceTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierDiscountTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_SupplierDiscountTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PaymentSupplierGrossTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_PaymentSupplierGrossTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PaymentSupplierDiscountTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_PaymentSupplierDiscountTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PaymentSupplierTaxWithheldTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_PaymentSupplierTaxWithheldTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PaymentSupplierNetTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_PaymentSupplierNetTotal" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="SupplierManualCheckTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="SupplierManualCheckTotal" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="ManualCheckNumber" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SupplierInvoiceTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_SupplierInvoiceTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SupplierDiscountTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_SupplierDiscountTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PaymentSupplierGrossTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PaymentSupplierGrossTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PaymentSupplierDiscountTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PaymentSupplierDiscountTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PaymentSupplierTaxWithheldTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PaymentSupplierTaxWithheldTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PaymentSupplierNetTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PaymentSupplierNetTotal" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CurrencyTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CurrencyTotalsDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Currency" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="CurrenciesUsed" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TBLCUR_Description" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CurrencyTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_CurrencyTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CurrencyDiscountTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_CurrencyDiscountTotal" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="CurrencyReportTotal" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="CurrencyReportTotal" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_CurrencyReportTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CurrencyDiscountReportTotal" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_CurrencyDiscountReportTotal" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ManualCheckSummary">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ManualCheckDetails" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ManualCheck" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="APSMST_Description" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="NumberOfSupplierManualChecks" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="ManualCheckSupplierTotal" type="xsd:double" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="ManualCheckTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="CheckTotal" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="ManualCheckNetAmount" type="xsd:double" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CompanyTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CompanyPaymentSupplierGrossTotal" type="xsd:double" minOccurs="0"/>
              <xsd:element name="Edited_CompanyPaymentSupplierGrossTotal" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyPaymentSupplierDiscountTotal" type="xsd:double" minOccurs="0"/>
              <xsd:element name="Edited_CompanyPaymentSupplierDiscountTotal" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyPaymentTaxWithheld" type="xsd:double" minOccurs="0"/>
              <xsd:element name="Edited_CompanyPaymentTaxWithheld" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyPaymentSupplierNetTotal" type="xsd:double" minOccurs="0"/>
              <xsd:element name="Edited_CompanyPaymentSupplierNetTotal" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CurrencyExceptionMessage" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TranslatedText">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Text_Release_payments" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Manual_check_list" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Void_check_list" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Hold_invoice_list" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Un_released_invoice_list" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_All" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Single" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
