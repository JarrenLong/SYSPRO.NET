<!-- 
     Schema for the AP Purchase Analysis History Business Object Out
-->

<xsd:schema id="APPurchaseAnalysisHistory" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="APPurchaseAnalysisHistory" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportSequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintIfHistoryZero" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TagFile" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BranchFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BranchFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierClassFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Suppliers">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="SupplierHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="APSMST_Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierOnHold" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HistoryAnalysis" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Class" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TBLAPC_Description" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="APSBRN_Description" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsCurrent" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev1" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev2" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev3" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev4" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev5" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev6" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev7" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev8" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev9" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev10" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierTotalsPrev11" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierPrevYearPurchases" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CompanyTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="SupplierTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SupplierTotalsCurrent" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev1" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev3" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev4" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev5" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev6" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev7" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev8" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev9" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev10" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierTotalsPrev11" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierPrevYearPurchases" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
