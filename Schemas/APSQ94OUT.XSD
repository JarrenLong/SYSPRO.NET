<!--
     Schema for the AP Payment Register Report Business Object
-->
<xsd:schema id="APPaymentRegisterReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="APPaymentRegisterReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReconcileReestablishedChecks" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReconcileReestablishedChecks" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PaymentRun" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PaymentRun" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MonthSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="YearSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseDateFilter" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_UseDateFilter" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PayMonthSelectionFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PayMonthSelectionFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PayMonthFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PayYearSelectionFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PayYearSelectionFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PayYearFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CheckRegisterFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CheckRegisterFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CheckRegisterFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_SupplierFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="RegisterEntries" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="RegisterEntriesHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RegisterNo" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PaymentType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_PaymentType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ForeignRegisterTypeMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RegisterType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RegisterTypeMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PaymentRun" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PostingMonth" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PostingYear" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BankDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CashGLCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WithholdingTax" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_WithholdingTax" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="TransactionProcesses" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="TransactionProcessing" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TransactionProcessingDetails" minOccurs="0">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="TransactionProcessingDetail" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="SupplierName" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Id" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="InvoiceDate" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="GrossAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="Edited_GrossAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="DiscountAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="Edited_DiscountAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="WithholdingTax" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="WithholdingTaxAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="Edited_WithholdingTaxAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="NetAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="Edited_NetAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="LocalCurrencyPayment" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="LocalCurrencyNumber" type="xsd:double" minOccurs="0"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="TransactionProcessingTotals" minOccurs="0">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="TotalMsg" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="GrossAmountTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_GrossAmountTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DiscountAmountTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_DiscountAmountTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TaxTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TaxTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="NetAmountTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_NetAmountTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DateTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="LocalCurrencyAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_LocalCurrencyAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="LocalCurrencyNumber" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="CheckNoMsg" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EFTNo" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TotalCheckAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TotalCheckAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CheckNo" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="RegisterEntriesTotal" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="RegisterTotalsMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NetLocalCurrMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NetLocalCurrencyAmount" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="GrossMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="GrossRegisterTotal" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_GrossRegisterTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DiscountMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DiscountRegisterTotal" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_DiscountRegisterTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TaxMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TaxRegisterTotal" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_TaxRegisterTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NoChecksMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NoOfChecks" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="NetMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NetRegisterTotal" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_NetRegisterTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalRegisterAmount" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_TotalRegisterAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JnlNoMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CashBookJnlNo" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
