<!--
     Schema for the AR Cash Journal Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="ARCashJournalReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="ARCashJournalReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="PrintJournalSummary" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PrintJournalSummary" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReprintJournalReport" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_ReprintJournalReport" type="xsd:string" minOccurs="0"/>
							<xsd:element name="MonthSelection" type="xsd:string" minOccurs="0"/>
							<xsd:element name="YearSelection" type="xsd:string" minOccurs="0"/>
							<xsd:element name="JournalNumberSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_JnlNumSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="JournalNumberSelectionFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_BranchSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchSelectionFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="JournalEntries">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="JournalEntriesHeader" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="JournalNo" type="xsd:double" minOccurs="0"/>
										<xsd:element name="PostingFinancialMonth" type="xsd:string" minOccurs="0"/>
										<xsd:element name="PostingFinancialYear" type="xsd:string" minOccurs="0"/>
										<xsd:element name="PostingGLPeriod" type="xsd:string" minOccurs="0"/>
										<xsd:element name="PostingGLYear" type="xsd:string" minOccurs="0"/>
										<xsd:element name="OperatorMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
										<xsd:element name="BankDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CurrencyDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Transactions" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Transaction" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="TransactionHeader" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="EntryNumber" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CustomerNo" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CustomerName" type="xsd:string" minOccurs="0"/>
																<xsd:element name="TransactionCode" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Translated_TransCode" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
																<xsd:element name="BranchDescription" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ReferenceCheck" type="xsd:string" minOccurs="0"/>
																<xsd:element name="PaymentDate" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Amount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedAmount" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LocalCurrencyAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedLocalCurrencyAmount" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
																<xsd:element name="AdjustmentDate" type="xsd:string" minOccurs="0"/>
																<xsd:element name="AdjustmentAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedAdjustmentAmount" type="xsd:string" minOccurs="0"/>
																<xsd:element name="InvBal" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedInvBal" type="xsd:string" minOccurs="0"/>
																<xsd:element name="InvoiceNo" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LocalAdjustmentAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedLocalAdjustmentAmount" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LedgerCode" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
																<xsd:element name="PaymentsNarration" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="TransactionDetails" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="TransactionDetail" minOccurs="0" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="BranchDescription" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="InvoiceNo" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="ReestablishedDate" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Reestablished" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Translated_Reestablished" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="InvoiceDate" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="ForeignInvBal" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedForeignInvBal" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="ForeignGross" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedForeignGross" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="ForeignDiscount" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedForeignDiscount" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="ForeignNet" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedForeignNet" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="ExchangeRate" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="LocalInvBal" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedLocalInvBal" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="LocalGross" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedLocalGross" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="LocalDiscount" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedLocalDiscount" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="LocalNet" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedLocalNet" type="xsd:string" minOccurs="0"/>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="BankChargeCurrency" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CurrencyConvRate" type="xsd:double" minOccurs="0"/>
																<xsd:element name="BankChargeAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedBankChargeAmount" type="xsd:string" minOccurs="0"/>
																<xsd:element name="PaymentsNarration" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="TransactionTotal" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="ForeignCurrencyTotal" minOccurs="0" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="Gross" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedGross" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Discount" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedDiscount" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Net" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedNet" type="xsd:string" minOccurs="0"/>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="LocalCurrencyTotal" minOccurs="0" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="Gross" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedGross" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Discount" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedDiscount" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Net" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedNet" type="xsd:string" minOccurs="0"/>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="CounterSalesMsg" type="xsd:string" minOccurs="0"/>
													<xsd:element name="CounterPayType" type="xsd:string" minOccurs="0"/>
													<xsd:element name="PaymentsNarration" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="JournalEntriesTotal" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="NoteAllValsLocalCurrMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="BalanceForward" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedBalanceForward" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AdjustmentCount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AdjustmentMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AdjustmentAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedAdjustmentAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ChecksCount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CheckMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ChecksAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedChecksAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DiscountsAllowedCount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DiscountMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DiscountAllowedAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedDiscountAllowedAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TotalPaymentsAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedTotalPaymentsAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ResultingBalance" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedResultingBalance" type="xsd:string" minOccurs="0"/>
										<xsd:element name="MiscCount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="MiscMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="MiscPaymentsAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedMiscPaymentsAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TotalTaxAdjMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AdjustmentTax" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedAdjustmentTax" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TotalTaxDiscMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DiscountTax" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedDiscountTax" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TotalBankChargeAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedTotalBankChargeAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="JnlControlErrorMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CtlTotDiffAdjMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ControlTotalDiffAdjAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedControlTotalDiffAdjAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CtlTotDiffChecksMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ControlTotalDiffChecksAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedControlTotalDiffChecksAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CtlTotDiffDiscMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ControlTotalDiffDiscAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedControlTotalDiffDiscAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CtlTotDiffResBalMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ControlTotalDiffResBalAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedControlTotalDiffResBalAmount" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
