        <xsd:element ref="IbtCustomer" minOccurs="0"/>
        <xsd:element ref="DateCustAdded" minOccurs="0"/>
        <xsd:element ref="BuyingGroup1" minOccurs="0"/>
        <xsd:element ref="BuyingGroup2" minOccurs="0"/>
        <xsd:element ref="BuyingGroup3" minOccurs="0"/>
        <xsd:element ref="BuyingGroup4" minOccurs="0"/>
        <xsd:element ref="BuyingGroup5" minOccurs="0"/>
        <xsd:element ref="SoldToAddr1" minOccurs="0"/>
        <xsd:element ref="SoldToAddr2" minOccurs="0"/>
        <xsd:element ref="SoldToAddr3" minOccurs="0"/>
        <xsd:element ref="SoldToAddr4" minOccurs="0"/>
        <xsd:element ref="SoldToAddr5" minOccurs="0"/>
        <xsd:element ref="SoldPostalCode" minOccurs="0"/>
        <xsd:element ref="ShipToAddr1" minOccurs="0"/>
