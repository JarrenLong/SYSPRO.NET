<!-- 
     Schema for the BOM Relationship Validation Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="BomRelationshipValidationQuery" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="BomRelationshipValidationQuery" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="OperationChainMissing" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_OpChainMissing" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleOperationChain" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleOpChain" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="SubcontractOperationRequired" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_SubcOpRequired" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ComponentStructureMissing" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_CompStructureMissing" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleComponentStructure" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleCompStructure" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ExcludedComponentHasIncludedParent" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_ExcCompHasInclParent" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleParentCategory" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleParentCat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleStatisticalItem" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleStatItem" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleBulkIssueFlag" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleBulkIssueFlag" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleTraceableInspectionFlag" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleTracInspFlg" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CannotBeATemporaryItem" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_CannotBeTempItem" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="StockOnHandNotEqualToZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_SOHNotZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="LeadTimeEqualToZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_LeadTimeZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DTFEqualToZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_DTFEqualZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="TraceableComponentsParentNonTraceable" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_TrcCompParNonTrc" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="SubcontractOperationWithZeroCostExists" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_SubcOpZeroCost" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleSubcontractOperationExists" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleSubcOp" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ParentMissing" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_ParentMissing" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleParent" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleParent" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleMPSItem" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleMPSItem" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleSerialTrackingFlag" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleSerTrkFlg" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="InadmissibleKitType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InadmissibleKitType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="StockOnOrderNotEqualToZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_StkOnOrdNotZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DTFGreaterThanLeadTime" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_DTFGreaterLT" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="WarehouseToUseNotFound" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_WhToUseNotFound" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="WarehouseToUseCostEqualToZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_WhToUseCostZero" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Co-productIsNotAttachedToNotionalPart" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_CoprdNotAttToNotional" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="NotionalPartHasNoParentInfoAttached" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_NotionalNoParentInfo" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="NotionalPartHasNoCo-productsAttached" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_NotionalHasNoCoprd" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="UnitOfMeasureRounding" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_UOMRounding" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="NotionalWarehouse" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_NotionalWh" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_StockCodeFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="RoutingFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_RoutingFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="RoutingFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="RelationshipValidation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="StockCodeHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="StockCode" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Description" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="StockingUom" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PartCategory" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Translated_PartCategory" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="MPSFlag" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Translated_MPSFlag" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="MakeToOrderFlag" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Translated_MakeToOrderFlag" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="MessageItems" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="MessageItem" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Message" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Translated_Message" type="xsd:string" minOccurs="0"></xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
