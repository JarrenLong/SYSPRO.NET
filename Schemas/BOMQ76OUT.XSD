<!-- 
     Schema for the BOM Structure Amendment Journal Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="BOMStructureAmendmentJournal" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="BOMStructureAmendmentJournal" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
                                      <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PrintAdditions" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PrintAdditions" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PrintChanges" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PrintChanges" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PrintDeletions" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PrintDeletions" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DeleteJournalFileAfterReport" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_DeleteJournalFileAfterReport" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PrintManufacturingUMQuantities" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PrintManufacturingUMQuantities" type="xsd:string" minOccurs="0"/>

						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="AmendmentDetails">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Amendment" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="AmendmentType" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_AmendmentType" type="xsd:string" minOccurs="0"/>
										<xsd:element name="JnlDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="JnlTime" type="xsd:string" minOccurs="0"/>
										<xsd:element name="JnlLine" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ParentPart" type="xsd:string" minOccurs="0"/>
										<xsd:element name="INVMST_ParentDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Release" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Route" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SequenceNum" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Component" type="xsd:string" minOccurs="0"/>
										<xsd:element name="INVMST_ComponentDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="INVMST_StockUom" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Operator" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Reason" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ComVersion" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ComRelease" type="xsd:string" minOccurs="0"/>
										<xsd:element name="StructOnDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="StructOffDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="OpOffsetFlag" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_OpOffsetFlag" type="xsd:string" minOccurs="0"/>
										<xsd:element name="OperationOffset" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_OperationOffset" type="xsd:string" minOccurs="0"/>
										<xsd:element name="QtyPer" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_QtyPer" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ScrapPct" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Edited_ScrapPct" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ScrapQty" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_ScrapQty" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ComponentAmendment" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="FieldDescription" type="xsd:string" minOccurs="0"/>
													<xsd:element name="OldFieldValue" type="xsd:string" minOccurs="0"/>
													<xsd:element name="NewFieldValue" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="FileDeleted">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Delete" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_Delete" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>



