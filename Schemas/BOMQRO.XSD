<!--
      Schema for BOM Route Query Business Object 
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="Query">
    <xsd:annotation>
      <xsd:documentation>This is the Root element. This business object will pass back all available routes. If alternate routes are not in use - the query will still return route '0'. The Route code, Route description, a flag to indicate if the route is ECC controlled, a flag to indicate if jobs are allowed for the route and a flag to indicate if a password is required will be returned. 
</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="false"/>
  </xsd:element>
</xsd:schema>
