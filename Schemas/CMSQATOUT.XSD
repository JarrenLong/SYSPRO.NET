<!-- 
     Schema for the Contact Activity Query Business Object
-->
<xsd:schema id="NewDataSet" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="ContactId" nillable="true">
    <xsd:complexType>
      <xsd:simpleContent msdata:ColumnName="ContactId_Text" msdata:Ordinal="1">
        <xsd:extension base="xsd:string">
          <xsd:attribute name="caption" type="xsd:string" />
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="QueryActivity">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Activities" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Contact" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ContactInfo" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element ref="ContactId" minOccurs="0" maxOccurs="unbounded" />
                          <xsd:element name="FullName" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="FullName_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="Title" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:attribute name="caption" type="xsd:string" />
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="FirstName" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="FirstName_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="MiddleName" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:attribute name="caption" type="xsd:string" />
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="LastName" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="LastName_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="Suffix" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:attribute name="caption" type="xsd:string" />
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="PreferredName" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="PreferredName_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="Supplier" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="Supplier_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="SupplierName" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="SupplierName_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
