                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="Source" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="Source_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="Attendees" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:attribute name="caption" type="xsd:string" />
