                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="StartTime" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="StartTime_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="EndDate" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="EndDate_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="EndTime" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="EndTime_Text" msdata:Ordinal="1">
