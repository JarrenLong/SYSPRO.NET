                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="CustomerName" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="CustomerName_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="caption" type="xsd:string" />
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Activity" minOccurs="0" maxOccurs="unbounded">
