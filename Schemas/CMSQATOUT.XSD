                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="PartialBody" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="PartialBody_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="FullBody" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="FullBody_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="Regarding" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="Regarding_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="UserField1" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:simpleContent msdata:ColumnName="Userfield1_Text" msdata:Ordinal="1">
                                <xsd:extension base="xsd:string">
                                  <xsd:attribute name="caption" type="xsd:string" />
                                </xsd:extension>
                              </xsd:simpleContent>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="UserField2" nillable="true" minOccurs="0" maxOccurs="unbounded">
