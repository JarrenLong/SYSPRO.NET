<!-- 
     Schema for the Contact Management Query Organisation List Business Object
-->
<xsd:schema id="NewDataSet" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="QueryList">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="QueryOptions" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReturnAll" type="xsd:string" minOccurs="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="List" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Header" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="FileName" type="xsd:string" minOccurs="0" />
                    <xsd:element name="Description" type="xsd:string" minOccurs="0" />
                    <xsd:element name="ListType" type="xsd:string" minOccurs="0" />
                    <xsd:element name="ListCode" type="xsd:string" minOccurs="0" />
                    <xsd:element name="LastChgOper" type="xsd:string" minOccurs="0" />
                    <xsd:element name="LastChgOperNam" type="xsd:string" minOccurs="0" />
                    <xsd:element name="LastChgDatTim" type="xsd:string" minOccurs="0" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="Language" type="xsd:string" />
      <xsd:attribute name="Language2" type="xsd:string" />
      <xsd:attribute name="CssStyle" type="xsd:string" />
      <xsd:attribute name="DecFormat" type="xsd:string" />
      <xsd:attribute name="DateFormat" type="xsd:string" />
      <xsd:attribute name="Role" type="xsd:string" />
      <xsd:attribute name="Version" type="xsd:string" />
      <xsd:attribute name="OperatorPrimaryRole" type="xsd:string" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="QueryList" />
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>