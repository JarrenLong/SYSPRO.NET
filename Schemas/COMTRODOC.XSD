<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2010 SYSPRO Ltd.--><!--     Schema for the Post Role Security Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="AccessControl">		<xsd:annotation>			<xsd:documentation>AccessControl structure</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence minOccurs="0">				<xsd:element ref="ConfiguredByRole" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="ActivitiesFields">		<xsd:annotation>			<xsd:documentation>ActivitiesFields structure</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence minOccurs="0">				<xsd:element ref="ConfiguredByRole" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="CompanyId" type="xsd:string">		<xsd:annotation>			<xsd:documentation>Should contain the Company code for the required company. Can be left blank and the current company will be used.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="ConfiguredByRole">		<xsd:annotation>			<xsd:documentation>When set to "Y" will configure the time by Role. Y - Yes, N - No.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="N"/>				<xsd:enumeration value="Y"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="OperationAccess">		<xsd:annotation>			<xsd:documentation>When set to "Y","S" or "N" will apply the setting for the specified role. Y - Allowed and Secured, S - Secured, N - Not  secured and not allowed.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="N"/>				<xsd:enumeration value="S"/>				<xsd:enumeration value="Y"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="PostRole">		<xsd:annotation>			<xsd:documentation>PostRole element, can occur multiple times</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="PostRoleId"/>				<xsd:element ref="UserInterface" minOccurs="0"/>				<xsd:element ref="eSignatures" minOccurs="0"/>				<xsd:element ref="ProgramAccess" minOccurs="0"/>				<xsd:element ref="ActivitiesFields" minOccurs="0"/>				<xsd:element ref="AccessControl" minOccurs="0"/>				<xsd:element ref="Workflow" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="PostRoleId" type="xsd:string">		<xsd:annotation>			<xsd:documentation>Must contain a valid role id.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="PostRoles">		<xsd:annotation>			<xsd:documentation>PostRoles RootNode</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="PostRole" maxOccurs="unbounded"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="ProgramAccess">		<xsd:annotation>			<xsd:documentation>ProgramAccess structure</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence minOccurs="0">				<xsd:element ref="ConfiguredByRole" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="SecureAllOperations">		<xsd:annotation>			<xsd:documentation>When set to "Y","S" or "N" will apply the setting to all workflow operations for the specified role. Y - Allowed and Secured, S - Secured, N - Not  secured and not allowed.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="N"/>				<xsd:enumeration value="Y"/>				<xsd:enumeration value="S"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="UserInterface">		<xsd:annotation>			<xsd:documentation>UserInterfaces structure</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence minOccurs="0">				<xsd:element ref="ConfiguredByRole" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Workflow">		<xsd:annotation>			<xsd:documentation>Workflow structure</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence minOccurs="0">				<xsd:element ref="ConfiguredByRole" minOccurs="0"/>				<xsd:element ref="SecureAllOperations" minOccurs="0"/>				<xsd:element ref="WorkflowRoleItem" minOccurs="0" maxOccurs="unbounded"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="WorkflowName" type="xsd:string">		<xsd:annotation>			<xsd:documentation>Must contain the name of the workflow.  The WorkflowName is case sensitive.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="WorkflowOperation">		<xsd:annotation>			<xsd:documentation>Must contain the name of the workflow.  The WorkflowOperation is case sensitive.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="ApproveCreditLimit"/>				<xsd:enumeration value="CreateCustomer"/>				<xsd:enumeration value="UpdateBranch"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="WorkflowRoleItem">		<xsd:annotation>			<xsd:documentation>WorkflowRoleItem element, can occur multiple times.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence minOccurs="0">				<xsd:element ref="CompanyId" minOccurs="0"/>				<xsd:element ref="WorkflowName" minOccurs="0"/>				<xsd:element ref="WorkflowOperation" minOccurs="0"/>				<xsd:element ref="OperationAccess" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="eSignatures">		<xsd:annotation>			<xsd:documentation>eSignatures structure</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence minOccurs="0">				<xsd:element ref="ConfiguredByRole" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element></xsd:schema>