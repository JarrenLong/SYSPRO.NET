<!--
     Schema for the Cash Book Distribution Business Object
-->
<xsd:schema id="CashBookGLDistributionReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="CashBookGLDistributionReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CurrentGLMonth" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CurrentGLYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="EntrySelectionBy" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_EntrySelectionBy" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReprintDistributionOptions" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_ReprintDisOption" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerPeriodSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_LedPerSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerPeriodSelectionFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerYearSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_LedYearSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerYearSelectionFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerEntriesGroupSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_LedEntGrpSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerEntriesGroupSelectionFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerJournalSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_LedJnlSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerJournalSelectionFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerCodeSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_LedCodeSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LedgerCodeSelectionFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BankSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_BankSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BankSelectionFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="EntriesItem">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="EntriesGroupHeader" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="PeriodFrom" type="xsd:string" minOccurs="0"/>
										<xsd:element name="PeriodTo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="YearFrom" type="xsd:string" minOccurs="0"/>
										<xsd:element name="YearTo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="EntriesGroup" type="xsd:string" minOccurs="0"/>
										<xsd:element name="FromEntriesGroup" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ToEntriesGroup" type="xsd:string" minOccurs="0"/>
										<xsd:element name="JournalNo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="FromJournalNo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ToJournalNo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="GlJournalEnNoDesc" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LedgerPeriod" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LedgerYear" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CompanyID" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
										<xsd:element name="BankDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="LedgerCodes" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="LedgerCode" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="LedgerCodeHeader" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="LedgerCode" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
																<xsd:element name="BankEntry" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Translated_BankEntry" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="LedgerCodeDetails" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="LedgerCodeDetail" minOccurs="0" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="Date" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="EntryType" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="AdjustmentType" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Check" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Journal" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Entry" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="NoteMsg" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="DebitAmount" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedDebit" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="CreditAmount" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedCredit" type="xsd:string" minOccurs="0"/>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="LedgerCodeTotal" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="TotalMsg" type="xsd:string" minOccurs="0"/>
																<xsd:element name="DebitAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedDebit" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CreditAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedCredit" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="BankTotal" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="TotalMsg" type="xsd:string" minOccurs="0"/>
																<xsd:element name="DebitAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedDebit" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CreditAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedCredit" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="EntriesGroupTotal" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="TotalMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DebitAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedDebit" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CreditAmount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedCredit" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Message" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
