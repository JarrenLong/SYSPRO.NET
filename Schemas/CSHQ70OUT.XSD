<!-- 
     Schema for the Cash Book Tax Report Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="CashBookTaxReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="CashBookTaxReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="TaxType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_TaxType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType_ReportName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DeleteTaxDistFiles" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DeleteTaxDistFiles" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TransactionDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_TransactionDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TransactionDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JournalFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JournalFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JournalFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TaxCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_TaxCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TaxCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PeriodFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PeriodFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PeriodFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="YearFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_YearFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="YearFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Tax">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Journals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="JournalHeader" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="PostingMonth" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PostingYear" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Journal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CanadianGstRequired" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_CanadianGstRequired_HeadingTax" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_CanadianGstRequired_HeadingTaxAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_CanadianGstRequired_HeadingTaxDistribution" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_CanadianGstRequired_HeadingTaxDistributionSummary" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_CanadianGstRequired_Sub_HeadingTaxAmount" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="TransactionDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="BankDetail" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="BankName" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="Distribution" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="DistributionDetails" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="EntryNumber" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Translated_TransactionType" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Translated_ReportTypeHeading" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="EntryDate" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="TransactionAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="Edited_TransactionAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="TotalTaxAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="Edited_TotalTaxAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Check" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Adjustment" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Translated_Adjustment" type="xsd:string" minOccurs="0"/>
		                          <xsd:element name="Report_SequencingFormula" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="TaxDistribution" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="TaxDetails" minOccurs="0" maxOccurs="unbounded">
                                              <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="TaxCode" type="xsd:string" minOccurs="0"/>
                                                  <xsd:element name="TaxableAmount" type="xsd:double" minOccurs="0"/>
                                                  <xsd:element name="Edited_TaxableAmount" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                              </xsd:complexType>
                                            </xsd:element>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="JournalTotal" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="TotalTransactionAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TotalTransactionAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TotalTaxAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TotalTaxAmount" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CompanyTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="WithdrawalTypeTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="WithdrawalTypeJournalTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TotalTransactionAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotalTransactionAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotalTaxAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotalTaxAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotalTaxableAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotalTaxableAmount" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="WithdrawalTaxCodeTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="WithdrawalTaxCode" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Code" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ADMTAX_Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CurTaxRate" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_CurTaxRate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TotalTransactionAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TotalTransactionAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TotalTaxAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TotalTaxAmount" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="DepositTypeTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="DepositTypeJournalTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TotalTransactionAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotalTransactionAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotalTaxAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotalTaxAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotalTaxableAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotalTaxableAmount" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="DepositTaxCodeTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="DepositTaxCode" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Code" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ADMTAX_Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CurTaxRate" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_CurTaxRate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TotalTransactionAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TotalTransactionAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TotalTaxAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TotalTaxAmount" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>

                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="TaxSummaryAllTransactions" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SummaryTaxCode" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Code" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ADMTAX_Description" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CurTaxRate" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_CurTaxRate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="OutputTax" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_OutputTax" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="InputTax" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_InputTax" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="FileDeleted">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CSHTDDDeleted" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CSHTDDDeleted" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CSHTDHDeleted" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CSHTDHDeleted" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
