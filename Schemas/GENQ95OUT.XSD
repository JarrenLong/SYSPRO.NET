<!-- 
     Schema for the GL Period End Worksheet
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="GlPeriodEndWorksheet" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="GlPeriodEndWorksheet" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportSequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportSequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AdjustmentToPrint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_AdjustmentToPrint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LedgerYearAndPeriod" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_LedgerYearAndPeriod" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LedgerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LedgerPeriod" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeAssets" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeAssets" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeCapital" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeCapital" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeLiabilities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeLiabilities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeExpense" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeExpense" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeRevenue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeRevenue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeInactiveAccounts" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeInactiveAccounts" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WorklinesToPrint" type="xsd:double" minOccurs="0"/>
              <xsd:element name="BlankLinesBetweenAccounts" type="xsd:double" minOccurs="0"/>
              <xsd:element name="GlCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_GlCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlGroupFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_GlGroupFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlGroupFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="LedgerItem">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="GlCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GENMST_Description" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AccountType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_AccountType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GENGRP_Description" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CurrentBalance" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_CurrentBalance" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Adjustments" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_Adjustments" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AdjustmentsJnl" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AdjustmentsType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AdjustedBalance" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_AdjustedBalance" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CompanyTotal">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="LastJournalNumber" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LedgerPeriod" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
