<!-- 
     Schema for the Asset Label Format Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="AssetLabelFormatReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="AssetLabelFormatReport" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="FormatNo" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Detail">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="LabelLength" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GapBetweenLabels" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LabelsAcrossPage" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LabelType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AssetCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AssetCodeWithAttribute" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AssetDescription" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IdCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IdCodeWithAttribute" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocationCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocationCodeWithAttribute" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocationDescription" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchCodeWithAttribute" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchDescription" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CostCentreCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CostCentreCodeWithAttribute" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CostCentreCodeDescription" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CapexCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CapexCodeWithAttribute" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CapexDescription" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ChassisNumber" type="xsd:string" minOccurs="0"/>
							<xsd:element name="EngineNumber" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CycleCountCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AgreementNumber" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AgreementCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Owner" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ResponsibleUser" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AssetGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AssetGroupDescription" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AssetType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AssetTypeDescription" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SupplierName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="TextFields" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="TextField" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="LabelText" type="xsd:string" minOccurs="0"/>
													<xsd:element name="LabelTextPosition" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
