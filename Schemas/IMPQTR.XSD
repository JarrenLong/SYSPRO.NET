<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2010 SYSPRO Ltd.--><!--      This is an example XML instance to demonstrate     use of the Tax return Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="Option">    <xsd:annotation>      <xsd:documentation>The parent option element</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Reprint"/>        <xsd:element ref="TaxPeriod"/>        <xsd:element ref="TaxYear"/>        <xsd:element name="TaxPeriodTo" minOccurs="0">          <xsd:annotation>            <xsd:documentation>Defines the tax period range for which the return is to be printed. If supplied it must be greater than TaxPeriod and it must be a valid period.</xsd:documentation>          </xsd:annotation>        </xsd:element>        <xsd:element name="TaxYearTo" minOccurs="0">          <xsd:annotation>            <xsd:documentation>Defines the tax year range for which the return is to be printed. If supplied it must be greate than TaxYear and it must be a valid period.</xsd:documentation>          </xsd:annotation>        </xsd:element>        <xsd:element ref="TaxCodesToOmit" minOccurs="0"/>        <xsd:element ref="ReportType" minOccurs="0"/>        <xsd:element ref="ReportSubmit" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Query">    <xsd:annotation>      <xsd:documentation>The root element</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Option"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="ReportSubmit" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Indicates whether the tax return is to be submitted. If set to 'Y' the Submit flag will be updated</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="ReportType" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Indicates how much detail should be returned. S - summary and D - Detail.  If no value is supplied then a default value of 'D' will be used. </xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Reprint" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Indicates whether the report will be a reprint. If no value is supplied then a default of 'N' will be used</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="TaxCodesToOmit" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Defines the tax code to be obmitted from the return</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="TaxPeriod" type="xsd:byte">    <xsd:annotation>      <xsd:documentation>Defines the tax period for which the return is to be printed</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="TaxYear" type="xsd:short">    <xsd:annotation>      <xsd:documentation>Defines the tax period for which the return is to be printed</xsd:documentation>    </xsd:annotation>  </xsd:element></xsd:schema>