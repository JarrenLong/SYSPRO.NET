                                            <xsd:element name="EditedLotSavedQty" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="LotCapturedQty" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="LotCapturedQtyUnits" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="EditedLotCapturedQty" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="LotVarianceQty" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="LotVarianceQtyUnits" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="EditedLotVarianceQty" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="LotVariancePercentage" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="LotUnitCost" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedLotUnitCost" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="LotSavedValue" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedLotSavedValue" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="LotCapturedValue" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedLotCapturedValue" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="LotVarianceValue" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedLotVarianceValue" type="xsd:string" minOccurs="0"/>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="SerialDetail" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="SerialItem" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Serial" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="SerStockCode" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="SerSavedOnHand" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="SerSavedOnHandUnits" type="xsd:string" minOccurs="0"/>
