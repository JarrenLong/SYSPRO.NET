                          <xsd:element name="EditedTotCapturedValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotVarianceValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedTotVarianceValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotVarianceFifo" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedTotVarianceFifo" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="FifoVariance" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedFifoVariance" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
