                                <xsd:element name="CapturedValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="EditedCapturedValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="VarianceValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="EditedVarianceValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="LotDetails" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="LotDetail" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="Lot" type="xsd:string" minOccurs="0"/>
