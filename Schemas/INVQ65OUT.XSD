              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Sequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Sequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SequenceDescription" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ValueStockAt" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ValueStockAt" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ValueFifo" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ValueFifo" type="xsd:string" minOccurs="0"/>
