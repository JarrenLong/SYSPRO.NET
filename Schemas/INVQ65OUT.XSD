              <xsd:element name="Translated_IncludeZeroSaveQty" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeQtyNotCaptured" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeQtyNotCaptured" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeBatchSerials" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeBatchSerials" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeManualSerials" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeManualSerials" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ExcludeZeroSerials" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ExcludeZeroSerials" type="xsd:string" minOccurs="0"/>
