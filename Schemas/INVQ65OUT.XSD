              <xsd:element name="WhTotQtyCaptured" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedWhTotQtyCaptured" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WhTotVarianceQty" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedWhTotVarianceQty" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WhTotSavedValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedWhTotSavedValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WhTotCapturedValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedWhTotCapturedValue" minOccurs="0"/>
              <xsd:element name="WhTotVarianceValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedWhTotVarianceValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TranslatedText" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Text_All" minOccurs="0"/>
              <xsd:element name="Text_____On_full_hold____" minOccurs="0"/>
              <xsd:element name="Text_____On_partial_hold____" minOccurs="0"/>
              <xsd:element name="Text_Yes" minOccurs="0"/>
              <xsd:element name="Text_No" minOccurs="0"/>
              <xsd:element name="Text_Stock_code" minOccurs="0"/>
              <xsd:element name="Text_Bin_location" minOccurs="0"/>
              <xsd:element name="Text_Cost_on_warehouse_file" minOccurs="0"/>
              <xsd:element name="Text_Cost_on_stock_take_file" minOccurs="0"/>
              <xsd:element name="Text_Variance" minOccurs="0"/>
              <xsd:element name="Text_Variance_" minOccurs="0"/>
              <xsd:element name="Text_FIFO_Variance" minOccurs="0"/>
              <xsd:element name="Text__cont_d" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
