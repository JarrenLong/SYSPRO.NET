                                      <xsd:element name="SerVersion" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="SerRelease" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="StockCodeTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TotOrigQtyOnHand" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="TotOrigQtyOnHandUnits" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedTotOrigQtyOnHand" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotQtyCaptured" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="TotQtyCapturedUnits" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedTotQtyCaptured" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotVarianceQty" type="xsd:double" minOccurs="0"/>
