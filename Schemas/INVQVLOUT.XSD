                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="WarehouseBinTotal" minOccurs="0">
                      <xsd:complexType>
                        <xsd:sequence minOccurs="0">
                          <xsd:element name="Value" type="xsd:string" minOccurs="0"/>
