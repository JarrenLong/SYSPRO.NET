              <xsd:element name="Translated_CheckNegativeAtWarehouse" type="xsd:string"/>
              <xsd:element name="ActualCosting" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ActualCosting" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_BinBucket" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_BinOr" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_BinTotal" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TranslatedWarehouseTotal" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Continued" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_GroupedBy" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="StockItem" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence minOccurs="0">
