                    <xsd:element name="EditedSQuantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Mass" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedMass" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="WarehouseTotal" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence minOccurs="0">
              <xsd:element name="WarehouseItemTotal" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence minOccurs="0">
                    <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CurrentInventoryValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedCurrentInventoryValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalMass" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedTotalMass" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HashQuantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SHashQuantity" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SHashQuantityUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSHashQuantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CostingMethod" type="xsd:string" minOccurs="0"/>
