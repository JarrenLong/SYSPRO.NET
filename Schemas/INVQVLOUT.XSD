                          <xsd:element name="EditedValue" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="WarehouseEccTotal" minOccurs="0">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Value" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Quantity" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedSQuantity" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="FifoBucketItem" minOccurs="0">
                      <xsd:complexType>
                        <xsd:sequence minOccurs="0">
                          <xsd:element name="FifoBucket" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyOnHand" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SQtyOnHand" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="SqtyOnHandUnits" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedSQtyOnHand" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="UnitCost" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedUnitCost" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LastReceiptDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LastIssueDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Value" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedValue" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="WarehouseFifoTotal" minOccurs="0">
                      <xsd:complexType>
                        <xsd:sequence minOccurs="0">
                          <xsd:element name="Value" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Quantity" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedSQuantity" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="BinItem" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence minOccurs="0">
                          <xsd:element name="BinLocation" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyOnHand" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SQtyOnHand" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="SQtyOnHandUnits" type="xsd:string" minOccurs="0"/>
