                    <xsd:element name="SHashQuantity" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SHashQuantityUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSHashQuantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalMass" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedTotalMass" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ProductClassTotal" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ProductClassItem" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ProductClass" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Value" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Quantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSQuantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Mass" type="xsd:string" minOccurs="0"/>
