              <xsd:element name="WarehouseFilterType" type="xsd:string"/>
              <xsd:element name="Translated_WarehouseFilterType" type="xsd:string"/>
              <xsd:element name="WarehouseFilterValue" type="xsd:string"/>
              <xsd:element name="IncludeZeroValueWarehouses" type="xsd:string"/>
              <xsd:element name="Translated_IncludeZeroValueWarehouses" type="xsd:string"/>
              <xsd:element name="Translated_IncludeZeroQtyCostWhs" type="xsd:string"/>
              <xsd:element name="Translated_IncludeZeroQtyWhs" type="xsd:string"/>
              <xsd:element name="IncludeProductClassDesc" type="xsd:string"/>
              <xsd:element name="Translated_IncludeProductClassDesc" type="xsd:string"/>
              <xsd:element name="EccItemsOnly" type="xsd:string"/>
              <xsd:element name="Translated_EccItemsOnly" type="xsd:string"/>
              <xsd:element name="IncludeZeroEccRevRel" type="xsd:string"/>
              <xsd:element name="Translated_IncludeZeroEccRevRel" type="xsd:string"/>
