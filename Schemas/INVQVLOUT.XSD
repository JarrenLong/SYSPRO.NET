              <xsd:element name="Translated_MultipleBinsPrintTotalsOnly" type="xsd:string"/>
              <xsd:element name="IncludeZeroBins" type="xsd:string"/>
              <xsd:element name="Translated_IncludeZeroBins" type="xsd:string"/>
