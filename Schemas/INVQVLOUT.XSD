                    <xsd:element name="Quantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSQuantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Mass" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedMass" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_TotalText" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
