                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="VersionRelease" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="WarehouseMass" type="xsd:string" minOccurs="0"/>
