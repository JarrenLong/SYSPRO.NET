                    <xsd:element name="EditedValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WarehouseMass" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedWarehouseMass" type="xsd:string" minOccurs="0"/>
