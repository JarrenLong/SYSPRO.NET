<!-- 
     Schema for the Selection Set Amendment Journal Query Business Object
-->
<xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="DeleteJournalFile" default="N">    <xsd:annotation>      <xsd:documentation>Indicates whether  the Selection Set amendment journal should be deleted after being printed. ( N - No and Y - Yes). If no value is supplied then a default of 'N' will be used. </xsd:documentation>      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Option">    <xsd:complexType mixed="false">      <xsd:sequence>        <xsd:element ref="Reprint"/>        <xsd:element ref="PrintSelectionSetAdditions"/>        <xsd:element ref="PrintSelectionSetChanges"/>        <xsd:element ref="PrintSelectionSetDeletions"/>        <xsd:element ref="DeleteJournalFile"/>        <xsd:element ref="XslStyleSheet"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="PrintSelectionSetAdditions" default="Y">    <xsd:annotation>      <xsd:documentation>Indicates whether  the Selection Set additions should be included. (Y - Yes and N - No). If no value is supplied then a default of 'Y' will be used.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="PrintSelectionSetChanges" default="Y">    <xsd:annotation>      <xsd:documentation>Indicates whether  the Selection Set changes should be included. (Y - Yes and N - No). If no value is supplied then a default of 'Y' will be used.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="PrintSelectionSetDeletions" default="Y">    <xsd:annotation>      <xsd:documentation>Indicates whether  the Selection Set deletions should be included. (Y - Yes and N - No). If no value is supplied then a default of 'Y' will be used. </xsd:documentation>      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Query">    <xsd:complexType mixed="false">      <xsd:sequence>        <xsd:element ref="Option"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Reprint" default="N">    <xsd:annotation>      <xsd:documentation>Indicates whether  the amendment journal should be reprinted. (Y - Yes and N - No). If set to 'Y' then all entries already printed that match the other options will be printed. If no value is supplied then a default of 'N' will be used.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="XslStyleSheet">    <xsd:complexType/>  </xsd:element></xsd:schema>