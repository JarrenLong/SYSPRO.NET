<!--
     Schema for PORQ70 Business Object for Xcelsius
-->
<xsd:schema id="SupplierPerformance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="Summary">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="SumQtyReceived" type="xsd:string" minOccurs="0" />
        <xsd:element name="AvgOrigPurchPrice" type="xsd:string" minOccurs="0" />
        <xsd:element name="AvgPriceReceived" type="xsd:string" minOccurs="0" />
        <xsd:element name="AvgPurchaseOrderCount" type="xsd:string" minOccurs="0" />
        <xsd:element name="SumPurchaseReceiptCount" type="xsd:string" minOccurs="0" />
        <xsd:element name="SumReceiptCount" type="xsd:string" minOccurs="0" />
        <xsd:element name="SumPurchaseRejectionCount" type="xsd:string" minOccurs="0" />
        <xsd:element name="SumRejectionCount" type="xsd:string" minOccurs="0" />
        <xsd:element name="SumRejectionQty" type="xsd:string" minOccurs="0" />
        <xsd:element name="SumQtyVariance" type="xsd:string" minOccurs="0" />
        <xsd:element name="AvgPriceVariance" type="xsd:string" minOccurs="0" />
        <xsd:element name="AvgOnTimeInFullQuality" type="xsd:string" minOccurs="0" />
        <xsd:element name="AvgOrderAge" type="xsd:string" minOccurs="0" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Years">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Year" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="YearHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Year" type="xsd:string" minOccurs="0" />
                    <xsd:element name="QtyReceived" type="xsd:string" minOccurs="0" />
                    <xsd:element name="OrigPurchPrice" type="xsd:string" minOccurs="0" />
                    <xsd:element name="PriceReceived" type="xsd:string" minOccurs="0" />
                    <xsd:element name="PurchaseOrderCount" type="xsd:string" minOccurs="0" />
                    <xsd:element name="PurchaseReceiptCount" type="xsd:string" minOccurs="0" />
                    <xsd:element name="ReceiptCount" type="xsd:string" minOccurs="0" />
                    <xsd:element name="PurchaseRejectionCount" type="xsd:string" minOccurs="0" />
                    <xsd:element name="RejectionCount" type="xsd:string" minOccurs="0" />
                    <xsd:element name="RejectionQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="OnTimeInFullQuality" type="xsd:string" minOccurs="0" />
                    <xsd:element name="QtyVariance" type="xsd:string" minOccurs="0" />
                    <xsd:element name="PriceVariance" type="xsd:string" minOccurs="0" />
                    <xsd:element name="OrderAge" type="xsd:string" minOccurs="0" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Periods" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Period" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="PeriodNumber" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodQtyReceived" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodOrigPurchPrice" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodPriceReceived" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodPurchaseOrderCount" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodPurchaseReceiptCount" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodReceiptCount" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodPurchaseRejectionCount" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodRejectionCount" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodRejectionQty" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodOnTimeInFullQuality" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodQtyVariance" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodPriceVariance" type="xsd:string" minOccurs="0" />
                          <xsd:element name="PeriodOrderAge" type="xsd:string" minOccurs="0" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SupplierPerformance" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="Summary" />
        <xsd:element ref="Years" />
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0" />
              <xsd:element name="Language" type="xsd:string" minOccurs="0" />
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0" />
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0" />
              <xsd:element name="Role" type="xsd:string" minOccurs="0" />
              <xsd:element name="Version" type="xsd:string" minOccurs="0" />
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0" />
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0" />
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0" />
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0" />
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0" />
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0" />
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0" />
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="DateSelection" type="xsd:string" minOccurs="0" />
              <xsd:element name="Currency" type="xsd:string" minOccurs="0" />
              <xsd:element name="InclSubTotalsSupplier" type="xsd:string" minOccurs="0" />
              <xsd:element name="InclSubTotalsWarehouse" type="xsd:string" minOccurs="0" />
              <xsd:element name="InclSubTotalsStockCode" type="xsd:string" minOccurs="0" />
              <xsd:element name="InclSubTotalsProductClass" type="xsd:string" minOccurs="0" />
              <xsd:element name="DateFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="DateFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="SupplierFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="SupplierFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="WarehouseFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="WarehouseFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="ProductClassFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="ProductClassFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="OrderAgeFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="OrderAgeFilterValue" type="xsd:string" minOccurs="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="SupplierTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Suppliers" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Supplier" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="SupplierHeader" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Supplier" type="xsd:string" minOccurs="0" />
                                <xsd:element name="APSMST_Description" type="xsd:string" minOccurs="0" />
                                <xsd:element name="OnHold" type="xsd:string" minOccurs="0" />
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element ref="Summary" minOccurs="0" maxOccurs="unbounded" />
                          <xsd:element ref="Years" minOccurs="0" maxOccurs="unbounded" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="WarehouseTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Warehouses" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Warehouse" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="WarehouseHeader" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Warehouse" type="xsd:string" minOccurs="0" />
                                <xsd:element name="INVWHC_Description" type="xsd:string" minOccurs="0" />
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element ref="Summary" minOccurs="0" maxOccurs="unbounded" />
                          <xsd:element ref="Years" minOccurs="0" maxOccurs="unbounded" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="StockCodeTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="StockCodes" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="StockCode" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="StockCodeHeader" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="StockCode" type="xsd:string" minOccurs="0" />
                                <xsd:element name="INVMST_Description" type="xsd:string" minOccurs="0" />
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element ref="Summary" minOccurs="0" maxOccurs="unbounded" />
                          <xsd:element ref="Years" minOccurs="0" maxOccurs="unbounded" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ProductClassTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ProductClasses" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ProductClass" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ProductClassHeader" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="ProductClass" type="xsd:string" minOccurs="0" />
                                <xsd:element name="SALPDS_Description" type="xsd:string" minOccurs="0" />
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element ref="Summary" minOccurs="0" maxOccurs="unbounded" />
                          <xsd:element ref="Years" minOccurs="0" maxOccurs="unbounded" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>