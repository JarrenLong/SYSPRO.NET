            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TotalsByPPVAccounts">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="TotalsByPPVItem" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="AccountNumber" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AccountNumDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AccountValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedAccountValue" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
