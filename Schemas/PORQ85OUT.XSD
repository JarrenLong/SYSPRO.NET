                    <xsd:element name="OrigGrnValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedOrigGrnValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StdGrnValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedStdGrnValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StdVarianceValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedStdVarianceValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PercentageValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedPercentageValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="InvoiceValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedInvoiceValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PriceVariance" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedPriceVariance" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="VarianceValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="EditedVarianceValue" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="GrandValueTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="GrndStandardValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedGrndStandardValue" type="xsd:string" minOccurs="0"/>
