              <xsd:element name="GRNValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedGRNValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="VarValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedVarValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotGrnValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedTotGrnValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotInvValue" type="xsd:double" minOccurs="0"/>
