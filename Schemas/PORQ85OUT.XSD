                    <xsd:element name="PurchaseOrder" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PurchaseOrderLine" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedPurchaseOrderLine" type="xsd:string" minOccurs="0"/>
