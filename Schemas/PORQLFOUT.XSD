<?xml version="1.0" encoding="Windows-1252"?>
<!-- Copyright 1994-2011 SYSPRO Ltd.-->
<!-- 
     Schema for the LCT Shipment Tracking Query Business Object
-->
<xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" id="LCTShipmentTrackingReportQueryListing" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="LCTShipmentTrackingReportQueryListing" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseXrefForExpectedMerchCost" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_UseXrefForExpectedMerchCost" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseTotalQtyForActualMerchCost" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_UseTotalQtyForActualMerchCost" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseExpectedElementValueWhenActualZero" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_UseExpectedElementValueWhenActualZero" type="xsd:string" minOccurs="0"/>
              <xsd:element name="VesselFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_VesselFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="VesselFilterValue">
                <xsd:complexType/>
              </xsd:element>
              <xsd:element name="ReceiptDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReceiptDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReceiptDateFilterValue">
                <xsd:complexType/>
              </xsd:element>
              <xsd:element name="ShipmentReferenceFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ShipmentReferenceFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ShipmentReferenceFilterValue">
                <xsd:complexType/>
              </xsd:element>
              <xsd:element name="ArchiveShipmentReferenceFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ArchiveShipmentReferenceFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ArchiveShipmentReferenceFilterValue">
                <xsd:complexType/>
              </xsd:element>
              <xsd:element name="Exchange_rate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CurrentInventoryUnitCost" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="VesselDetails" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:choice maxOccurs="unbounded">
              <xsd:element name="Vessel">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="VesselName">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="DepartureDate">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="EstArrivalDate">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="Route">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="RouteDescription">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="Shipment" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence maxOccurs="unbounded">
                    <xsd:element name="ShipmentReference">
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string"/>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="ShipmentEntry" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence maxOccurs="unbounded">
                          <xsd:element name="StockCode">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="StockDescription">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="PurchaseOrder">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="PurchaseOrderLine">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="Edited_Purchase_Order" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Currency">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="ExchRate">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="MultiplyDivide">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="ForeignPrice">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="EditedForeignPrice" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Supplier">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="SupplierName" type="xsd:string"/>
                          <xsd:element name="ShipQty">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="ShipQtyUnits" type="xsd:string"/>
                          <xsd:element name="EditedShipQty" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyReceived">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="QtyReceivedUnits" type="xsd:string"/>
                          <xsd:element name="EditedQtyReceived" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="UnitOfMeasure">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="Warehouse">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="Bin">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="SupplierCatalogueNumber">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="MerchandiseExpCost">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="EditedMerchandiseExpCost" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="MerchandiseActCost">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="EditedMerchandiseActCost" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="MerchandiseGRNValue">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string"/>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="EditedMerchandiseGRNValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ElementDetails" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence maxOccurs="unbounded">
                                <xsd:element name="CostElement">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="ExpectedCost">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedExpectedCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ActualCost">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedActualCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="GRNValue">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedGRNValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Invoiced">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="ElementDetailTotals">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="TotalExpectedCost">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedTotalExpectedCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TotalActualCost">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedTotalActualCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TotalGRNValue">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedTotalGRNValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CalculatedUnitCostExp">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedCalculatedUnitCostExp" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CalculatedUnitCostAct">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedCalculatedUnitCostAct" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CalculatedUnitCostGRN">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedCalculatedUnitCostGRN" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CalculatedUnitCostUm">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="CurrentInventoryCost">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedCurrentInventoryCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CurrentInventoryCostUm">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="ShipmentSubtotals">
                      <xsd:complexType>
                        <xsd:sequence maxOccurs="unbounded">
                          <xsd:element name="SubtotalElements" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence maxOccurs="unbounded">
                                <xsd:element name="CostElement">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="Translated_CostElement" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ExpectedCost">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedExpectedCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ActualCost">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedActualCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="GRNValue">
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string"/>
                                  </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="EditedGRNValue" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="SubtotalTotal">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="ExpectedCost" type="xsd:string"/>
                                <xsd:element name="EditedExpectedCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ActualCost" type="xsd:string"/>
                                <xsd:element name="EditedActualCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="GRNValue" type="xsd:string"/>
                                <xsd:element name="EditedGRNValue" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:choice>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
