                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="SalesOrder" type="xsd:unsignedShort"/>
                          <xsd:element name="SalesOrderLine" type="xsd:string"/>
                          <xsd:element name="MOrderQty" type="xsd:string"/>
                          <xsd:element name="MOrderUom" type="xsd:string"/>
