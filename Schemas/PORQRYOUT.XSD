                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="MRequisition" type="xsd:string"/>
                                <xsd:element name="RequisitionLine" type="xsd:string"/>
                                <xsd:element name="RoutedUser" type="xsd:string"/>
                                <xsd:element name="RouteNarrative" type="xsd:string"/>
                                <xsd:element name="ReceiptDate"/>
                                <xsd:element name="ReceiptQuantity" type="xsd:string"/>
                                <xsd:element name="OutstandingQty" type="xsd:string"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
