                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="LctDetail" minOccurs="0">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Vessel" type="xsd:unsignedInt"/>
