        <xsd:element name="DocumentFormat" type="xsd:string"/>
        <xsd:element name="CustomForm">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Field" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Sequence" type="xsd:string"/>
                    <xsd:element name="Name" type="xsd:string"/>
                    <xsd:element name="Prompt" type="xsd:string"/>
                    <xsd:element name="Type" type="xsd:string"/>
                    <xsd:element name="Length" type="xsd:string"/>
                    <xsd:element name="Decimals" type="xsd:string"/>
                    <xsd:element name="Default" type="xsd:string"/>
                    <xsd:element name="AllowNull" type="xsd:string"/>
