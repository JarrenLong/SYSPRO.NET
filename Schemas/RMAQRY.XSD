<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2009 SYSPRO Ltd.--><!--     Schema for the RMA Query Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="IncludeComments">    <xsd:annotation>      <xsd:documentation>This element is used to indicate whether the comments should be included.  The valid options are Y=Yes or N=No. The default is Y.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:maxLength value="1"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeLots">    <xsd:annotation>      <xsd:documentation>This element is used to indicate whether the lots should be included.  The valid options are Y=Yes or N=No. The default is Y.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:maxLength value="1"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeSerials">    <xsd:annotation>      <xsd:documentation>This element is used to indicate whether the serials should be included.  The valid options are Y=Yes or N=No. The default is Y.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:maxLength value="1"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeBins">    <xsd:annotation>      <xsd:documentation>This element is used to indicate whether the bins should be included.  The valid options are Y=Yes or N=No. The default is Y.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:maxLength value="1"/>        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeReceiptedLines">    <xsd:annotation>      <xsd:documentation>This element is used to indicate whether the receipted lines should be included.  The valid options are Y=Yes or N=No. The default is Y.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:maxLength value="1"/>        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeCrossShipmentLines">    <xsd:annotation>      <xsd:documentation>This element is used to indicate whether the cross shipment lines should be included.  The valid options are Y=Yes or N=No. The default is Y.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:maxLength value="1"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeExchangesLines">    <xsd:annotation>      <xsd:documentation>This element is used to indicate whether the exchanges lines should be included.  The valid options are Y=Yes or N=No. The default is Y.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:maxLength value="1"/>        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Option">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="IncludeReceiptedLines" minOccurs="0"/>        <xsd:element ref="IncludeCrossShipmentLines" minOccurs="0"/>        <xsd:element ref="IncludeExchangesLines" minOccurs="0"/>        <xsd:element ref="IncludeComments" minOccurs="0"/>        <xsd:element ref="IncludeBins" minOccurs="0"/>        <xsd:element ref="IncludeLots" minOccurs="0"/>        <xsd:element ref="IncludeSerials" minOccurs="0"/>        <xsd:element ref="xslstyleSheet" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Key">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="RMANumber"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Query">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Key"/>        <xsd:element ref="Option"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="RMANumber">    <xsd:annotation>      <xsd:documentation>This element is used to pass the RMA number.  It is a string of 8 characters.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:maxLength value="8"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="xslstyleSheet">    <xsd:complexType/>  </xsd:element></xsd:schema>