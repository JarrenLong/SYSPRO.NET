<!--
     Schema for the RMA Query Business Object
-->
<xs:schema id="RMAQuery" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="RMAQuery" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="QueryOptions">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="IncludeReceiptedLines" type="xs:string" minOccurs="0"/>
              <xs:element name="IncludeCrossShipmentLines" type="xs:string" minOccurs="0"/>
              <xs:element name="IncludeExchangesLines" type="xs:string" minOccurs="0"/>
              <xs:element name="IncludeComments" type="xs:string" minOccurs="0"/>
              <xs:element name="IncludeBins" minOccurs="0"/>
              <xs:element name="IncludeLots" type="xs:string" minOccurs="0"/>
              <xs:element name="IncludeSerials" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="RMAHeader">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="RMANumber" type="xs:string" minOccurs="0"/>
              <xs:element name="EntryDate" type="xs:string" minOccurs="0"/>
              <xs:element name="Status" type="xs:string" minOccurs="0"/>
              <xs:element name="CustomerBranch" type="xs:string" minOccurs="0"/>
              <xs:element name="LastTransactDate" type="xs:string" minOccurs="0"/>
              <xs:element name="Currency" type="xs:string" minOccurs="0"/>
              <xs:element name="UserField1" type="xs:string" minOccurs="0"/>
              <xs:element name="Area" type="xs:string" minOccurs="0"/>
              <xs:element name="SpecialInstrs" type="xs:string" minOccurs="0"/>
              <xs:element name="PrintedFlag" type="xs:string" minOccurs="0"/>
              <xs:element name="Operator" type="xs:string" minOccurs="0"/>
              <xs:element name="AlternateKey" type="xs:string" minOccurs="0"/>
              <xs:element name="OrderType" type="xs:string" minOccurs="0"/>
              <xs:element name="ServiceTicket" type="xs:string" minOccurs="0"/>
              <xs:element name="MultiShipCode" type="xs:string" minOccurs="0"/>
              <xs:element name="Comments" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="CustomerInformation">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Customer" type="xs:string" minOccurs="0"/>
              <xs:element name="CustomerName" type="xs:string" minOccurs="0"/>
              <xs:element name="Contact" type="xs:string" minOccurs="0"/>
              <xs:element name="Telephone" type="xs:string" minOccurs="0"/>
              <xs:element name="Fax" type="xs:string" minOccurs="0"/>
              <xs:element name="TaxStatus" type="xs:string" minOccurs="0"/>
              <xs:element name="TaxExemptNumber" type="xs:string" minOccurs="0"/>
              <xs:element name="Address1" type="xs:string" minOccurs="0"/>
              <xs:element name="Address2" type="xs:string" minOccurs="0"/>
              <xs:element name="Address3" type="xs:string" minOccurs="0"/>
              <xs:element name="Address4" type="xs:string" minOccurs="0"/>
              <xs:element name="Address5" type="xs:string" minOccurs="0"/>
              <xs:element name="ShipPostalCode" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="DetailLines">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="DetailLine" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="LineInformation" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="RMALineNumber" type="xs:string" minOccurs="0"/>
                          <xs:element name="StockCode" type="xs:string" minOccurs="0"/>
                          <xs:element name="Decimals" type="xs:string" minOccurs="0"/>
                          <xs:element name="AuthorizedQty" type="xs:string" minOccurs="0"/>
                          <xs:element name="AuthorizedQtyUnits" type="xs:string" minOccurs="0"/>
                          <xs:element name="IssueDate" type="xs:string" minOccurs="0"/>
                          <xs:element name="ReceivedDate" type="xs:string" minOccurs="0"/>
                          <xs:element name="LineStatus" type="xs:string" minOccurs="0"/>
                          <xs:element name="LIneType" type="xs:string" minOccurs="0"/>
                          <xs:element name="Warehouse" type="xs:string" minOccurs="0"/>
                          <xs:element name="PurchaseDate" type="xs:string" minOccurs="0"/>
                          <xs:element name="NonStFlag" type="xs:string" minOccurs="0"/>
                          <xs:element name="ProductClass" type="xs:string" minOccurs="0"/>
                          <xs:element name="RmaUom" type="xs:string" minOccurs="0"/>
                          <xs:element name="Invoice" type="xs:string" minOccurs="0"/>
                          <xs:element name="OrigTax" type="xs:string" minOccurs="0"/>
                          <xs:element name="UserField1" type="xs:string" minOccurs="0"/>
                          <xs:element name="UserField2" type="xs:string" minOccurs="0"/>
                          <xs:element name="UnitCost" type="xs:string" minOccurs="0"/>
                          <xs:element name="ProblemCode" type="xs:string" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Comments" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ProblemComment" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="CommentLine" type="xs:string" minOccurs="0"/>
                                <xs:element name="Commenttext" type="xs:string" minOccurs="0"/>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ReceiptDetails" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ReceivedDate" type="xs:string" minOccurs="0"/>
                          <xs:element name="ReceivedQty" type="xs:string" minOccurs="0"/>
                          <xs:element name="ReceivedQtyUnits" type="xs:string" minOccurs="0"/>
                          <xs:element name="Price" type="xs:string" minOccurs="0"/>
                          <xs:element name="Action" type="xs:string" minOccurs="0"/>
                          <xs:element name="RecSalesOrder" type="xs:string" minOccurs="0"/>
                          <xs:element name="RepairSalesOrder" type="xs:string" minOccurs="0"/>
                          <xs:element name="RecCreditNumber" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType/>
                          </xs:element>
                          <xs:element name="Job" type="xs:string" minOccurs="0"/>
                          <xs:element name="WarrantyDate" type="xs:string" minOccurs="0"/>
                          <xs:element name="InventoryUpdated" type="xs:string" minOccurs="0"/>
                          <xs:element name="Serial" type="xs:string" minOccurs="0"/>
                          <xs:element name="Lot" type="xs:string" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="CrossShipmentDetails" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="StockCode" type="xs:string" minOccurs="0"/>
                          <xs:element name="Description" type="xs:string" minOccurs="0"/>
                          <xs:element name="ProductClass" type="xs:string" minOccurs="0"/>
                          <xs:element name="Warehouse" type="xs:string" minOccurs="0"/>
                          <xs:element name="QtyCrossShipped" type="xs:string" minOccurs="0"/>
                          <xs:element name="QtyCrossShippedUnits" type="xs:string" minOccurs="0"/>
                          <xs:element name="Price" type="xs:string" minOccurs="0"/>
                          <xs:element name="UnitCost" type="xs:string" minOccurs="0"/>
                          <xs:element name="SalesOrder" type="xs:string" minOccurs="0"/>
                          <xs:element name="SalesOrderLine" type="xs:string" minOccurs="0"/>
                          <xs:element name="DateCrossShipped" type="xs:string" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>
