<!--  
     Schema for the Sales Analysis Commission Query Business Object
-->
<xsd:schema id="SalesCommission" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="SalesCommission" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="MonthToPrint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="YearToPrint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SalesPersonFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_SalesPersonFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SalesPersonFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BranchFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_BranchFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BranchFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ComsnValSuprs"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Commissions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Branches" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Branch" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="BranchHeader" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="SalesPersonDetails" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="SalesPersonHeader" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="SalesPerson" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="SalespersonName" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="CommissionDetails" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="InvDate" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Name" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Invoice" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="InvoiceValue" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="EditedInvoiceValue" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="CustomerPoNumber" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="OrderType" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="MerchandiseValue" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="EditedMerchandiseValue" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="CommissionableSales" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="EditedCommissionableSales" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="CommissionValue" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="EditedCommissionValue" type="xsd:string" minOccurs="0"/>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="SalesPersonTotals" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="TotalInvoiceValue" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="EditedTotalInvoiceValue" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="TotalMerchandiseValue" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="EditedTotalMerchandiseValue" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="TotalCommissionableSales" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="EditedTotalCommissionableSales" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="TotalCommissionValue" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="EditedTotalCommissionValue" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="BranchTotals" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="BranchTotalInvoiceValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedBranchTotalInvoiceValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="BranchTotalMerchandiseValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedBranchTotalMerchandiseValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="BranchTotalCommissionableSales" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedBranchTotalCommissionableSales" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="BranchTotalCommissionValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedBranchTotalCommissionValue" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="CompanyTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="CompanyTotalInvoiceValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedCompanyTotalInvoiceValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CompanyTotalMerchandiseValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedCompanyTotalMerchandiseValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CompanyTotalCommissionableSales" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedCompanyTotalCommissionableSales" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CompanyTotalCommissionValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedCompanyTotalCommissionValue" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
