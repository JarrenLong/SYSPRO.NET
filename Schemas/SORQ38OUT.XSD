<!-- 
     Schema for the Dispatch Note Trial Balance Query Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="DispatchTrialBalanceQuery" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeTotalsPerNote" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeTotalsPerNote" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeZeroBalanceNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeZeroBalanceNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Period" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Period" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_GlCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlCodeFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="GlCodes">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="GlCode" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="DispatchNoteDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Header" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="LedgerCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="LedgerDescription" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DispatchNote" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Period" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Invoice" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="InvPeriod" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Dr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedDr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Cr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedCR" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="GlDetail" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="DispatchNote" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Period" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EntryNumber" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Date" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Invoice" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="InvPeriod" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Dr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedDr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Cr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedCr" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="DispatchNoteTotals" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="DispatchNoteTotalDr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedDispatchNoteTotalDr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DispatchNoteTotalCr" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedDispatchNoteTotalCr" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="GlTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="GlTotalDr" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedGlTotalDr" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="GlTotalCr" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedGlTotalCr" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="ReportTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ReportTotalDr" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedReportTotalDr" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ReportTotalCr" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedReportTotalCr" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
