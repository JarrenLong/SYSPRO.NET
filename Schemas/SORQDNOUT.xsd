<!-- 
     Schema for the Sales Order Dispatch Note Query Business Object
-->
<xsd:schema id="NewDataSet" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="Serial">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Lot" type="xsd:string" minOccurs="0" />
        <xsd:element name="NewExpiryDate" type="xsd:string" minOccurs="0" />
        <xsd:element name="StockQtyToShip" type="xsd:string" minOccurs="0" />
        <xsd:element name="QtyThisSession" type="xsd:string" minOccurs="0" />
        <xsd:element name="NewScrapDate" type="xsd:string" minOccurs="0" />
        <xsd:element ref="Serial" minOccurs="0" maxOccurs="unbounded" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Bin">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Lot" type="xsd:string" minOccurs="0" />
        <xsd:element name="StockQtyToShip" type="xsd:string" minOccurs="0" />
        <xsd:element ref="Bin" minOccurs="0" maxOccurs="unbounded" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DispatchNoteDetail">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DispatchNote" type="xsd:string" minOccurs="0" msdata:Ordinal="0" />
        <xsd:element name="NextDetailLine" type="xsd:string" minOccurs="0" msdata:Ordinal="1" />
        <xsd:element name="ShippingInstrs" type="xsd:string" minOccurs="0" msdata:Ordinal="3" />
        <xsd:element name="SpecialInstrs" type="xsd:string" minOccurs="0" msdata:Ordinal="4" />
        <xsd:element name="DispatchComments1" type="xsd:string" minOccurs="0" msdata:Ordinal="5" />
        <xsd:element name="DispatchComments2" type="xsd:string" minOccurs="0" msdata:Ordinal="6" />
        <xsd:element name="DispatchComments3" type="xsd:string" minOccurs="0" msdata:Ordinal="7" />
        <xsd:element name="DispatchComments4" type="xsd:string" minOccurs="0" msdata:Ordinal="8" />
        <xsd:element name="UserField1" type="xsd:string" minOccurs="0" msdata:Ordinal="9" />
        <xsd:element name="UserField2" type="xsd:string" minOccurs="0" msdata:Ordinal="10" />
        <xsd:element name="Salesperson" type="xsd:string" minOccurs="0" msdata:Ordinal="11" />
        <xsd:element name="Branch" type="xsd:string" minOccurs="0" msdata:Ordinal="12" />
        <xsd:element name="Area" type="xsd:string" minOccurs="0" msdata:Ordinal="13" />
        <xsd:element name="MulDiv" type="xsd:string" minOccurs="0" msdata:Ordinal="14" />
        <xsd:element name="Currency" type="xsd:string" minOccurs="0" msdata:Ordinal="15" />
        <xsd:element name="DiscPct1" type="xsd:string" minOccurs="0" msdata:Ordinal="16" />
        <xsd:element name="DiscPct2" type="xsd:string" minOccurs="0" msdata:Ordinal="17" />
        <xsd:element name="DiscPct3" type="xsd:string" minOccurs="0" msdata:Ordinal="18" />
        <xsd:element name="OrderType" type="xsd:string" minOccurs="0" msdata:Ordinal="19" />
        <xsd:element name="InvTermsOverride" type="xsd:string" minOccurs="0" msdata:Ordinal="20" />
        <xsd:element name="ReprintFormat" type="xsd:string" minOccurs="0" msdata:Ordinal="21" />
        <xsd:element name="TaxExemptFlag" type="xsd:string" minOccurs="0" msdata:Ordinal="22" />
        <xsd:element name="TaxExemptOverride" type="xsd:string" minOccurs="0" msdata:Ordinal="23" />
        <xsd:element name="DispatchName" type="xsd:string" minOccurs="0" msdata:Ordinal="24" />
        <xsd:element name="PlannedDeliverDate" type="xsd:string" minOccurs="0" msdata:Ordinal="25" />
        <xsd:element name="ActualDeliveryDate" type="xsd:string" minOccurs="0" msdata:Ordinal="26" />
        <xsd:element name="ShipAddressFlag" type="xsd:string" minOccurs="0" msdata:Ordinal="27" />
        <xsd:element name="CustomerPoNumber" type="xsd:string" minOccurs="0" msdata:Ordinal="28" />
        <xsd:element name="CompanyTaxNo" type="xsd:string" minOccurs="0" msdata:Ordinal="29" />
        <xsd:element name="Invoice" type="xsd:string" minOccurs="0" msdata:Ordinal="30" />
        <xsd:element name="InvoiceCreatedDate" type="xsd:string" minOccurs="0" msdata:Ordinal="31" />
        <xsd:element name="ConsolidateInvFlag" type="xsd:string" minOccurs="0" msdata:Ordinal="32" />
        <xsd:element name="TaxExemptNumber" type="xsd:string" minOccurs="0" msdata:Ordinal="33" />
        <xsd:element name="DispatchNoteStatus" type="xsd:string" minOccurs="0" msdata:Ordinal="34" />
        <xsd:element name="PrevStatus" type="xsd:string" minOccurs="0" msdata:Ordinal="35" />
        <xsd:element name="Reason" type="xsd:string" minOccurs="0" msdata:Ordinal="36" />
        <xsd:element name="StatusWhenHeld" type="xsd:string" minOccurs="0" msdata:Ordinal="37" />
        <xsd:element name="Nationality" type="xsd:string" minOccurs="0" msdata:Ordinal="38" />
        <xsd:element name="DeliveryTerms" type="xsd:string" minOccurs="0" msdata:Ordinal="39" />
        <xsd:element name="TransactionNature" type="xsd:string" minOccurs="0" msdata:Ordinal="40" />
        <xsd:element name="TransportMode" type="xsd:string" minOccurs="0" msdata:Ordinal="41" />
        <xsd:element name="ProcessFlag" type="xsd:string" minOccurs="0" msdata:Ordinal="42" />
        <xsd:element name="DispatchCustName" type="xsd:string" minOccurs="0" msdata:Ordinal="43" />
        <xsd:element name="DispatchAddress1" type="xsd:string" minOccurs="0" msdata:Ordinal="44" />
        <xsd:element name="DispatchAddress2" type="xsd:string" minOccurs="0" msdata:Ordinal="45" />
        <xsd:element name="DispatchAddress3" type="xsd:string" minOccurs="0" msdata:Ordinal="46" />
        <xsd:element name="DispatchAddress4" type="xsd:string" minOccurs="0" msdata:Ordinal="47" />
        <xsd:element name="DispatchAddress5" type="xsd:string" minOccurs="0" msdata:Ordinal="48" />
        <xsd:element name="DispatchPostalCode" type="xsd:string" minOccurs="0" msdata:Ordinal="49" />
        <xsd:element name="State" type="xsd:string" minOccurs="0" msdata:Ordinal="50" />
        <xsd:element name="CountyZip" type="xsd:string" minOccurs="0" msdata:Ordinal="51" />
        <xsd:element name="ExtendedTaxCode" type="xsd:string" minOccurs="0" msdata:Ordinal="52" />
        <xsd:element name="MultiShipCode" type="xsd:string" minOccurs="0" msdata:Ordinal="53" />
        <xsd:element name="Quote" type="xsd:string" minOccurs="0" msdata:Ordinal="54" />
        <xsd:element name="GtrReference" type="xsd:string" minOccurs="0" msdata:Ordinal="55" />
        <xsd:element name="LastOperator" type="xsd:string" minOccurs="0" msdata:Ordinal="56" />
        <xsd:element name="Email" type="xsd:string" minOccurs="0" msdata:Ordinal="57" />
        <xsd:element name="CreditAuthority" type="xsd:string" minOccurs="0" msdata:Ordinal="58" />
        <xsd:element name="SalesOrder" nillable="true" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:simpleContent msdata:ColumnName="SalesOrder_Text" msdata:Ordinal="0">
              <xsd:extension base="xsd:string">
              </xsd:extension>
            </xsd:simpleContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Customer" nillable="true" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:simpleContent msdata:ColumnName="Customer_Text" msdata:Ordinal="0">
              <xsd:extension base="xsd:string">
              </xsd:extension>
            </xsd:simpleContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CustomerName" nillable="true" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:simpleContent msdata:ColumnName="CustomerName_Text" msdata:Ordinal="0">
              <xsd:extension base="xsd:string">
              </xsd:extension>
            </xsd:simpleContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="FixExchangeRate" nillable="true" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:simpleContent msdata:ColumnName="FixExchangeRate_Text" msdata:Ordinal="0">
              <xsd:extension base="xsd:string">
              </xsd:extension>
            </xsd:simpleContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="DispatchNoteLine" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="MiscCharge" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="DispatchNoteLine" type="xsd:string" minOccurs="0" />
                    <xsd:element name="NComment" type="xsd:string" minOccurs="0" />
                    <xsd:element name="NCommentFromLin" type="xsd:string" minOccurs="0" />
                    <xsd:element name="NMscChargeValue" type="xsd:string" minOccurs="0" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Freight" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="DispatchNoteLine" type="xsd:string" minOccurs="0" />
                    <xsd:element name="NComment" type="xsd:string" minOccurs="0" />
                    <xsd:element name="NCommentFromLin" type="xsd:string" minOccurs="0" />
                    <xsd:element name="NMscChargeValue" type="xsd:string" minOccurs="0" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Merchandise" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="DispatchNoteLine" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MStockCode" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MStockDes" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MWarehouse" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MBin" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MCusSupStkCode" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MOrderQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MShipQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MStockQtyToShp" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MBackOrderQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MOrderUom" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MStockingUom" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MStockUnitMass" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MStockUnitVol" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MPrice" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MPriceUom" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MDiscPct1" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MDiscPct2" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MDiscPct3" type="xsd:string" minOccurs="0" />
                    <xsd:element name="OrderLineValue" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MCustRequestDat" type="xsd:string" minOccurs="0" />
                    <xsd:element name="MHierarchyJob" type="xsd:string" minOccurs="0" />
                    <xsd:element ref="Serial" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="Bin" minOccurs="0" maxOccurs="unbounded" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="Language" type="xsd:string" />
      <xsd:attribute name="CssStyle" type="xsd:string" />
      <xsd:attribute name="DecFormat" type="xsd:string" />
      <xsd:attribute name="DateFormat" type="xsd:string" />
      <xsd:attribute name="Role" type="xsd:string" />
      <xsd:attribute name="Version" type="xsd:string" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="Serial" />
        <xsd:element ref="Bin" />
        <xsd:element ref="DispatchNoteDetail" />
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>