<!--
     Schema for the Sales Order Performance Query Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="AreaFilterType" type="xsd:string"/>
  <xsd:element name="AreaFilterValue">
    <xsd:complexType/>
  </xsd:element>
  <xsd:element name="BranchFilterType" type="xsd:string"/>
  <xsd:element name="BranchFilterValue">
    <xsd:complexType/>
  </xsd:element>
  <xsd:complexType name="CompanyTotalsType">
    <xsd:sequence>
      <xsd:element ref="TotalQtyInvoiced"/>
      <xsd:element ref="TotalQtyOutstanding"/>
      <xsd:element ref="TotalNetSlsValExTax"/>
      <xsd:element ref="TotalInvCount"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="CustomerClassFilterType" type="xsd:string"/>
  <xsd:element name="CustomerClassFilterValue">
    <xsd:complexType/>
  </xsd:element>
  <xsd:element name="CustomerFilterType" type="xsd:string"/>
  <xsd:element name="CustomerFilterValue">
    <xsd:complexType/>
  </xsd:element>
  <xsd:element name="DateSelection" type="xsd:string"/>
  <xsd:element name="InclSubTotalsMain" type="xsd:string"/>
