    <xsd:complexType/>
  </xsd:element>
  <xsd:element name="WarehouseFilterType" type="xsd:string"/>
  <xsd:element name="WarehouseFilterValue">
    <xsd:complexType/>
  </xsd:element>
</xsd:schema>
