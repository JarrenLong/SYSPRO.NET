      <xsd:element ref="AreaFilterValue"/>
      <xsd:element ref="OrderTypeFilterType"/>
      <xsd:element ref="OrderTypeFilterValue"/>
      <xsd:element ref="CustomerClassFilterType"/>
      <xsd:element ref="CustomerClassFilterValue"/>
      <xsd:element ref="SalesPersonFilterType"/>
      <xsd:element ref="SalesPersonFilterValue"/>
      <xsd:element ref="CustomerFilterType"/>
      <xsd:element ref="CustomerFilterValue"/>
      <xsd:element ref="StockCodeFilterType"/>
      <xsd:element ref="StockCodeFilterValue"/>
      <xsd:element ref="ProductClassFilterType"/>
      <xsd:element ref="ProductClassFilterValue"/>
      <xsd:element ref="WarehouseFilterType"/>
      <xsd:element ref="WarehouseFilterValue"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="SalesOrderPerformance">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="QueryOptions" type="QueryOptionsType"/>
        <xsd:element name="CompanyTotals" type="CompanyTotalsType"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SalesPersonFilterType" type="xsd:string"/>
  <xsd:element name="SalesPersonFilterValue">
    <xsd:complexType/>
  </xsd:element>
  <xsd:element name="SelectedDateFilterType" type="xsd:string"/>
