  <xsd:element name="Sub3BasedOnMeasure" type="xsd:string"/>
  <xsd:element name="Sub3PeriodDetail" type="xsd:string"/>
  <xsd:element name="SubMeasure1" type="xsd:string"/>
  <xsd:element name="SubMeasure2" type="xsd:string"/>
