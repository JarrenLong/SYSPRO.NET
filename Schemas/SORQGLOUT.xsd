<!-- 
     Schema for the Sales Order GL Integration Query Business Object
-->
<xsd:schema id="NewDataSet" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="SalesOrderIntegration">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="SalesOrder" type="xsd:string" minOccurs="0" msdata:Ordinal="0" />
        <xsd:element name="Branch" type="xsd:string" minOccurs="0" msdata:Ordinal="1" />
        <xsd:element name="GeographicArea" type="xsd:string" minOccurs="0" msdata:Ordinal="2" />
        <xsd:element name="GlIntegrationStatus" type="xsd:string" minOccurs="0" msdata:Ordinal="3" />
      </xsd:sequence>
      <xsd:attribute name="Language" type="xsd:string" />
      <xsd:attribute name="Language2" type="xsd:string" />
      <xsd:attribute name="CssStyle" type="xsd:string" />
      <xsd:attribute name="DecFormat" type="xsd:string" />
      <xsd:attribute name="DateFormat" type="xsd:string" />
      <xsd:attribute name="Role" type="xsd:string" />
      <xsd:attribute name="Version" type="xsd:string" />
      <xsd:attribute name="OperatorPrimaryRole" type="xsd:string" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="SalesOrderIntegration" />
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>