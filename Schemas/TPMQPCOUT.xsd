<!-- 
     Schema for the TPM Promotion Code Query Business Object
-->
<xsd:schema id="NewDataSet" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="ListCode" nillable="true">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="xsd:string"/>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TPMPromotionCodeQuery">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="QueryOptions" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="IncludeExpiredPromotions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlobalPromotions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PromotionCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PromotionCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PromotionTypeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PromotionTypeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ExpiryDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ExpiryDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StartDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StartDateFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="PromotionCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ExpiryDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StartDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCatagory" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCatCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustomerCatagory" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustCatCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AmountType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Basis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Threshold" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PromotionValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GrossNet" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PromotionType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OffInvoiceType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AccrualLdg" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ExpenseLdg" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DeductionCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductClass" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AccrualType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ApplySub" type="xsd:string" minOccurs="0"/>
              <xsd:element name="FreeGoodType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MaxThreshold" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlobalFlag" type="xsd:string" minOccurs="0"/>
              <xsd:element name="FreeGoodsApplyFlag" type="xsd:string" minOccurs="0"/>
              <xsd:element name="InvoicePrice" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustomerPrice" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TieredPromFlag" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MultipleOfFlag" type="xsd:string" minOccurs="0"/>
              <xsd:element name="FreeGoodRounding" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TpmSequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockList" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element ref="ListCode" minOccurs="0" maxOccurs="unbounded"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="CustomerList" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element ref="ListCode" minOccurs="0" maxOccurs="unbounded"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Thresholds" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ThresholdDetail" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Threshold" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyToship" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TranslatedText" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Text_Include" minOccurs="0"/>
              <xsd:element name="Text_Exclude" minOccurs="0"/>
              <xsd:element name="Text_All" minOccurs="0"/>
              <xsd:element name="Text_Yes" minOccurs="0"/>
              <xsd:element name="Text_No" minOccurs="0"/>
              <xsd:element name="Text_Percentage" minOccurs="0"/>
              <xsd:element name="Text_Price" minOccurs="0"/>
              <xsd:element name="Text_Value" minOccurs="0"/>
              <xsd:element name="Text_Mass" minOccurs="0"/>
              <xsd:element name="Text_Volume" minOccurs="0"/>
              <xsd:element name="Text_Quantity" minOccurs="0"/>
              <xsd:element name="Text_Basis" minOccurs="0"/>
              <xsd:element name="Text_For_all_customers" minOccurs="0"/>
              <xsd:element name="Text_For_customer" minOccurs="0"/>
              <xsd:element name="Text_For_geographic_area" minOccurs="0"/>
              <xsd:element name="Text_For_customer_class" minOccurs="0"/>
              <xsd:element name="Text_For_branch" minOccurs="0"/>
              <xsd:element name="Text_For_customer_list" minOccurs="0"/>
              <xsd:element name="Text_For_buying_group" minOccurs="0"/>
              <xsd:element name="Text___par" minOccurs="0"/>
              <xsd:element name="Text_Delivery_date" minOccurs="0"/>
              <xsd:element name="Text_Order_date" minOccurs="0"/>
              <xsd:element name="Text_Accrual_type" minOccurs="0"/>
              <xsd:element name="Text_Off_invoice_allowance" minOccurs="0"/>
              <xsd:element name="Text_Accrual_GL_account" minOccurs="0"/>
              <xsd:element name="Text_Discount_method" minOccurs="0"/>
              <xsd:element name="Text_Expense_GL_account" minOccurs="0"/>
              <xsd:element name="Text_Invoice_price" minOccurs="0"/>
              <xsd:element name="Text_Deduction_code" minOccurs="0"/>
              <xsd:element name="Text_Customer_price" minOccurs="0"/>
              <xsd:element name="Text_Actual_price_change" minOccurs="0"/>
              <xsd:element name="Text_Detail_discount" minOccurs="0"/>
              <xsd:element name="Text_Summary_discount" minOccurs="0"/>
              <xsd:element name="Text_Order_stock_code" minOccurs="0"/>
              <xsd:element name="Text_Specific_stock_code" minOccurs="0"/>
              <xsd:element name="Text_____Global____" minOccurs="0"/>
              <xsd:element name="Text_Gross" minOccurs="0"/>
              <xsd:element name="Text_Net" minOccurs="0"/>
              <xsd:element name="Text_Net_price" minOccurs="0"/>
              <xsd:element name="Text_Only" minOccurs="0"/>
              <xsd:element name="Text_Line_promotion" minOccurs="0"/>
              <xsd:element name="Text_Line_discount" minOccurs="0"/>
              <xsd:element name="Text_Price_change" minOccurs="0"/>
              <xsd:element name="Text_Price_type" minOccurs="0"/>
              <xsd:element name="Text_Product_class" minOccurs="0"/>
              <xsd:element name="Text_Off_invoice" minOccurs="0"/>
              <xsd:element name="Text_Accrual" minOccurs="0"/>
              <xsd:element name="Text_Free_goods" minOccurs="0"/>
              <xsd:element name="Text_For_all_stock_codes" minOccurs="0"/>
              <xsd:element name="Text__For_stock_code" minOccurs="0"/>
              <xsd:element name="Text_For_stock_list" minOccurs="0"/>
              <xsd:element name="Text_For_department" minOccurs="0"/>
              <xsd:element name="Text_For_product_class" minOccurs="0"/>
              <xsd:element name="Text_For_product_group" minOccurs="0"/>
              <xsd:element name="Text_In_mutliples_of" minOccurs="0"/>
              <xsd:element name="Text__Round_up_" minOccurs="0"/>
              <xsd:element name="Text__Round_down_" minOccurs="0"/>
              <xsd:element name="Text_Threshold_Weight" minOccurs="0"/>
              <xsd:element name="Text_Threshold_Volume" minOccurs="0"/>
              <xsd:element name="Text_Threshold_Quantity" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="Language" type="xsd:string"/>
      <xsd:attribute name="CssStyle" type="xsd:string"/>
      <xsd:attribute name="DecFormat" type="xsd:string"/>
      <xsd:attribute name="DateFormat" type="xsd:string"/>
      <xsd:attribute name="Role" type="xsd:string"/>
      <xsd:attribute name="Version" type="xsd:string"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ListCode"/>
        <xsd:element ref="TPMPromotionCodeQuery"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
