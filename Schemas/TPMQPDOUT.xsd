<!-- 
     Schema for the TPM Pricing Detail Query Business Object
-->
<xsd:schema id="NewDataSet" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="TPMPricingDetailQuery">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="QueryOptions" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="IncludeStockCodeType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeStockCodeType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeProductClassType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeProductClassType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeProductGroupType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeProductGroupType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeDepartmentType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeDepartmentType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeWeightBasis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeWeightBasis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeVolumeBasis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeVolumeBasis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeQuantityBasis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeQuantityBasis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ProductClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductClassFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductGroupFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ProductGroupFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductGroupFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DepartmentFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DepartmentFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DepartmentFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="PriceType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PriceType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="KeyArea" type="xsd:string" minOccurs="0"/>
              <xsd:element name="KeyAreaDescription" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PickupDelivery" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SelectionType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_SelectionType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SelectionArea" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Basis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Basis" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PriceDiscountFlag" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PriceBreakItems" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="PriceBreakItem" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="PriceBreak" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedPriceBreak" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Price" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedPrice" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TranslatedText" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Text_All" minOccurs="0"/>
              <xsd:element name="Text_Mass" minOccurs="0"/>
              <xsd:element name="Text_Quantity" minOccurs="0"/>
              <xsd:element name="Text_Volume" minOccurs="0"/>
              <xsd:element name="Text_Stock_codes" minOccurs="0"/>
              <xsd:element name="Text_Product_classes" minOccurs="0"/>
              <xsd:element name="Text_Product_groups" minOccurs="0"/>
              <xsd:element name="Text_Departments" minOccurs="0"/>
              <xsd:element name="Text_Area" minOccurs="0"/>
              <xsd:element name="Text_Buying_group" minOccurs="0"/>
              <xsd:element name="Text_Customer_class" minOccurs="0"/>
              <xsd:element name="Text_Yes" minOccurs="0"/>
              <xsd:element name="Text_No" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="Language" type="xsd:string"/>
      <xsd:attribute name="CssStyle" type="xsd:string"/>
      <xsd:attribute name="DecFormat" type="xsd:string"/>
      <xsd:attribute name="DateFormat" type="xsd:string"/>
      <xsd:attribute name="Role" type="xsd:string"/>
      <xsd:attribute name="Version" type="xsd:string"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="TPMPricingDetailQuery"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
