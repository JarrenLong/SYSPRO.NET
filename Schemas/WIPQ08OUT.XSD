<!-- 
     Schema for the WIP Detail Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="WIPDetailReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="WIPDetailReport" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="IncludeSubTotals" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_IncludeSubTotals" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ListSubJobsUnderMasterJobs" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_InclSubJobsUnderMast" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="NewPagePerJob" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_NewPagePerJob" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="IncludeReference" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_IncludeReference" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="IncludeNotation" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_IncludeNotation" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="IncludeCoProducts" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_IncludeCoProducts" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="IncludeJobsOnHold" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_IncludeJobsOnHold" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="IncludeCompletedJobs" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_IncludeCompletedJobs" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="IncludeUnconfirmedJobs" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_IncludeUnconfirmedJobs" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="PrintManufacturingUMQuantities" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_PrintMUMQuantities" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="JobFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_JobFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="JobFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="JobClassificationFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_JobClassFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="JobClassificationFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CustomerFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_CustomerFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CustomerFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="JobDeliveryDateFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_JobDelDateFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="JobDeliveryDateFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="TransactionDateFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_TrnDateFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="TransactionDateFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="JobDetails">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="JobDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Job" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Description" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="JobClassification" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="CustomerName" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="StockCode" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Priority" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="MakeQuantity" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_MakeQuantity" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="OutstandingQuantity" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_OutstandingQuantity" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="DeliveryDate" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="HoldFlag" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Translated_HoldFlag" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="JobType" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Translated_JobType" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="MasterJob" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="CoProductDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="CoProduct" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="CoProductDescription" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="CoProductJob" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="CoProductMakeQty" type="xsd:double" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_CoProductMakeQty" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="CoProductOutstandingQty" type="xsd:double" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_CoProductOutstandingQty" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="CoProductDueDate" type="xsd:string" minOccurs="0"></xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="TransactionDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TransactionDetail" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Transaction" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Translated_Transaction" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="ItemType" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Translated_ItemType" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Item" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="ItemDescription" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="UnitOfMeasure" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="OperationNumber" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="TimeInHours" type="xsd:double" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_TimeInHours" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Quantity" type="xsd:double" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_Quantity" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="ProductClass" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="UnitCost" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_UnitCost" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="RunRate" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_RunRate" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="PurchaseOrderNumber" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Reference" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Employee" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Journal" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="TrnDate" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="ValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_ValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Source" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Translated_Source" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="MaterialAdditionalReference" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="LabourReference" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="LabourAdditionalReference" type="xsd:string" minOccurs="0"></xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="ItemTotals" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Type" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Translated_Type" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Item" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Quantity" type="xsd:double" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_Quantity" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="TimeInHours" type="xsd:double" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_TimeInHours" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="ValueIssued" type="xsd:double" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_ValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="JobTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="LabourTotalValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_LabTotalValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="MaterialTotalValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_MatTotalValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="TotalValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_TotalValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="WorkCentreProductClassSummary">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Summary" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Type" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Translated_Type" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="ItemSummary" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Item" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Quantity" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_Quantity" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="TimeInHours" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_TimeInHours" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="ValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_ValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="SummaryTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Quantity" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_Quantity" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="TimeInHours" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_TimeInHours" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="ValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_ValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ReportTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ValueIssued" type="xsd:double" minOccurs="0"></xsd:element>
              <xsd:element name="Edited_ValueIssued" type="xsd:string" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
