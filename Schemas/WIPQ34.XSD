<!-- 
     Schema for the WIP Part Billings Journal Query Business Object
-->
<xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="CompletionPercentage">
    <xsd:annotation>
      <xsd:documentation>Indicates whether to print only those jobs with a completion percentage that exceeds the percentage value entered at the Greater than field.  All, Single completion percentage to be returned. FilterType options - A(All), S(Single).  Default to A(All).</xsd:documentation>
      <xsd:appinfo source="screenItem=Y; A=All;S=Single"></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="FilterType" use="optional" default="A">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="A"></xsd:enumeration>
            <xsd:enumeration value="R"></xsd:enumeration>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"></xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Filter">
    <xsd:annotation>
      <xsd:documentation>This is the parent Filter element</xsd:documentation>
      <xsd:appinfo source="screenItem=N"></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="Job"></xsd:element>
        <xsd:element ref="JobClassification"></xsd:element>
        <xsd:element ref="PlannedDeliveryDate"></xsd:element>
        <xsd:element ref="CompletionPercentage"></xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="OnlyJobsWithChgInPercentageComp">
    <xsd:annotation>
      <xsd:documentation>Indicates whether or not to only show jobs that have a change in the percentage complete. (Y)Yes to only show jobs with changes to percentage complete or (N)No to show all jobs. Default (N) No</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"></xsd:enumeration>
        <xsd:enumeration value="Y"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PrintManufacturingUMQuantities">
    <xsd:annotation>
      <xsd:documentation>This is an option to print manufacturing unit of measure quantities instead of stocking unit of measure quantities. Valid entries are Y(Yes) or N(No). Default depends on your setup options.</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"></xsd:enumeration>
        <xsd:enumeration value="N"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="IncludeSubJobsUnderMasterJobs">
    <xsd:annotation>
      <xsd:documentation>Indicates whether or not to include sub jobs under master jobs. (Y)Yes to include sub jobs or (N)No to exlude them. If no option is submitted here, a default of (Y) is used</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"></xsd:enumeration>
        <xsd:enumeration value="N"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="JobClassification">
    <xsd:annotation>
      <xsd:documentation>All, Single, Range of job classifications to be returned. FilterType options - A(All), S(Single), R(Range).  Default to A(All).</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;browse=WipJobClass;A=All;S=Single;R=Range"></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="FilterType" use="optional" default="A">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="A"></xsd:enumeration>
            <xsd:enumeration value="R"></xsd:enumeration>
            <xsd:enumeration value="S"></xsd:enumeration>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"></xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Job">
    <xsd:annotation>
      <xsd:documentation>All, Single, Range of jobs to be returned. FilterType options - A(All), S(Single), R(Range).  Default to A(All).</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;browse=WipMaster;A=All;S=Single;R=Range"></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="FilterType" use="optional" default="A">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="A"></xsd:enumeration>
            <xsd:enumeration value="R"></xsd:enumeration>
            <xsd:enumeration value="S"></xsd:enumeration>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"></xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Option">
    <xsd:annotation>
      <xsd:documentation>This is the parent Option element</xsd:documentation>
      <xsd:appinfo source="screenItem=N"></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ReportSequence"></xsd:element>
        <xsd:element ref="ValuesBy"></xsd:element>
        <xsd:element ref="IncludeSubJobsUnderMasterJobs"></xsd:element>
        <xsd:element ref="OnlyJobsWithChgInPercentageComp"></xsd:element>
        <xsd:element ref="PrintManufacturingUMQuantities"></xsd:element>
        <xsd:element ref="IncludeCreatedFromQuotes"></xsd:element>
        <xsd:element ref="IncludeCreatedFromWIP"></xsd:element>
        <xsd:element ref="IncludeCreatedFromMRP"></xsd:element>
        <xsd:element ref="IncludeCreatedFromSOR"></xsd:element>
        <xsd:element ref="XslStylesheet"></xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PlannedDeliveryDate">
    <xsd:annotation>
      <xsd:documentation>All, Single, Range of Planned Delivery dates to be returned. FilterType options - A(All), S(Single), R(Range).  Default to A(All).</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;browse={date};A=All;S=Single;R=Range"></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="FilterType" use="optional" default="A">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="A"></xsd:enumeration>
            <xsd:enumeration value="R"></xsd:enumeration>
            <xsd:enumeration value="S"></xsd:enumeration>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"></xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Query">
    <xsd:annotation>
      <xsd:documentation>This is the Root element</xsd:documentation>
      <xsd:appinfo source="screenItem=N"></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="Option"></xsd:element>
        <xsd:element ref="Filter"></xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ReportSequence">
    <xsd:annotation>
      <xsd:documentation>Indicates the sequence in which to print the report (J) Job, (C) Class .  If no value is submitted here a default of (J) Job will be used.</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;J=Job;C=Job classification"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="J"></xsd:enumeration>
        <xsd:enumeration value="C"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ValuesBy">
    <xsd:annotation>
      <xsd:documentation>Indicates how the values will be displayed in the report.  (N) Notional or (C) Co-product. if no selection is made then a default of (N) will be used.</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;N=Notional;C=CoProduct"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"></xsd:enumeration>
        <xsd:enumeration value="C"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="IncludeCreatedFromQuotes">
    <xsd:annotation>
      <xsd:documentation>This is an option to include Jobs created from Quotations. Valid entries are Y(Yes - Default) and N(No). </xsd:documentation>
      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"></xsd:enumeration>
        <xsd:enumeration value="N"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="IncludeCreatedFromWIP">
    <xsd:annotation>
      <xsd:documentation>This is an option to include Jobs created from Work in progress.  Valid entries are Y(Yes - Default) and N(No)</xsd:documentation>
      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"></xsd:enumeration>
        <xsd:enumeration value="N"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="IncludeCreatedFromMRP">
    <xsd:annotation>
      <xsd:documentation>This is an option to Include Jobs created from Requirements planning. Valid entries are Y(Yes - Default) and N(No). </xsd:documentation>
      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"></xsd:enumeration>
        <xsd:enumeration value="N"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="IncludeCreatedFromSOR">
    <xsd:annotation>
      <xsd:documentation>This is an option to Include Jobs created from Sales order review. Valid entries are Y(Yes - Default) and N(No). </xsd:documentation>
      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"></xsd:appinfo>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"></xsd:enumeration>
        <xsd:enumeration value="N"></xsd:enumeration>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="XslStylesheet">
    <xsd:annotation>
      <xsd:documentation>This is the StyleSheet that is used</xsd:documentation>
      <xsd:appinfo source="screenItem=N"></xsd:appinfo>
    </xsd:annotation>
    <xsd:complexType></xsd:complexType>
  </xsd:element>
</xsd:schema>
