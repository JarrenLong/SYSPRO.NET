<!-- 
     Schema for the WIP Labor Journal Query Business Object
-->
<xsd:schema id="WIPLaborJournalReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="WIPLaborJournalReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="PrintNewPage" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintNewPage" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MonthSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="YearSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JournalNumberSelectionFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JournalNumberSelectionFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JournalNumberSelectionFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="JournalEntries">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="JournalEntriesHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="LedgerMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LedgerPeriod" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LedgerYear" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JournalNo" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="OperatorMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="JobPosting" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="JobPostingDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Entry" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Workcentre" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Source" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Machine" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Employee" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Job" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Operation" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Date" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StartupTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Edited_StartupTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="RunTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Edited_RunTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SetupTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Edited_SetupTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TeardownTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Edited_TeardownTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LaborValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_LaborValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="NonProductCode" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyCompleted" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Edited_QtyCompleted" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotalQtyScrapped" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Edited_TotalQtyScrapped" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="JournalEntriesTotal" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="JournalTotalsMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_JournalTotalsMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StartupTimeTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_StartupTimeTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RunTimeTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_RunTimeTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SetupTimeTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_SetupTimeTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TeardownTimeTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_TeardownTimeTotal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LaborvalueTotal" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_LaborvalueTotal" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="EndJournalMsg" type="xsd:string" minOccurs="0"/>
              <xsd:element name="EndJnlNo" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
