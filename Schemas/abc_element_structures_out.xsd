<!--
    SRS ABC List of Element Structures
    SYSPRO Reporting Service XSDOut
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="ABCElementStructures" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="ABCElementStructures" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
       				        <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
      				        <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
       				        <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
   				              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
   				              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
    				              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
        				        <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
       				        <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
        				        <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
         				        <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
         				        <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
         				        <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
         				        <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
          				        <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
           				        <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
            				  <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
           				        <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
           					  <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
            				  <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
            				  <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
            				  <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
            				  <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
           					  <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
     						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_StockCodeFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ElementFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_ElementFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ElementFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncidenceSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncidenceSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncidenceSelectionFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ElementStructures">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ElementStructure" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Header" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="Details" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Incidence" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Element" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ElementDescription" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CalcMethod" type="xsd:string" minOccurs="0"/>
																<xsd:element name="DriverQuantity" type="xsd:double" minOccurs="0"/>
																<xsd:element name="RouteCode" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
