                    <xsd:element name="Edited_ReqPastDuePayables" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ReqCurrentPayables" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_ReqCurrentPayables" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Required1" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_Required1" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Required2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_Required2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Required3" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_Required3" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Required4" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_Required4" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Required5" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_Required5" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Required6" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_Required6" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierCreditsAvailable" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_SupplierCreditsAvailable" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupplierOnHold" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CompanyTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="TotalCreditsAvailable" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_TotalCreditsAvailable" type="xsd:string" minOccurs="0"/>
              <xsd:element name="HeadingsDescriptions" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Heading1" type="xsd:string" minOccurs="0"/>
