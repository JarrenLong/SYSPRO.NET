              <xsd:element name="Text_Local" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Foreign" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_At_current_exchange_rate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_At_entry_time" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_All" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Single" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Range" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
