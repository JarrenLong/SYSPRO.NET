<!-- 
     Schema for the AP Label Format Report
-->
<xsd:schema id="NewDataSet" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="APLabelFormat">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="QueryOptions" type="xsd:string" minOccurs="0" />
        <xsd:element name="SystemInformation" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0" />
              <xsd:element name="Language" type="xsd:string" minOccurs="0" />
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0" />
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0" />
              <xsd:element name="Version" type="xsd:string" minOccurs="0" />
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0" />
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0" />
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0" />
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0" />
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="LinesPerLabel" type="xsd:string" minOccurs="0" />
              <xsd:element name="ColsBetwLabels" type="xsd:string" minOccurs="0" />
              <xsd:element name="NoLabelsOnPage" type="xsd:string" minOccurs="0" />
              <xsd:element name="NoLabelsPerSupp" type="xsd:string" minOccurs="0" />
              <xsd:element name="PrnDateFormat" type="xsd:string" minOccurs="0" />
              <xsd:element name="Supplier" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppType" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppClass" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppBank" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppBankName" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppBrn" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppBrnName" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppCurr" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppCurrDesc" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppName" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppAdd1" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppAdd2" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppAdd3" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppAdd4" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppAdd5" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppPCod" type="xsd:string" minOccurs="0" />
              <xsd:element name="SuppShortNam" type="xsd:string" minOccurs="0" />
              <xsd:element name="Telephone" type="xsd:string" minOccurs="0" />
              <xsd:element name="Contact" type="xsd:string" minOccurs="0" />
              <xsd:element name="GeogArea" type="xsd:string" minOccurs="0" />
              <xsd:element name="LabelPrnDate" type="xsd:string" minOccurs="0" />
              <xsd:element name="YTDMerchAmt" type="xsd:string" minOccurs="0" />
              <xsd:element name="CoName" type="xsd:string" minOccurs="0" />
              <xsd:element name="CoAdd1" type="xsd:string" minOccurs="0" />
              <xsd:element name="CoAdd2" type="xsd:string" minOccurs="0" />
              <xsd:element name="CoAdd3" type="xsd:string" minOccurs="0" />
              <xsd:element name="CoRegNo" type="xsd:string" minOccurs="0" />
              <xsd:element name="RemittanceName" type="xsd:string" minOccurs="0" />
              <xsd:element name="RemittanceAdd1" type="xsd:string" minOccurs="0" />
              <xsd:element name="RemittanceAdd2" type="xsd:string" minOccurs="0" />
              <xsd:element name="RemittanceAdd3" type="xsd:string" minOccurs="0" />
              <xsd:element name="RemittanceAdd4" type="xsd:string" minOccurs="0" />
              <xsd:element name="RemittanceAdd5" type="xsd:string" minOccurs="0" />
              <xsd:element name="RemittancePCod" type="xsd:string" minOccurs="0" />
              <xsd:element name="AccumGrossPayments" type="xsd:string" minOccurs="0" />
              <xsd:element name="PAYERefNumber" type="xsd:string" minOccurs="0" />
              <xsd:element name="UserDefined1" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="UserDef1Desc" type="xsd:string" minOccurs="0" />
                    <xsd:element name="UserDef1Value" type="xsd:string" minOccurs="0" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="TextFields" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="TextField" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="PositionOfMessage" type="xsd:string" minOccurs="0" />
                          <xsd:element name="ContentsOfMessage" type="xsd:string" minOccurs="0" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="APLabelFormat" />
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>