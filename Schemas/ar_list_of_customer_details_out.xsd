              <xsd:element name="CreditLimitSelectionFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Customer">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CustomerListHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
