<!-- 
     Schema for the List of Post Dated Checks Business Object Out
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="ARListofPostDatedChecks" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="ARListofPostDatedChecks" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>							
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ReportSequence" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CustomerFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CustomerFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SalespersonFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SalespersonFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GeographicAreaFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GeographicAreaFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PostDatedChequeDateFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PostDatedChequeDateFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="PostDatedbyMonth">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="MonthCheckDated" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PostDatedCheck" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ARSMST_Name" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ShortName" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Cheque" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AvailableDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Amount" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SALBRN_Description" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Class" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TBLARC_Description" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Area" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SALARE_Description" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Salesperson" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SALSLS_Name" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CompanyTotals">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ReportCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="TotalPostDatedCh" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Total" type="xsd:string" minOccurs="0"/>
							<xsd:element name="TotalsbyMonthDue" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="MonthDueTotals" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="MonthDue" type="xsd:string" minOccurs="0"/>
													<xsd:element name="MonthTotal" type="xsd:double" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
