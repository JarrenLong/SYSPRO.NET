<!-- 
     Schema for the BOM Component Where-used Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="BOMWhereUsedReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="BOMWhereUsedReport" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:double" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="RevisionRelease" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_RevRel" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Routing" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NewPagePerItem" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_NewPagePerItem" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintByRevisionRelease" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintByRevisionRelease" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LevelOfStructure" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_LevelOfStructure" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintManufacturingUMQuantities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintManufacturingUMQnts" type="xsd:string" minOccurs="0"/>
              <xsd:element name="XslStylesheet" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WarehouseFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_WarehouseFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WarehouseFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="BOMStockItem">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="BOMStockHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="INVMST_Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DrawOfficeNum" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StockingUnitOfMeasure" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PartCategory" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ReportKey" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="BOMDetails" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="BOMDetail" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Level" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ParentPart" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SequenceNum" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ParentPartDescription" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QuantityPer" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="StockingUnitOfMeasure" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="FixedQuantity" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ParentUnitOfMeasure" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StructureOnDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StructureOffDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ScrapQuantity" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="ScrapPercentage" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="OperationNumber" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="OperationOffset" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="OperationOffsetFlag" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_OperationOffset" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PartCategory" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
