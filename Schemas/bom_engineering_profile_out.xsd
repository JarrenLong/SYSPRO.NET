<!-- 
     Schema for the BOM Engineering Profile Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="BOMEngineeringProfile" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="BOMEngineeringProfile" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:double" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="RevisionRelease" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_RevisionRelease" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Routing" type="xsd:string" minOccurs="0"/>
              <xsd:element name="QuantityToMake" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_QuantityToMake" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintManufacturingUMQuantities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintMUMQuantities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="EngineeringProfile">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="StockCodeHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="INVMST_Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LongDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Decimals" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="QuantityToMake" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_QuantityToMake" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ParentUnitOfMeasure" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EconomicBatchQuantity" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_EconomicBatchQuantity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PanQuantity" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_PanQuantity" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Type" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Type" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Level" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Component" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Component" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Offset" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Offset" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Operation" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Operation" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WorkCenter" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CoProduct" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_WorkCenter" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PartCategory" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_PartCategory" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Route" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UnitOfMeasure" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalQty" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_TotalQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UnitQty" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_UnitQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_TotalTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UnitTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_UnitTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="QuantityPer" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MaterialPerc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LaborPerc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SubcontractPerc" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
