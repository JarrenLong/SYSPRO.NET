              <xsd:element name="Translated_InclInadmissBulkIssueFlag" type="xsd:string" minOccurs="0"/>
              <xsd:element name="InadmissibleTraceableInspectionFlag" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ExclInadmissTrcInspectFlg" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_InclInadmissTrcInspectFlg" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CannotBeATemporaryItem" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ExclCannotBeATempItem" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_InclCannotBeATempItem" type="xsd:string" minOccurs="0"/>
