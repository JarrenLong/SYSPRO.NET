<!-- 
     Schema for the CB Journal Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="CashbookJournalReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="CashbookJournalReport" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_ReprintDisOption" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PostingPeriod" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PostingYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BankFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_BankFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BankFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="JournalFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_JournalFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="JournalFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="JournalItem">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="JournalHeader" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Journal" type="xsd:string" minOccurs="0"/>
										<xsd:element name="FinancialPostingPeriod" type="xsd:string" minOccurs="0"/>
										<xsd:element name="GLPostingPeriod" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
										<xsd:element name="APSBNK_Description" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Operator" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ADMOPR_Name" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Source" minOccurs="0"/>
										<xsd:element name="Translated_Source" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Reprint" minOccurs="0"/>
										<xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
										<xsd:element name="JnlDate" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="JournalDetails" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="JournalDetail" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="TrnDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="CbTrnTime" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
													<xsd:element name="AdjustmentType" minOccurs="0"/>
													<xsd:element name="Translated_AdjustmentType" type="xsd:string" minOccurs="0"/>
													<xsd:element name="EntryType" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Translated_EntryType" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Cheque" type="xsd:string" minOccurs="0"/>
													<xsd:element name="TrnValue" type="xsd:double" minOccurs="0"/>
													<xsd:element name="EditedTrnValue" type="xsd:string" minOccurs="0"/>
													<xsd:element name="PostCurrency" type="xsd:string" minOccurs="0"/>
													<xsd:element name="TBLCUR_Description" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ConvRate" type="xsd:double" minOccurs="0"/>
													<xsd:element name="PostValue" type="xsd:double" minOccurs="0"/>
													<xsd:element name="EditedPostValue" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Narration1" type="xsd:string" minOccurs="0"/>
													<xsd:element name="TriangulationEntry" minOccurs="0"/>
													<xsd:element name="DistributionEntries" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="DistributionEntry" minOccurs="0" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="Narration2" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="GlCode" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="GENMST_Description" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Company" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="DistValue" type="xsd:double" minOccurs="0"/>
																			<xsd:element name="EditedDistValue" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="JobNumber" minOccurs="0"/>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="JournalTotal" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="BankCurrency" type="xsd:string" minOccurs="0"/>
										<xsd:element name="BankCurrencyDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="BankCurrencyTotalDeposits" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedBankCurrencyTotalDeposits" type="xsd:string" minOccurs="0"/>
										<xsd:element name="BankCurrencyTotalWithdrawals" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedBankCurrencyTotalWithdrawals" type="xsd:string" minOccurs="0"/>
										<xsd:element name="BankCurrencyTotal" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedBankCurrencyTotal" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LocalCurrencyDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LocalCurrencyTotalDeposits" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedLocalCurrencyTotalDeposits" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LocalCurrencyTotalWithdrawals" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedLocalCurrencyTotalWithdrawals" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LocalCurrencyTotal" type="xsd:double" minOccurs="0"/>
										<xsd:element name="EditedLocalCurrencyTotal" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
