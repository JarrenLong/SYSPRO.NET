<!-- 
     Schema for the CB Online Payment Register
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="CashbookOnlineRegsiter" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="CashbookOnlineRegsiter" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>														
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PurgeRegisters" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PostingPeriod" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PostingYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="XslStyleSheet" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RegisterFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RegisterFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="RegisterItem">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="RegisterHeader" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Register" type="xsd:string" minOccurs="0"/>
										<xsd:element name="RegisterDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="FinancialPeriod" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
										<xsd:element name="APSBNK_Description" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="RegisterDetails" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="RegisterDetail" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="Line" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Cheque" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ChequeDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ChequeValue" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
													<xsd:element name="TBLCUR_Description" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Notation" minOccurs="0"/>
													<xsd:element name="AlignmentFunction" minOccurs="0"/>
													<xsd:element name="AlignmentDummyCheckFrom" minOccurs="0"/>
													<xsd:element name="AlignmentDummyCheckTo" minOccurs="0"/>
													<xsd:element name="Remittanceoverflow" minOccurs="0"/>
													<xsd:element name="RemittanceDummyCheckFrom" minOccurs="0"/>
													<xsd:element name="RemittanceDummyCheckTo" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
