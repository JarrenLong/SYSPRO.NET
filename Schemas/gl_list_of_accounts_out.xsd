<!-- 
     Schema for the GL List of Accounts Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="GlListOfAccounts" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="GlListOfAccounts" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>														
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Sequence" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeAssets" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeLiabilities" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeCapital" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeRevenue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeExpenses" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeStatistical" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeTemplate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlGroupFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlGroupFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlCodeFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlCodeFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="AccountDetails">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="GlCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AccountType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ControlAccount" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OnHold" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Password" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportIndex1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportIndex2" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IntegrationMethod" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RevaluationCategory" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CompanyTotals">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="NoOfAccountsListed" type="xsd:double" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
