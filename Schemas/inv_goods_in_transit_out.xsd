<!-- 
     Schema for the Inventory Goods In Transit Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="GITReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="GITReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Sequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Sequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeCompletedGit" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeCompletedGit" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RevisionRelease" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_RevisionRelease" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReferenceFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReferenceFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReferenceFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ProductClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductClassFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SourceWarehouseFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_SourceWarehouseFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SourceWarehouseFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TargetWarehouseFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_TargetWarehouseFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TargetWarehouseFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="References">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReferenceItem" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ReferenceHeader" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="GtrReference" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Line" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SourceWarehouse" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SourceWhDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TargetWarehouse" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TargetWhDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ProductClass" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ProductClassDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StockCodeDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Decimals" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="TransferredQty" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="TransferredQtyUnits" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedTransferredQty" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TransferredValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedTransferredValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="RecQty" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="RecQtyUnits" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedRecQty" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="RecValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedRecValue" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="TransactionItem" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TransactionDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TransactionTypeDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_TransactionTypeDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Bin" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Quantity" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="QuantityUnits" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedQuantity" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Value" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedValue" minOccurs="0">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="17#500.00"/>
                                <xsd:enumeration value="35#000.00"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                          <xsd:element name="UnitCost" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedUnitCost" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="RevRel" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="StkOutstandingTotal" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TotQuantity" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="TotQuantityUnits" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EditedTotQuantity" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedTotValue" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
