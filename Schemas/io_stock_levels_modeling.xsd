<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2010 SYSPRO Ltd.--><!--      Schema for the IO Levels Query Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="AnalysisType">    <xsd:annotation>      <xsd:documentation>The analysis type required for the report. S-Sales, C-Current forecasts, D-Draft forecasts, Default = S.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; S=Sales;C=Current forecasts; D=Draft forecasts"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="S"/>        <xsd:enumeration value="C"/>        <xsd:enumeration value="D"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ApplyBatchingRule">    <xsd:annotation>      <xsd:documentation>This determines whether the the batching rule defined against the stock item will be used to pad the suggested maximum. Y-Yes, N-No, Default = N.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="N"/>        <xsd:enumeration value="Y"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ApplyGrossReqRule">    <xsd:annotation>      <xsd:documentation>This will apply the gross requirements rule as defined against the stock master for that stock code. If set to N, then the Forecasts rule is used. Y-Yes, N-No, Default = N.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Buyer">    <xsd:annotation>      <xsd:documentation>This specifies the selection of Buyers to include in the report.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;browse=InvBuyer;A=All;S=Single;R=Range"/>    </xsd:annotation>    <xsd:complexType>      <xsd:attribute name="FilterType" use="optional" default="A">        <xsd:simpleType>          <xsd:restriction base="xsd:string">            <xsd:enumeration value="A"/>            <xsd:enumeration value="S"/>            <xsd:enumeration value="R"/>          </xsd:restriction>        </xsd:simpleType>      </xsd:attribute>      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>    </xsd:complexType>  </xsd:element>  <xsd:element name="CalendarToUse" type="xsd:string">    <xsd:annotation>      <xsd:documentation>This is a user defined calendar that is used to calculate the periods against which all stock levels are modeled.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="FaComparisonType">    <xsd:annotation>      <xsd:documentation>The type of comparison you would like to use for comparing forecasts. P-Period on period, M-Moving average, Default = P.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; P=Period on period;M=Moving average"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="P"/>        <xsd:enumeration value="M"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="FaForecastToUse">    <xsd:annotation>      <xsd:documentation>This is the forecast that is compared to the sales. L–Last (the last snapshot taken for a period), l-Last outside lead time (the last snapshot taken outside of the lead time for the item), A-Average (average of the snapshots that apply to a period), a-Average outside of lead time (average of snapshots created for a period outside of lead time), default = L.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; L= Last;l=Last outside lead time;A=Average;a=Average outside of lead time"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="L"/>        <xsd:enumeration value="l"/>        <xsd:enumeration value="A"/>        <xsd:enumeration value="a"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="FaMonthsToCompare">    <xsd:annotation>      <xsd:documentation>This is the number of months back you wish to compare sales history against forecasts to check the forecast accuracy (range 1 - 12).</xsd:documentation>      <xsd:appinfo source="screenItem=Y; 01=01;02=02;03=03;04=04;05=05;06=06;07=07;08=08;09=09;10=10;11=11;12=12"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="01"/>        <xsd:enumeration value="02"/>        <xsd:enumeration value="03"/>        <xsd:enumeration value="04"/>        <xsd:enumeration value="05"/>        <xsd:enumeration value="06"/>        <xsd:enumeration value="07"/>        <xsd:enumeration value="08"/>        <xsd:enumeration value="09"/>        <xsd:enumeration value="10"/>        <xsd:enumeration value="11"/>        <xsd:enumeration value="12"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="FaMovingAverageMonths">    <xsd:annotation>      <xsd:documentation>This is the number of months to use to calculate a moving average for sales and forecasts (range 2 - 6).</xsd:documentation>      <xsd:appinfo source="screenItem=Y; 02=02;03=03;04=04;05=05;06=06"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="02"/>        <xsd:enumeration value="03"/>        <xsd:enumeration value="04"/>        <xsd:enumeration value="05"/>        <xsd:enumeration value="06"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="FaSnapshotMonths">    <xsd:annotation>      <xsd:documentation>How many months of forecasts you wish to use to calculate the average forecast (range 1 - 6).</xsd:documentation>      <xsd:appinfo source="screenItem=Y; 1=01;2=02;03=03;04=04;05=05;06=06"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="1"/>        <xsd:enumeration value="2"/>        <xsd:enumeration value="03"/>        <xsd:enumeration value="04"/>        <xsd:enumeration value="05"/>        <xsd:enumeration value="06"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Filter">    <xsd:annotation>      <xsd:documentation>The parent filter element</xsd:documentation>      <xsd:appinfo source="screenItem=N"/>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="StockCode"/>        <xsd:element ref="Warehouse"/>        <xsd:element ref="Planner"/>        <xsd:element ref="Buyer"/>        <xsd:element ref="Supplier"/>        <xsd:element ref="ProductClass"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="ForecastBase">    <xsd:annotation>      <xsd:documentation>This defines the forecast on which the modeling is based. L-Live forecast, D-Draft forecast, S-last Snapshot, Default = L.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; L=Live forecast; D=Draft forecast;S=Last snapshot"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="L"/>        <xsd:enumeration value="D"/>        <xsd:enumeration value="S"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="HorizonMonths" type="xsd:string">    <xsd:annotation>      <xsd:documentation>This sets the number of months into the future for which you want to model the stock levels (range 1 - 99).</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="IncludeClassItemsA">    <xsd:annotation>      <xsd:documentation>This will include class A items on the report. Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeClassItemsB">    <xsd:annotation>      <xsd:documentation>This will include class B items on the report. Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeClassItemsC">    <xsd:annotation>      <xsd:documentation>This will include class C items on the report. Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeClassItemsD">    <xsd:annotation>      <xsd:documentation>This will include class D items on the report. Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeDependantDemand">    <xsd:annotation>      <xsd:documentation>This determines whether or not to include the WIP demand for an item. Y-Yes, N-No, Default = N.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeMakeToOrderItems">    <xsd:annotation>      <xsd:documentation>This will include MTO items on the report: Y-Yes, N-No, O-Only, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No;O=Only"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>        <xsd:enumeration value="O"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeManuallyForecastItems">    <xsd:annotation>      <xsd:documentation>This will include manually forecast items on the report. Y-Yes, N-No, O-Only, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No;O=Only"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>        <xsd:enumeration value="O"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeMasterProductionScheduledItems">    <xsd:annotation>      <xsd:documentation>This will include MPS items on the report: Y-Yes, N-No, O-Only, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No;O=Only"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>        <xsd:enumeration value="O"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeNomovementClassItemsE">    <xsd:annotation>      <xsd:documentation>This will include class E items on the report which are items with no movements. Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludePartTypeBoughtOut">    <xsd:annotation>      <xsd:documentation>This will include bought-out parts on the report: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludePartTypeKitPart">    <xsd:annotation>      <xsd:documentation>This will include kit parts on the report: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="Y"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludePartTypeMadeIn">    <xsd:annotation>      <xsd:documentation>This will include made-in parts on the report: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludePartTypeSubcontracted">    <xsd:annotation>      <xsd:documentation>This will include subcontracted parts on the report: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludePartTypeByProduct">    <xsd:annotation>      <xsd:documentation>This will include by-products parts on the report: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludePartTypeCoProduct">    <xsd:annotation>      <xsd:documentation>This will include co-products on the report: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeReplenishmentItems">    <xsd:annotation>      <xsd:documentation>This will include replenishment items on the report: Y-Yes, N-No, O-Only, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No;O=Only"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>        <xsd:enumeration value="O"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="IncludeUnclassifiedItems">    <xsd:annotation>      <xsd:documentation>This will include unclassified items on the report. Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Option">    <xsd:annotation>      <xsd:documentation>The parent option element</xsd:documentation>      <xsd:appinfo source="screenItem=N"/>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="UseSelectionSet"/>        <xsd:element ref="SelectionSet"/>        <xsd:element ref="IncludePartTypeMadeIn"/>        <xsd:element ref="IncludePartTypeBoughtOut"/>        <xsd:element ref="IncludePartTypeSubcontracted"/>        <xsd:element ref="IncludePartTypeKitPart"/>        <xsd:element ref="IncludePartTypeCoProduct"/>        <xsd:element ref="IncludePartTypeByProduct"/>        <xsd:element ref="IncludeManuallyForecastItems"/>        <xsd:element ref="IncludeReplenishmentItems"/>        <xsd:element ref="IncludeMasterProductionScheduledItems"/>        <xsd:element ref="IncludeMakeToOrderItems"/>        <xsd:element ref="IncludeClassItemsA"/>        <xsd:element ref="IncludeClassItemsB"/>        <xsd:element ref="IncludeClassItemsC"/>        <xsd:element ref="IncludeClassItemsD"/>        <xsd:element ref="IncludeNomovementClassItemsE"/>        <xsd:element ref="IncludeUnclassifiedItems"/>        <xsd:element ref="AnalysisType"/>        <xsd:element ref="ParetoLevel"/>        <xsd:element ref="RunDate"/>        <xsd:element ref="ForecastBase"/>        <xsd:element ref="ApplyGrossReqRule"/>        <xsd:element ref="IncludeDependantDemand"/>        <xsd:element ref="ApplyBatchingRule"/>        <xsd:element ref="PeriodDaysToUse"/>        <xsd:element ref="FaMonthsToCompare"/>        <xsd:element ref="FaComparisonType"/>        <xsd:element ref="FaMovingAverageMonths"/>        <xsd:element ref="FaForecastToUse"/>        <xsd:element ref="FaSnapshotMonths"/>        <xsd:element ref="CalendarToUse"/>        <xsd:element ref="HorizonMonths"/>        <xsd:element ref="ReturnSkuLevel"/>        <xsd:element ref="ReturnConLevel"/>        <xsd:element ref="ReturnPclLevel"/>        <xsd:element ref="ReturnPlnLevel"/>        <xsd:element ref="ReturnBuyLevel"/>        <xsd:element ref="ReturnWhsLevel"/>        <xsd:element ref="ReturnParetoLevel"/>        <xsd:element ref="ReturnSupLevel"/>        <xsd:element name="ReturnShortage">          <xsd:annotation>            <xsd:documentation>Returns shortage levels Y =Yes; N=No</xsd:documentation>          </xsd:annotation>          <xsd:simpleType>            <xsd:restriction base="xsd:string">              <xsd:enumeration value="Y"/>              <xsd:enumeration value="N"/>            </xsd:restriction>          </xsd:simpleType>        </xsd:element>        <xsd:element name="ReturnAverage">          <xsd:annotation>            <xsd:documentation>Returns average levels = Y=Yes; N=No</xsd:documentation>          </xsd:annotation>          <xsd:simpleType>            <xsd:restriction base="xsd:string">              <xsd:enumeration value="Y"/>              <xsd:enumeration value="N"/>            </xsd:restriction>          </xsd:simpleType>        </xsd:element>        <xsd:element name="ReturnLiveLevels">          <xsd:annotation>            <xsd:documentation>Returns Live Levels Y=Yes; N=No</xsd:documentation>          </xsd:annotation>          <xsd:simpleType>            <xsd:restriction base="xsd:string">              <xsd:enumeration value="Y"/>              <xsd:enumeration value="N"/>            </xsd:restriction>          </xsd:simpleType>        </xsd:element>        <xsd:element name="ReturnDraftLevels">          <xsd:annotation>            <xsd:documentation>Returns draft levels Y=Yes; N=No</xsd:documentation>          </xsd:annotation>          <xsd:simpleType>            <xsd:restriction base="xsd:string">              <xsd:enumeration value="Y"/>              <xsd:enumeration value="N"/>            </xsd:restriction>          </xsd:simpleType>        </xsd:element>        <xsd:element ref="ReturnValues"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="ParetoLevel">    <xsd:annotation>      <xsd:documentation>The pareto level required. 1-Stock code, 2-Warehouse, 3-Product class, 4-Supplier, 5-Planner, 6-Buyer, Default = 1.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;1=Stock code; 2=Warehouse; 3=Product class; 4=Supplier; 5=Planner; 6=Buyer"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="1"/>        <xsd:enumeration value="2"/>        <xsd:enumeration value="3"/>        <xsd:enumeration value="4"/>        <xsd:enumeration value="5"/>        <xsd:enumeration value="6"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="PeriodDaysToUse">    <xsd:annotation>      <xsd:documentation>This selects whether to use all days in the periods or just the working days when caluculating the suggested maximums. T-Total, W-Working, Default = T.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;T=Total;W=Working"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="T"/>        <xsd:enumeration value="W"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Planner">    <xsd:annotation>      <xsd:documentation>This specifies the selection of Planners to include in the report.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;browse=InvPlanner; A=All;S=Single;R=Range"/>    </xsd:annotation>    <xsd:complexType>      <xsd:attribute name="FilterType" use="optional" default="A">        <xsd:simpleType>          <xsd:restriction base="xsd:string">            <xsd:enumeration value="A"/>            <xsd:enumeration value="S"/>            <xsd:enumeration value="R"/>          </xsd:restriction>        </xsd:simpleType>      </xsd:attribute>      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>    </xsd:complexType>  </xsd:element>  <xsd:element name="ProductClass">    <xsd:annotation>      <xsd:documentation>This specifies the selection of Product classes to include in the report.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;browse=SalProductClassDes;A=All;S=Single;R=Range"/>    </xsd:annotation>    <xsd:complexType>      <xsd:attribute name="FilterType" use="optional" default="A">        <xsd:simpleType>          <xsd:restriction base="xsd:string">            <xsd:enumeration value="A"/>            <xsd:enumeration value="S"/>            <xsd:enumeration value="R"/>          </xsd:restriction>        </xsd:simpleType>      </xsd:attribute>      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>    </xsd:complexType>  </xsd:element>  <xsd:element name="Query">    <xsd:annotation>      <xsd:documentation>The root element</xsd:documentation>      <xsd:appinfo source="screenItem=N"/>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Option"/>        <xsd:element ref="Filter"/>      </xsd:sequence>      <xsd:attribute name="FilterType" use="optional" default="A">        <xsd:simpleType>          <xsd:restriction base="xsd:string">            <xsd:enumeration value="A"/>            <xsd:enumeration value="S"/>            <xsd:enumeration value="R"/>          </xsd:restriction>        </xsd:simpleType>      </xsd:attribute>      <xsd:attribute name="FilterValue" type="xsd:string"/>    </xsd:complexType>  </xsd:element>  <xsd:element name="ReturnBuyLevel">    <xsd:annotation>      <xsd:documentation>This will return measures on a buyer level: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ReturnConLevel">    <xsd:annotation>      <xsd:documentation>This will return measures on a consolidated SKU level: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ReturnParetoLevel">    <xsd:annotation>      <xsd:documentation>This will return measures on a Pareto classification level: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ReturnSupLevel">    <xsd:annotation>      <xsd:documentation>This will return measures on a supplier level: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ReturnLiveLevels">    <xsd:annotation>      <xsd:documentation>This will return live levels: Y-Yes, N-No, Default = Y.</xsd:documentation>      <xsd:appinfo source="screenItem=Y; Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ReturnDraftLevels"> 