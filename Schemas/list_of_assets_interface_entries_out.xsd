<!-- 
     Schema for the List of Assets Interface Entries Business Object
-->
<xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="Query">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string"/>
              <xsd:element name="Language" type="xsd:string"/>
              <xsd:element name="DecFormat" type="xsd:string"/>
              <xsd:element name="DateFormat" type="xsd:string"/>
              <xsd:element name="Role" type="xsd:string"/>
              <xsd:element name="Version" type="xsd:string"/>
              <xsd:element name="CompanyId" type="xsd:string"/>
              <xsd:element name="CompanyName" type="xsd:string"/>
              <xsd:element name="OperatorCode" type="xsd:string"/>
              <xsd:element name="OperatorName" type="xsd:string"/>
              <xsd:element name="OperatorGroup" type="xsd:string"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string"/>
              <xsd:element name="OperatorLocation" type="xsd:string"/>
              <xsd:element name="LocalCurrency" type="xsd:string"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string"/>
              <xsd:element name="ReportDate" type="xsd:string"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string"/>
              <xsd:element name="SeparatorToUse" type="xsd:string"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions" type="xsd:string"/>
        <xsd:element name="ListOfAssetsInterfaceEntries">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Detail" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="AssetIntegDet_Branch" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_AssetCostCenter" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_AssetGroupCode" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_AssetCtlGlCode" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_ProfitLossGlCode" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_RevalResGlCode" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_AssetClearGlCode" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_DepnGlCode" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_AccumDepnGlCode" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_CapexGlCode" type="xsd:string"/>
                    <xsd:element name="AssetIntegDet_PlInterCompGlCode" type="xsd:string"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
