<!-- 
     Schema for the List of BOM Notional Parts Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="BOMNotionalParts" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="BOMNotionalParts" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BOMQtyPerDecimals" type="xsd:double" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ParentPartFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ParentPartFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RouteFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RouteFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="NotionalDetails">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="NotionalDetail" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="ParentPartCount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ParentPart" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ParentPartDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Release" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Route" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CoProductLine" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CoProduct" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CoProductDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CoProductVersion" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CoProductRelease" type="xsd:string" minOccurs="0"/>
										<xsd:element name="OperationOffset" type="xsd:string" minOccurs="0"/>
										<xsd:element name="OptionalFlag" type="xsd:string" minOccurs="0"/>
										<xsd:element name="QtyPer" type="xsd:double" minOccurs="0"/>
										<xsd:element name="PctMaterialCost" type="xsd:double" minOccurs="0"/>
										<xsd:element name="PctLaborCost" type="xsd:double" minOccurs="0"/>
										<xsd:element name="PctSubContractCost" type="xsd:double" minOccurs="0"/>
										<xsd:element name="PlanWeight" type="xsd:double" minOccurs="0"/>
										<xsd:element name="PctQuantity" type="xsd:double" minOccurs="0"/>
										<xsd:element name="MaterialLabourItems" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="MaterialLabourItem" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="ItemType" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LCoProductLine" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LOperation" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LPctLaborCost" type="xsd:float" minOccurs="0"/>
																<xsd:element name="MCoProductLine" type="xsd:string" minOccurs="0"/>
																<xsd:element name="MSequenceNum" type="xsd:string" minOccurs="0"/>
																<xsd:element name="MComponent" type="xsd:string" minOccurs="0"/>
																<xsd:element name="MComponentDescription" type="xsd:string" minOccurs="0"/>
																<xsd:element name="MPctMaterialCost" type="xsd:float" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
