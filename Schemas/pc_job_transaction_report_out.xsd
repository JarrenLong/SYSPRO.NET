<!-- 
     Schema for the P&C Job Transaction Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="PCJobTransactions" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="PCJobTransactions" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_TrnType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeJobsWithoutTransactions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncJobWithoutTrn" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobsOnHold" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JobsOnHold" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UnconfirmedJobs" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_UnconfJobs" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompletedJobs" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CompleteJobs" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintManufacturingUMQuantities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintMUM" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JobFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobClassificationFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JobClassFiltType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobClassificationFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobDeliveryDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JobDelDateFiltType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobDeliveryDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TransactionDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_TrnDateFiltType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_TransactionDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TransactionDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobCompletionDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JobCompDateFiltType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobCompletionDateFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="JobTransactions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="JobTransaction" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="JobHeader" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Job" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="JobDescription" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="JobClassification" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Contract" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LastReviewDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CustomerName" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="JobDeliveryDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ActCompleteDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="JobStatus" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_JobStatus" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SalesOrder" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SalesOrderLine" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="TransactionType" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TransactionDetail" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TransactionTypeDesc" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_TransactionTypeDesc" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="HeadSection" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_HeadSection" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJDET_Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPOT_PostingDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PORDET_MStockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PORDET_MWarehouse" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="OutstandingQuantity" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_OutstandingQuantity" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PurchaseOrderValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PurchaseOrderValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PORDET_MOrderUom" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PORDET_MPriceUom" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PoMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_PoMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PORDET_PurchaseOrder" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="LnMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_LnMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PORDET_Line" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PORDET_MStockDes" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SubOpMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_SubOpMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PORDET_MSubcontractOp" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_TrnDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_MStockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_MDescription" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_MWarehouse" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_MQtyIssued" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_WIPPST_MQtyIssued" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_TrnValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_WIPPST_TrnValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="OperationTime" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Edited_OperationTime" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_LQtyComplete" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_WIPPST_LQtyComplete" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_MUom" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_Journal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="OpMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_OpMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_LOperation" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SupplierMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_SupplierMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPLAL_SubSupplier" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WorkCenterMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_WorkCenterMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_LWorkCentre" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_LWorkCentreDesc" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ReferenceMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_ReferenceMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_LReference" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="NotationMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translation_NotationMessage" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="WIPPST_LAddReference" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_TrnDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_SalesOrder" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_SalesOrderLine" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_TrnSource" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_PRJPBD_TrnSource" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_BillingValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PRJPBD_BillingValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_CostOfSalesVal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PRJPBD_CostOfSalesVal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_Quantity" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PRJPBD_Quantity" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_QtyReleased" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_PRJPBD_QtyReleased" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJPBD_PartBillingJnl" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJORD_PostDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJORD_SalesOrder" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PRJORD_SalesOrderLine" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Value" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_Value" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SORDET_MStockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SORDER_MStockDes" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="TransactionTypeTotal" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="BillingsValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_BillingsValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CostOfSalesValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_CostOfSalesValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Value" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_Value" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
