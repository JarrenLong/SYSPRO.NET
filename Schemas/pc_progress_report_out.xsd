<!-- 
     Schema for the P&C Job Transaction Query Business Object
-->
<xsd:schema id="PCProgressQuery" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="PCProgressQuery" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0" />
              <xsd:element name="Language" type="xsd:string" minOccurs="0" />
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0" />
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0" />
              <xsd:element name="Role" type="xsd:string" minOccurs="0" />
              <xsd:element name="Version" type="xsd:string" minOccurs="0" />
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0" />
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0" />
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0" />
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0" />
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0" />
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0" />
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0" />
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0" />
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportSequence" type="xsd:string" minOccurs="0" />
              <xsd:element name="CompletedContracts" type="xsd:string" minOccurs="0" />
              <xsd:element name="CompletedJobs" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobsOnHold" type="xsd:string" minOccurs="0" />
              <xsd:element name="UnconfirmedJobs" type="xsd:string" minOccurs="0" />
              <xsd:element name="PrintZeroValueHeads" type="xsd:string" minOccurs="0" />
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="ContractFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="ContractFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobClassificationFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobClassificationFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="ResponsibilityCodeFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="ResponsibilityCodeFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="CustomerFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="CustomerFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="NextReviewDateFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="NextReviewDateFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="NextBillingDateFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="NextBillingDateFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="EstimatedContractCompletionDateFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="EstimatedContractCompletionDateFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobDeliveryDateFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobDeliveryDateFilterValue" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobCompletionDateFilterType" type="xsd:string" minOccurs="0" />
              <xsd:element name="JobCompletionDateFilterValue" type="xsd:string" minOccurs="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ProgressQuery">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ContractDetails" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Contract" type="xsd:string" minOccurs="0" />
                    <xsd:element name="ContractDescription" type="xsd:string" minOccurs="0" />
                    <xsd:element name="Responsibility" type="xsd:string" minOccurs="0" />
                    <xsd:element name="EstimatedCompleteDate" type="xsd:string" minOccurs="0" />
                    <xsd:element name="ActualCompleteDate" type="xsd:string" minOccurs="0" />
                    <xsd:element name="NextReviewDate" type="xsd:string" minOccurs="0" />
                    <xsd:element name="LastReviewDate" type="xsd:string" minOccurs="0" />
                    <xsd:element name="NextBillingDate" type="xsd:string" minOccurs="0" />
                    <xsd:element name="JobDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="JobNumber" type="xsd:string" minOccurs="0" />
                          <xsd:element name="JobDescription" type="xsd:string" minOccurs="0" />
                          <xsd:element name="Customer" type="xsd:string" minOccurs="0" />
                          <xsd:element name="CustomerName" type="xsd:string" minOccurs="0" />
                          <xsd:element name="JobClassification" type="xsd:string" minOccurs="0" />
                          <xsd:element name="JobDeliveryDate" type="xsd:string" minOccurs="0" />
                          <xsd:element name="ActCompleteDate" type="xsd:string" minOccurs="0" />
                          <xsd:element name="SalesOrder" type="xsd:string" minOccurs="0" />
                          <xsd:element name="SalesOrderLine" type="xsd:string" minOccurs="0" />
                          <xsd:element name="HeadCosts" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Head" type="xsd:string" minOccurs="0" />
                                <xsd:element name="HeadDescription" type="xsd:string" minOccurs="0" />
                                <xsd:element name="SalesOriginal" type="xsd:string" minOccurs="0" />
                                <xsd:element name="SalesAdjThisPeriod" type="xsd:string" minOccurs="0" />
                                <xsd:element name="SalesAdjThisYear" type="xsd:string" minOccurs="0" />
                                <xsd:element name="SalesAdjPriorYear" type="xsd:string" minOccurs="0" />
                                <xsd:element name="BillingsThisPeriod" type="xsd:string" minOccurs="0" />
                                <xsd:element name="BillingsThisYear" type="xsd:string" minOccurs="0" />
                                <xsd:element name="BillingsPriorYear" type="xsd:string" minOccurs="0" />
                                <xsd:element name="COSThisPeriod" type="xsd:string" minOccurs="0" />
                                <xsd:element name="COSThisYear" type="xsd:string" minOccurs="0" />
                                <xsd:element name="COSPriorYear" type="xsd:string" minOccurs="0" />
                                <xsd:element name="CommittedPurMaterial" type="xsd:string" minOccurs="0" />
                                <xsd:element name="CommittedPurSubcontract" type="xsd:string" minOccurs="0" />
                                <xsd:element name="CommittedWIP" type="xsd:string" minOccurs="0" />
                                <xsd:element name="UncommittedMaterial" type="xsd:string" minOccurs="0" />
                                <xsd:element name="UncommittedSubcontract" type="xsd:string" minOccurs="0" />
                                <xsd:element name="UncommittedLabor" type="xsd:string" minOccurs="0" />
                                <xsd:element name="MarginCostOriginal" type="xsd:string" minOccurs="0" />
                                <xsd:element name="MarginPercentageOriginal" type="xsd:string" minOccurs="0" />
                                <xsd:element name="HeadTotals" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Sales" type="xsd:string" minOccurs="0" />
                                      <xsd:element name="Billings" type="xsd:string" minOccurs="0" />
                                      <xsd:element name="BalanceToBill" type="xsd:string" minOccurs="0" />
                                      <xsd:element name="COS" type="xsd:string" minOccurs="0" />
                                      <xsd:element name="Committed" type="xsd:string" minOccurs="0" />
                                      <xsd:element name="Uncommitted" type="xsd:string" minOccurs="0" />
                                      <xsd:element name="Margin" type="xsd:string" minOccurs="0" />
                                      <xsd:element name="Percentage" type="xsd:string" minOccurs="0" />
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="JobTotals" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Sales" type="xsd:string" minOccurs="0" />
                                <xsd:element name="Billings" type="xsd:string" minOccurs="0" />
                                <xsd:element name="BalanceToBill" type="xsd:string" minOccurs="0" />
                                <xsd:element name="COS" type="xsd:string" minOccurs="0" />
                                <xsd:element name="Committed" type="xsd:string" minOccurs="0" />
                                <xsd:element name="Uncommitted" type="xsd:string" minOccurs="0" />
                                <xsd:element name="Margin" type="xsd:string" minOccurs="0" />
                                <xsd:element name="Percentage" type="xsd:string" minOccurs="0" />
                                <xsd:element name="ProfitTaken" type="xsd:string" minOccurs="0" />
                                <xsd:element name="ProjectedProfit" type="xsd:string" minOccurs="0" />
                                <xsd:element name="ProfitAvailable" type="xsd:string" minOccurs="0" />
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="ContractTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Sales" type="xsd:string" minOccurs="0" />
                          <xsd:element name="Billings" type="xsd:string" minOccurs="0" />
                          <xsd:element name="BalanceToBill" type="xsd:string" minOccurs="0" />
                          <xsd:element name="COS" type="xsd:string" minOccurs="0" />
                          <xsd:element name="Committed" type="xsd:string" minOccurs="0" />
                          <xsd:element name="Uncommitted" type="xsd:string" minOccurs="0" />
                          <xsd:element name="Margin" type="xsd:string" minOccurs="0" />
                          <xsd:element name="Percentage" type="xsd:string" minOccurs="0" />
                          <xsd:element name="ProfitTaken" type="xsd:string" minOccurs="0" />
                          <xsd:element name="ProjectedProfit" type="xsd:string" minOccurs="0" />
                          <xsd:element name="ProfitAvailable" type="xsd:string" minOccurs="0" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="ReportTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Sales" type="xsd:string" minOccurs="0" />
                    <xsd:element name="Billings" type="xsd:string" minOccurs="0" />
                    <xsd:element name="BalanceToBill" type="xsd:string" minOccurs="0" />
                    <xsd:element name="COS" type="xsd:string" minOccurs="0" />
                    <xsd:element name="Committed" type="xsd:string" minOccurs="0" />
                    <xsd:element name="Uncommitted" type="xsd:string" minOccurs="0" />
                    <xsd:element name="Margin" type="xsd:string" minOccurs="0" />
                    <xsd:element name="Percentage" type="xsd:string" minOccurs="0" />
                    <xsd:element name="ProfitTaken" type="xsd:string" minOccurs="0" />
                    <xsd:element name="ProjectedProfit" type="xsd:string" minOccurs="0" />
                    <xsd:element name="ProfitAvailable" type="xsd:string" minOccurs="0" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>