          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="DateSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OrderStatus" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintLineDueDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintCatalogue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeLocalPurchaseOrders" type="xsd:string" minOccurs="0"/>
