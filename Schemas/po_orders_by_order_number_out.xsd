              <xsd:element name="GrossCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="TotalOrderDiscountsOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="TotalOrderDiscountsCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="FreightChargesOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="FreightChargesCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="MiscChargesOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="MiscChargesCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="CompanyNetOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="CompanyNetCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="TotalNumberOutstandingOrders" type="xsd:double" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TranslatedText" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Text_None" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Entry" type="xsd:string" minOccurs="0"/>
