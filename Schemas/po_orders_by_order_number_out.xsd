              <xsd:element name="Text_Print_date" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Memo_date" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Line_due_date" type="xsd:string" minOccurs="0"/>
