                                <xsd:element name="Catalogue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="OutstandingValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="CurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="PurchaseOrderTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="GrossOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="GrossCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="TotalOrderDiscountsOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="TotalOrderDiscountsCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="FreightChargesOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="FreightChargesCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="MiscChargesOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="MiscChargesCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="HashOrderQty" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="HashReceivedQty" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="NetOutstandingValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="NetCurrentOutstandingValue" type="xsd:double" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
