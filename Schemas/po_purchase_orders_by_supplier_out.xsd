<!-- 
     Schema for the Purchase Orders by Supplier Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="PurchaseOrderBySupplier" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="PurchaseOrderBySupplier" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="RevisionRelease" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ConvertToLocalValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WarehouseFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WarehouseFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BuyerFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BuyerFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ProductClassFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MemoDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MemoDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MemoCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MemoCodeFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Suppliers">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Supplier" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SupplierHeader" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BuyerDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BuyerName" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CurrencyName" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="StockDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="StockCodeHeader" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Decimals" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="LineDetails" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="DetailLine" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="StkLineDesc" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="PurchaseOrder" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Line" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="LatestDueDate" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="LastReceiptDate" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="Price" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="ForeignPrice" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="PriceUom" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OrderQty" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="OrderQtyUnits" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="QtyReceived" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="QtyReceivedUnits" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OutstandingQty" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="OutstandingQtyUnits" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OrderUom" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="OrderPrice" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="OutstandingValue" type="xsd:double" minOccurs="0"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="StockCodeTotals" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="ReceivedQty" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="ReceivedQtyUnits" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="OutstandingQty" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="OutstandingQtyUnits" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="OutstandingValue" type="xsd:double" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="SupplierTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Total" type="xsd:double" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="CompanyTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="HashQtyReceived" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="HashQtyOutstanding" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="CompanyTotal" type="xsd:double" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TranslatedText" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Text_All" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Range" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Single" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Current_rate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Entry_time_rate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_cont_d" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text____Missing_supplier_s_name___" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
