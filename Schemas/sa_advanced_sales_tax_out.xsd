<!--  
     Schema for the Advanced Sales Tax Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="SalesAnalysisAdvancedTaxQuery" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="SalesAnalysisAdvancedTaxQuery" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateSelectionForSummaryReport" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DateSelectionForSummaryReport" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PeriodSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PeriodSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="FromPeriod" type="xsd:string" minOccurs="0"/>
              <xsd:element name="FromYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ToPeriod" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ToYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="FromDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ToDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DateSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StateSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_StateSelection" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SingleState" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_County" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_City" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="SalesTaxList">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="State" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="StateCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="County" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="CountyCode" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="City" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="CityCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Details" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="DetailLine" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="Document" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Date" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="DocumentType" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="CashOrCredit" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="TaxExemptFlag" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="InvoiceAmount" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedInvoiceAmount" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="NonTaxableAmount" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedNonTaxableAmount" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="TaxableAmount" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedTaxableAmount" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="StateAmount" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedStateAmount" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="CountyAmount" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedCountyAmount" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="CityAmount" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedCityAmount" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="TotalTaxAmount" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="EditedTotalTaxAmount" type="xsd:string" minOccurs="0"/>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="CityTotals" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="CityInvoiceAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="EditedCityInvoiceAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="CityNonTaxableAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="EditedCityNonTaxableAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="CityTaxableAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="EditedCityTaxableAmount" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="CityTotalTaxAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="EditedCityTotalTaxAmount" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="CountyTotals" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="CountyInvoiceAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="EditedCountyInvoiceAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CountyNonTaxableAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="EditedCountyNonTaxableAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CountyTaxableAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="EditedCountyTaxableAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CountyTotalTaxAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="EditedCountyTotalTaxAmount" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="StateTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="StateInvoiceAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedStateInvoiceAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StateNonTaxableAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedStateNonTaxableAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StateTaxableAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedStateTaxableAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StateTotalTaxAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedStateTotalTaxAmount" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ReportTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="TotalInvoiceAmount" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedTotalInvoiceAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotalNonTaxableAmount" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedTotalNonTaxableAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotalTaxableAmount" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedTotalTaxableAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotalCityAmount" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedTotalCityAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotalCountyAmount" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedTotalCountyAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotalStateAmount" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedTotalStateAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotalTaxAmount" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedTotalTaxAmount" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
