<!-- 
     Schema for the WIP Employee Efficiency Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="WipEmployeeEfficiency" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="WipEmployeeEfficiency" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="EmployeeFilterType" minOccurs="0">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="EmployeeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_EmployeeFilterType" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="EmployeeDetails">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="EmployeeDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="EmployeeNumber" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EmployeeName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MtdStdHrsEarned" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MtdNonProdHours" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MtdActProdHours" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="StdHrsPerMonth" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="WorkCenter" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EmployeeActCost" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates1" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates2" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates3" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates4" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates5" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates6" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates7" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates8" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Rates9" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="ShiftId" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LostTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="ActualProductiveTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="PercentageProductionUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="PercentageTotalUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="PercentageEfficiency" type="xsd:double" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
