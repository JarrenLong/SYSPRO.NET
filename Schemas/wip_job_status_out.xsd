                    <xsd:element name="JobDeliveryDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JobStartDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Complete" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ExpLabour" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_ExpLabor" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ExpMaterial" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_ExpMaterial" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="QtyToMake" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_QtyToMake" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="QtyOutstanding" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_QtyOutstanding" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="QtyManufactured" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_QtyManufactured" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Route" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BOMROU_Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ConfirmedFlag" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WipCtlGlCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LabCostToDate" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MatCostToDate" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="LabValueIssues" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MatValueIssues" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SchStartDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SchStartTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SchEndDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SchEndTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ScheduleFlag" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Co-Product" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="NotionalPart" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CoProductStockCode" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QuantityToMake" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_QuantityToMake" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="UnitOfMeasure" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="OutstandingQuantity" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_OutstandingQuantity" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="DeliveryDate" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="MaterialOperation" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="NodeType" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Operation" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="WorkCentre" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="WorkCentreDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotRunTimeReqd" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotRunTimeReqd" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotSetStrTrdReqd" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotSetStrTrdReqd" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="RunTimeIssued" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_RunTimeIssued" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotSetStrTrdIss" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TotSetStrTrdIss" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="HighestOpWorkCenter" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LabValReqd" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_LabValReqd" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StockDescription" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Uom" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyReqd" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_QtyReqd" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyIssued" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_QtyIssued" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyConsumed" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_QtyConsumed" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyBalance" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_QtyBalance" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="MatValReqd" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_MatValReqd" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ValueIssued" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_ValueIssued" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="OperCompleted" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="AllocCompleted" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="JobTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="MaterialValReq" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_MaterialValReq" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="MaterialValIss" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_MaterialValIss" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LaborValReq" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_LaborValReq" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LaborValIss" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_LaborValIss" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="JobTotalValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_JobTotalValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="JobTotalIssued" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_JobTotalIssued" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="IssuesFromWIP" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_IssuesFromWIP" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="JobWIPValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_JobWIPValue" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="ReportTotal" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                    <xsd:element name="TotalCompletedJobs" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_TotalCompletedJobs" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalWIPValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_TotalWIPValue" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
