<!-- 
     Schema for the WIP Job Status Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="WIPJobStatusQuery" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="WIPJobStatusQuery" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence minOccurs="0" maxOccurs="unbounded">
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence minOccurs="0" maxOccurs="unbounded">
              <xsd:element name="SubJobsUnderMasterJobs" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_SubJobsUnderMasterJobs" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OnlyCompletedJobsWithWip" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_OnlyCompletedJobsWithWip" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NewPagePerJob" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_NewPagePerJob" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Values" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Values" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeJobsOnHold" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeJobsOnHold" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeJobsCompleted" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeJobsCompleted" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeJobsUnconfirmed" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeJobsUnconfirmed" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ValuesBy" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ValuesBy" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintManufacturingUMQuantities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintManufacturingUMQnts" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JobFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobClassFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JobClassFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JobClassFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustomerFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustomerFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CustomerFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DeliveryDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DeliveryDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DeliveryDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompletionDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompletionDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CompletionDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="JobStatus">
          <xsd:complexType>
            <xsd:sequence minOccurs="0" maxOccurs="unbounded">
              <xsd:element name="Job" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                    <xsd:element name="JobNumber" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JobDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MasterJob" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JobType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MasterSubOrInt" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MasterSubOrIntFlag" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_MasterSubOrInt" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Level" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JobClassification" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ClassDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StockDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LongDesc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StockUom" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="INVWHC_Description" type="xsd:string" minOccurs="0"/>
