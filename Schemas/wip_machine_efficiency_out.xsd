<!-- 
     Schema for the WIP Machine Efficiency Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="Query" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="Query" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="MachineSelectionFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MachineSelectionFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_MachineSelectionFilterType" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="MachineList">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Machine" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MachineDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MtdStdHrsEarned" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MtdNonProdHours" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MtdActualProductiveHours" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="StdHoursPerMonth" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="WorkCenter" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ShiftId" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StandardTimeEarned" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="NonProductiveTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="StandardTimePerMonth" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="LostTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="ActualProductiveTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="PercentageProductionUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="PercentageTotalUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="PercentageEfficiency" type="xsd:double" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
       
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
