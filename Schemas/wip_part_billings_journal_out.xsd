<!-- 
     Schema for the WIP Part Billings Journal Query Business Object
-->
<xsd:schema id="WIPPartBillingsJournalReport" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="WIPPartBillingsJournalReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
							<xsd:element name="MonthSelection" type="xsd:string" minOccurs="0"/>
							<xsd:element name="YearSelection" type="xsd:string" minOccurs="0"/>
							<xsd:element name="JournalNumberSelectionFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_JournalNumberSelFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="JournalNumberSelectionFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="JournalEntries">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="JournalEntriesHeader" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="JournalNo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="FinancialMonth" type="xsd:string" minOccurs="0"/>
										<xsd:element name="FinancialYear" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
										<xsd:element name="OperatorMsg" type="xsd:string" minOccurs="0"/>
										<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="JobPosting" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="JobPostingDetails" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="JobPostingDetail" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Entry" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Job" type="xsd:string" minOccurs="0"/>
																<xsd:element name="SalesOrder" type="xsd:string" minOccurs="0"/>
																<xsd:element name="EntryDate" type="xsd:string" minOccurs="0"/>
																<xsd:element name="QtySupplied" type="xsd:string" minOccurs="0"/>
																<xsd:element name="MaterialValue" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedMaterialValue" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LaborValue" type="xsd:double" minOccurs="0"/>
																<xsd:element name="EditedLaborValue" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="JobPostingTotals" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="JnlTotMsg" type="xsd:string" minOccurs="0"/>
													<xsd:element name="QuantitySupplied" type="xsd:string" minOccurs="0"/>
													<xsd:element name="MaterialValue" type="xsd:double" minOccurs="0"/>
													<xsd:element name="EditedMaterialValue" type="xsd:string" minOccurs="0"/>
													<xsd:element name="LaborValue" type="xsd:double" minOccurs="0"/>
													<xsd:element name="EditedLaborValue" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
