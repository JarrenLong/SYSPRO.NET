<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--     Schema List for the AP Release Schedule Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="BranchSelection">		<xsd:annotation>			<xsd:documentation>This sequence is used to indicate the branch selection.The default is A and the other valid values is S. </xsd:documentation>			<xsd:appinfo source="screenItem=Y;browse=ApBranch;A=All;S=Single"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="optional" default="A">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="Filter">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="SupplierSelection" minOccurs="0"/>				<xsd:element ref="SupplierClassSelection" minOccurs="0"/>				<xsd:element ref="BranchSelection" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="IncludeCreditInvoiceBalance">		<xsd:annotation>			<xsd:documentation>This sequence is used to indicate whether to include the credit invoice balance. The default is Y and the other valid value is N.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;Y=Yes;N=No"/>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="Y"/>				<xsd:enumeration value="N"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="NextPaymentDate" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This sequence is used to indicate the next payment date.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;browse={date}"/>		</xsd:annotation>	</xsd:element>	<xsd:element name="Option">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="PaymentDate" minOccurs="0"/>				<xsd:element ref="NextPaymentDate" minOccurs="0"/>				<xsd:element ref="IncludeCreditInvoiceBalance" minOccurs="0"/>				<xsd:element ref="XslStylesheet" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="PaymentDate" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This sequence is used to indicate the payment date.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;browse={date}"/>		</xsd:annotation>	</xsd:element>	<xsd:element name="Query">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Option"/>				<xsd:element ref="Filter"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="SupplierClassSelection">		<xsd:annotation>			<xsd:documentation>This sequence is used to indicate the supplier class selection.The default is A and the other valid values is S. </xsd:documentation>			<xsd:appinfo source="screenItem=Y;browse=TblSupplierClass;A=All;S=Single"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="optional" default="A">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="SupplierSelection">		<xsd:annotation>			<xsd:documentation>This sequence is used to indicate the supplier selection.The default is A and the other valid values are R and S.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;browse=ApSupplier;A=All;R=Range;S=Single"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="optional" default="A">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="R"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="XslStylesheet">		<xsd:annotation>			<xsd:documentation>This is the style sheet to be applied.</xsd:documentation>		</xsd:annotation>		<xsd:complexType/>	</xsd:element></xsd:schema>