                                <xsd:element name="InvoiceDisc" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_InvoiceDisc" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TaxableAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TaxableAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TaxAmount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TaxAmount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DistributionLines" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Code" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="TaxableAmount" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="Edited_TaxableAmount" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="JournalTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="InvoiceValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_InvoiceValue" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="InvoiceDisc" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_InvoiceDisc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TaxableAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TaxableAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Amount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_Amount" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CompanyTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportJournalTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SecondTierDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Amount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Code" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Taxable_Amount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Rate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Calculated" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="InvoiceValue" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_InvoiceValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="InvoiceDisc" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_InvoiceDisc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TaxableAmount" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_TaxableAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TaxAmount" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_TaxAmount" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="DistributionTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="TaxCodes" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TaxCode" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Code" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ADMTAX_Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CurTaxRate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Edited_CurTaxRate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TaxableAmountTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_TaxableAmountTotal" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AmountTotal" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_AmountTotal" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="TaxCodeTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TaxableAmountTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_TaxableAmountTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="AmountTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_AmountTotal" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
