              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DeleteDistributionFile" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DeleteDistributionFile" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JnlDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JnlDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JnlDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JournalFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JournalFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JournalFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JnlMonthFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JnlMonthFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JnlMonthFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JnlYearFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_JnlYearFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="JnlYearFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="JournalDetails">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="JournalDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="JournalHeader" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Journal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="JnlDate" type="xsd:string" minOccurs="0"/>
