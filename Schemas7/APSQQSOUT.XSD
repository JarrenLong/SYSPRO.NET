                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="JournalLines" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="JournalLine" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SupplierName" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_TransactionType" type="xsd:string" minOccurs="0"/>
