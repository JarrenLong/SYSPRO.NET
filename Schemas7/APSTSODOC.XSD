<?xml version="1.0" encoding="Windows-1252"?>
<!-- Copyright 1994-2014 SYSPRO Ltd.-->
<!-- 
     Schema for the Accounts Payable Setup Transaction Posting Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="AccountsPayableOptions">
		<xsd:annotation>
			<xsd:documentation>This is the root element.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="General" minOccurs="0"/>
				<xsd:element ref="Tax" minOccurs="0"/>
				<xsd:element ref="History" minOccurs="0"/>
				<xsd:element ref="Numbering" minOccurs="0"/>
				<xsd:element ref="Integration" minOccurs="0"/>
				<xsd:element ref="UserDefined" minOccurs="0"/>
				<xsd:element ref="PeriodCurrentYear" minOccurs="0"/>
				<xsd:element ref="PeriodPreviousYear" minOccurs="0"/>
				<xsd:element ref="CurrentMonth" minOccurs="0"/>
				<xsd:element ref="GRN" minOccurs="0"/>
				<xsd:element ref="AutoVouchering" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SuppAmendJnlsReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether the supplier amendment journals are required. (N - No, Y - Yes, A - Include additions). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;A=Include Additions;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
				<xsd:enumeration value="A"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PermEntReqAp">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether permanent entries are required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AskNotPerDetDistrEnt">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to ask for notation per detail distribution entry. (Y - Yes, N - No). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="Y=Yes;N=No;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="Y"/>
				<xsd:enumeration value="N"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ConsolInvByMthDue">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to consolidate invoices by month due at month end. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ValidateSuppClass">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to validate the supplier class on the supplier master. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ValidateGeogArea">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to validate the geographic area on the supplier master. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ContraInvByBranch">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to use contra invoices by branch. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CashProjPeriodDays">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the cash projection period in days. The initial value for this element is 30.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="0"/>
				<xsd:maxInclusive value="999"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CheckPrintReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the check print requirements. (C - Combined check & remittance, S - Separate check & remittance, N - Not required). If no value is supplied then the default value of C will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="C=Combined check and remittance;S=Separate check and remittance;N=Not required;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="S"/>
				<xsd:enumeration value="N"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PayRelDefExchRate">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to use the default invoice exchange rate for a payment. (Y - Use default invoice exchange rate, C - Use current table exchange rate). If no value is supplied then the default of Y will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="Y=Use default invoice exchange rate;C=Use current table exchange rate;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="Y"/>
				<xsd:enumeration value="C"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PayRelApplyToOsInvBal">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to apply current table exchange rate for a payment. (N - No, Y - Use default invoice exchange rate). If no value is supplied then default value of N will be assumed. If 'PayRelDefExchRate' is set to Y, this element must contain N. If 'PayRelDefExchRate' is set to C, then this element may contain either N or Y.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Use default invoice exchange rate;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="General">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SuppAmendJnlsReq"/>
				<xsd:element ref="PermEntReqAp"/>
				<xsd:element ref="AskNotPerDetDistrEnt"/>
				<xsd:element ref="ConsolInvByMthDue"/>
				<xsd:element ref="ValidateSuppClass"/>
				<xsd:element ref="ValidateGeogArea"/>
				<xsd:element ref="ContraInvByBranch"/>
				<xsd:element ref="CashProjPeriodDays"/>
				<xsd:element ref="CheckPrintReq"/>
				<xsd:element ref="PayRelDefExchRate"/>
				<xsd:element ref="PayRelApplyToOsInvBal"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ApTaxDistrMethod">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the tax distribution method. (N - Not required, D - Detail, S - Summary). If no value is supplied then the default value of N will be assumed. This may not be set to S if the EC VAT system is required.]]></xsd:documentation>
			<xsd:appinfo source="N=Not required;D=Detail;S=Summary;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="D"/>
				<xsd:enumeration value="S"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SecondTierTaxReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether the second tier tax is required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. Only applies if 'ApTaxDistrMethod' is not set to N.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SecondTierTaxDesc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[The is the second tier tax description. This element is optional. Only applies if 'ApTaxDistrMethod' is not set to N and 'SecondTierTaxReq' is set to Y.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SecondTierTaxCalcOnTax">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to calculate the 2nd tier tax on merchandise value only or merchandise value + tax. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. Only applies if 'ApTaxDistrMethod' is not set to N.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DisbTaxOverMultGlAccs">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to disburse the tax amount over single or multiple GL tax accounts for invoice posting. (S - Single, M - Multiple). If no value is supplied then the default value of S will be assumed. Only applies if 'ApTaxDistrMethod' is not set to N.]]></xsd:documentation>
			<xsd:appinfo source="S=Single;M=Multiple;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="S"/>
				<xsd:enumeration value="M"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefTaxCodeApInvPost">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the default tax code to use for invoice posting. This element can only be in the range of A-Z. If no value is supplied then A will be assumed. Only applies if 'ApTaxDistrMethod' is not set to N.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefQstCode">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the default 2nd tax code to use for invoice posting. This element can only be in the range of A-Z. If no value is supplied then A will be assumed. Only applies if 'ApTaxDistrMethod' is not set to N.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AllowTaxForForeignSupp">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to allow tax entry for foreign currency suppliers. (Option updated with 0 when N and 1 when Y). If no value is supplied the the default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SuppDefTaxCodeReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether the supplier default tax code is required. (N - No, Y - Yes). If no value is supplied then N will be assumed. Only applies if EC VAT is required.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ReqTaxAmForSettleDisc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to request the tax amount for settlement discount. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. Only applies if 'ApTaxDistrMethod' is not set to N. May not be set to Y if Withholding tax is in use.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefTaxCodeApSettDisc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the default tax code for the tax portion of the discount. This element can only be in the range of A-Z. If no value is supplied then A will be assumed. Only applies if 'ApTaxDistrMethod' is not set to N and 'ReqTaxAmForSettleDisc' is set to Y.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ReqQstAmForSettDisc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to request the 2nd tax amount for settlement discount. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. Only applies if 'ApTaxDistrMethod' is not set to N. May not be set to Y if Withholding tax is in use.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefQstCodeSettDisc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the default tax code for the 2nd tax portion of the discount. If a value is supplied it may only be in the range of A-Z. Only applies if 'ApTaxDistrMethod' is not set to N and 'ReqQstAmForSettDisc' is set to Y.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="Tax">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ApTaxDistrMethod"/>
				<xsd:element ref="SecondTierTaxReq"/>
				<xsd:element ref="SecondTierTaxDesc"/>
				<xsd:element ref="SecondTierTaxCalcOnTax"/>
				<xsd:element ref="SecondTierTaxForeign"/>
				<xsd:element ref="DisbTaxOverMultGlAccs"/>
				<xsd:element ref="DefTaxCodeApInvPost"/>
				<xsd:element ref="DefQstCode"/>
				<xsd:element ref="AllowTaxForForeignSupp"/>
				<xsd:element ref="SuppDefTaxCodeReq"/>
				<xsd:element ref="ReqTaxAmForSettleDisc"/>
				<xsd:element name="CalcTaxUsingHistRates">
					<xsd:annotation>
						<xsd:documentation><![CDATA[Indicates whether tax on settlement discount is to be calculated using the current tax rate or the historical tax rate based on the invoice date. Only applies if 'ReqTaxAmForSettleDisc' is not set to 'N'. (Space - Current, H- Historical). ]]></xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string"/>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="DefTaxCodeApSettDisc"/>
				<xsd:element ref="ReqQstAmForSettDisc"/>
				<xsd:element ref="DefQstCodeSettDisc"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MthsRetZeroBalInv">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain zero balance invoices. This element may not be zeros. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetExpJnls">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain expense journals. This element may not be zeros. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetPayCycle">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain payment cycle details. Entered values must be greater than 5. If no value is supplied then the default value of 6 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetTaxDistrAp">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain tax distribution details. This element may not be zeros. If no value is supplied then the default value of 99 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlPerRetExpDistr">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of G/L periods to retain the expenses distribution entries.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="0"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlPerRetDisbDistr">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of G/L periods to retain the disbursement distribution entries.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="0"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="History">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MthsRetZeroBalInv"/>
				<xsd:element ref="MthsRetExpJnls"/>
				<xsd:element ref="MthsRetPayCycle"/>
				<xsd:element ref="MthsRetTaxDistrAp"/>
				<xsd:element ref="GlPerRetExpDistr"/>
				<xsd:element ref="GlPerRetDisbDistr"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SupplierNumMethod">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the supplier numbering method to use. (M - Manual, A - Automatic, S - Scripted). If no value is supplied then the default value of M will be assumed. This element may only be A if the suppliers key type is set to numeric.]]></xsd:documentation>
			<xsd:appinfo source="M=Manual;A=Automatic;S=Scripted;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="M"/>
				<xsd:enumeration value="A"/>
				<xsd:enumeration value="S"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NextSupplierNum">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the next supplier number. Applies only if 'SupplierNumMethod' is A. If no value is supplied the default of 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PayRunNumMethod">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the payment run numbering method. (M - Manual, A - Automatic). If no value is supplied then the default value of M will be assumed. This element may only be A if the payment run key type is set up as numeric.]]></xsd:documentation>
			<xsd:appinfo source="M=Manual;A=Automatic;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="M"/>
				<xsd:enumeration value="A"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NextPayRunNum">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the next payment run number. Applies only if 'PayRunNumMethod' is A. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="999999999999999"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="Numbering">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SupplierNumMethod"/>
				<xsd:element ref="NextSupplierNum"/>
				<xsd:element ref="PayRunNumMethod"/>
				<xsd:element ref="NextPayRunNum"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ApLinkedToCb">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether the payments are to be posted to the Cash Book. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. Set this option if you wish to link Accounts Payable payments to the Cash Book module. If set, the Accounts Payable Payment Register will post all checks directly into the Cash Book module.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="Integration">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ApLinkedToCb"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DescSupplierUdf1">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the description for the user defined field 1. This element is optional.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
