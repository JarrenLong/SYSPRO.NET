<!--
     Schema for the Query AR Post Dated Check Status Report Business Object
-->
<xsd:schema id="ARPDCStatusReport" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="ARPDCStatusReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="IncludeInvoiceDetail" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeInvoiceDetail" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeLinkedToCollector" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeLinkedToCollector" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeCaptured" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeCaptured" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeBanked" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeBanked" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeCleared" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeCleared" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeVoid" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeVoid" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludePosted" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludePosted" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustomerFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CustomerFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustomerFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DepositBankFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DepositBankFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DepositBankFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CollectorRunNumberFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CollectorRunNumberFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CollectorRunNumberFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AvailableDatesFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_AvailableDatesFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AvailableDatesFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CustomerHeader">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Cutomer" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustomerName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PostDatedCheck" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Cheque" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AvailableDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Amount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_Amount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Narration" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DateReceived" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DepositBank" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DepositBankDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BranchDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CollectorNumberAr" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="VoidAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_VoidAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ClearedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_ClearedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BankedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_BankedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CapturedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_CapturedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Invoices" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="InvoiceNumber" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Invoice" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DocumentType" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_DocumentType" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Amount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Edited_Amount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Allocation" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_Allocation" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="CustomerTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="CapturedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_CapturedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BankedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_BankedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ClearedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_ClearedAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="VoidAmount" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_VoidAmount" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CompanyTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CapturedAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_CapturedAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BankedAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_BankedAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ClearedAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_ClearedAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="VoidAmount" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_VoidAmount" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
