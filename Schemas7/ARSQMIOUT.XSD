<!-- 
     Schema for the AR Missing Invoices Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="ArMissingInvoiceQuery" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="ArMissingInvoiceQuery" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Year" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeInvoices" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeInvoices" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeCreditNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeCreditNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeDebitNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeDebitNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PeriodFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PeriodFilterType" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ArInvoices">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ArInvoice" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="DocumentType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_DocumentType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Invoice" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ArTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="MissingInvoices" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_MissingInvoices" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MissingCreditNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_MissingCreditNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="MissingDebitNotes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Edited_MissingDebitNotes" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
