<!--
     Schema for the AR Payment Run Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="PaymentRunQuery" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="PaymentRunQuery" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="IncludeInvoice" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncludeInvoice" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludePostDatedChecks" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncludePostChecks" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeCollectorRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncludeCollectorRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeARInvoiceRelease" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncludeARInvRelease" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeOpenPayRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncludeOpenPayRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeAuthorizedPayRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncludeAuthdPayRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludePostedPayRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncludePostedPayRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeCancelledPayRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_IncludeCancPayRun" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CustomerFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_CustomerFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BankFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_BankFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PaymentRunNumberFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PaymntRunFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PaymentDateFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PayDateFilterType" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="PaymentRunEntries" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="PaymentHeader" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="PaymentNumber" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Collector" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CollectorDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CollectorNumberAr" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
										<xsd:element name="BankDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CurrencyDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="PaymentType" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_PaymentType" type="xsd:string" minOccurs="0"/>
										<xsd:element name="PaymentRunStatus" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_PaymentRunStatus" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CreatedDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="PaymentDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Salesperson" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SalespersonName" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Area" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AreaDescription" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerEntries" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="CustomerItem" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CustomerName" type="xsd:string" minOccurs="0"/>
																<xsd:element name="PayAmount" type="xsd:double" minOccurs="0"/>
																<xsd:element name="Edited_PayAmount" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CusBalance" type="xsd:double" minOccurs="0"/>
																<xsd:element name="Edited_CusBalance" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CustomerInvoices" minOccurs="0" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="InvoiceItem" minOccurs="0" maxOccurs="unbounded">
																				<xsd:complexType>
																					<xsd:sequence>
																						<xsd:element name="Invoice" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="DocumentType" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="Translated_DocumentType" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="SalesOrder" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="InvoiceDate" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="NoOfAgeDays" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="ConvRate" type="xsd:double" minOccurs="0"/>
																						<xsd:element name="Edited_ConvRate" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="MulDiv" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="Translated_MulDiv" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="GrossPayValue" type="xsd:double" minOccurs="0"/>
																						<xsd:element name="Edited_GrossPayValue" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="DiscPayValue" type="xsd:double" minOccurs="0"/>
																						<xsd:element name="Edited_DiscPayValue" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="WithTaxValue" type="xsd:double" minOccurs="0"/>
																						<xsd:element name="Edited_WithTaxValue" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="TpmValue" type="xsd:double" minOccurs="0"/>
																						<xsd:element name="Edited_TpmValue" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="InvoiceNotation" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="ReferencePoNumber" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="NetPayValue" type="xsd:double" minOccurs="0"/>
																						<xsd:element name="Edited_NetPayValue" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="BankPaymentType" type="xsd:string" minOccurs="0"/>
																						<xsd:element name="Translated_BankPaymentType" type="xsd:string" minOccurs="0"/>
																					</xsd:sequence>
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="CustomerTotals" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="TotalCusPayAmount" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_TotalCusPayAmount" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CompanyTotals" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="TotalCompPayAmount" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_TotalCompPayAmount" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
