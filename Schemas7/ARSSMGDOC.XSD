<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--      Schema for the AP Geographic Area Maintenance Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="Area">		<xsd:annotation>			<xsd:documentation>This is the key to the AR Geographic Area being maintained. This is mandatory. If this AR Geographic Area is being added, it must not pre-exist. If this AR Geographic Area is being changed or deleted, it must be a valid Syspro AR Geographic Area.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="AreaGstCode">		<xsd:annotation>			<xsd:documentation>This is the name of the Geographic area GST codeand is used primarily to identify the AR Geographic Area GST code. Only used for Canadian GST </xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="Description" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This is the name of the AR Geographic Area and is used primarily to identify the AR Geographic Area. </xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Item">		<xsd:annotation>			<xsd:documentation>Mandatory element.This is the top element for all data required for the transaction. This element can occur multiple times as the business object caters for the posting additions, changes and deletions to AR Geographic Area.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="Description"/>				<xsd:element ref="TaxCode" minOccurs="0"/>				<xsd:element ref="AreaGstCode" minOccurs="0"/>				<xsd:element ref="ProvTaxFlag" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Key">		<xsd:annotation>			<xsd:documentation>This element contains the key information required to maintain a AR Geographic Area.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Area"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="ProvTaxFlag">		<xsd:annotation>			<xsd:documentation>Specifies whether to use Provincial tax calculated. (N - Don't use provincial calculated tax , Y - Use provincial calculated tax). Only used for Canadian GST</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="SetupArGeographicArea">		<xsd:annotation>			<xsd:documentation>This is the top element for all data required for AR Geographic Area maintenance.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="TaxCode">		<xsd:annotation>			<xsd:documentation>This is the name of the Geographic area tax codeand is used primarily to identify the AR Geographic Area tax code.When using Descriptive Tax Codes, a descriptive tax code must be provided. </xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="UseStockCodeGST">		<xsd:annotation>			<xsd:documentation>Specifies whether to use Geographic area GST code. (N - Use GST code, Y - Use GST code from stock item). Only used for Canadian GST</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="UseStockCodeTax">		<xsd:annotation>			<xsd:documentation>Specifies whether to use tax code from stock item. (N - Use tax code, Y - Use tax code from stock item).</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element></xsd:schema>