<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="Branch">
    <xsd:annotation>
      <xsd:documentation>This element is used to add update or delete the Branch.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:whiteSpace value="preserve"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CommissionPct">
    <xsd:annotation>
      <xsd:documentation>This element is used to add update or delete the Commission Percentage.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:whiteSpace value="preserve"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Item">
    <xsd:annotation>
      <xsd:documentation>This is the sub root node</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="Key"/>
        <xsd:element ref="Name"/>
        <xsd:element ref="CommissionPct" minOccurs="0"/>
        <xsd:element ref="SalesBudget1" minOccurs="0"/>
        <xsd:element ref="SalesBudget2" minOccurs="0"/>
        <xsd:element ref="SalesBudget3" minOccurs="0"/>
        <xsd:element ref="SalesBudget4" minOccurs="0"/>
