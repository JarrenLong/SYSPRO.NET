        <xsd:element ref="SalesBudget11" minOccurs="0"/>
        <xsd:element ref="SalesBudget12" minOccurs="0"/>
        <xsd:element ref="SalesBudget13" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Key">
    <xsd:annotation>
      <xsd:documentation>This is the key node.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="Branch"/>
        <xsd:element ref="Salesperson"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
