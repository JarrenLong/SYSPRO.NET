<!--
     Schema for the Post AR to Cashbook Integration Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="Parameters">
		<xsd:annotation>
			<xsd:documentation>This contains paramaters. This business object does not require any parameters to be supplied.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="false">
			<xsd:sequence/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PostArToCashbookIntegration">
		<xsd:annotation>
			<xsd:documentation>This is the root element.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="Parameters"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
