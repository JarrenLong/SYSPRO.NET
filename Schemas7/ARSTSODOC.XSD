<?xml version="1.0" encoding="Windows-1252"?>
<!-- Copyright 1994-2014 SYSPRO Ltd.-->
<!-- 
     Schema for the Accounts Receivable Setup Transaction Posting Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="AccountsReceivableOptions">
		<xsd:annotation>
			<xsd:documentation>This is the root element.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="General" minOccurs="0"/>
				<xsd:element ref="TermsDiscount" minOccurs="0"/>
				<xsd:element ref="Tax" minOccurs="0"/>
				<xsd:element ref="History" minOccurs="0"/>
				<xsd:element ref="Numbering" minOccurs="0"/>
				<xsd:element ref="Integration" minOccurs="0"/>
				<xsd:element ref="UserDefined" minOccurs="0"/>
				<xsd:element ref="PeriodCurrentYear" minOccurs="0"/>
				<xsd:element ref="PeriodPreviousYear" minOccurs="0"/>
				<xsd:element ref="CurrentMonth" minOccurs="0"/>
				<xsd:element name="PaymentRun" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="PaymentsBySalesperson"/>
							<xsd:element name="PaymentsByGeographicArea">
								<xsd:annotation>
									<xsd:documentation><![CDATA[Indicates whether to allow payments by geographic area (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="CollectorRequired">
								<xsd:annotation>
									<xsd:documentation><![CDATA[Indicates whether a collector is required(N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="AuthorizationRequired">
								<xsd:annotation>
									<xsd:documentation><![CDATA[Indicates whether authorization is required(N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="AllocateInvoiceToPDCheck">
								<xsd:annotation>
									<xsd:documentation><![CDATA[Indicates whether to allocate an invoice to a PD check (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed. This option will be forced a Y if collector required has a Y value.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="PDCheckStatusRequired">
								<xsd:annotation>
									<xsd:documentation><![CDATA[Indicates whether PD Check status is required or not (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="PaymentRunNumMethod">
								<xsd:annotation>
									<xsd:documentation><![CDATA[This is the payment run numbering method. (M - Manual, A - Automatic). If no value is supplied then the default value of M will be assumed. This element may only be A if the payment run key type is set up as numeric.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="NextPaymentRunNum">
								<xsd:annotation>
									<xsd:documentation><![CDATA[This is the next payment run number. Applies only if 'PaymentRunNumMethod' is A. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="CollectionRunNumMethod">
								<xsd:annotation>
									<xsd:documentation><![CDATA[This is the collection run numbering method. (M - Manual, A - Automatic). If no value is supplied then the default value of M will be assumed. This element may only be A if the payment run key type is set up as numeric.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="NextCollectionRunNum">
								<xsd:annotation>
									<xsd:documentation><![CDATA[This is the next collection run number. Applies only if 'CollectionRunNumMethod' is A. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CustAmendJnlsReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether Customer amendment journals are required. (Y - Yes, N - No, A - Yes, including additions). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="Y=Yes;N=No;A=Yes, including additions;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="Y"/>
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="A"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="InvoiceAgeingMethod">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates invoice ageing method. (S - Statement, (A - Aged statement, I - Invoice date, D - Invoice due date). If no value is supplied then a default value of I will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="S=Statement;A=Aged statement;I=Invoice;D=Invoice due date;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="S"/>
				<xsd:enumeration value="A"/>
				<xsd:enumeration value="I"/>
				<xsd:enumeration value="D"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefFinChgTerms">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the invoice terms code to use for Finance charges. If no value is supplied then a default value of 9 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefCustBrowseToOpBr">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether default the customer browse to the operator's default branch codee displayed. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="BankDepSlipReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether a bank deposit slip is required (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ForeignCurrSalesReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether foreign currency sales are required. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PermEntriesReprtReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether permanent entry invoice print is required. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ConsolBrFwdCust">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to consolidate brought-forward customers at month end. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ValidateCustClass">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to validate the customer class on customer master. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="General">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CustAmendJnlsReq"/>
				<xsd:element ref="InvoiceAgeingMethod"/>
				<xsd:element ref="DefFinChgTerms"/>
				<xsd:element ref="DefCustBrowseToOpBr"/>
				<xsd:element ref="BankDepSlipReq"/>
				<xsd:element ref="ForeignCurrSalesReq"/>
				<xsd:element ref="PermEntriesReprtReq"/>
				<xsd:element ref="ConsolBrFwdCust"/>
				<xsd:element ref="ValidateCustClass"/>
				<xsd:element ref="AllowBranchOverwrite"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="TrmsDiscInclTax">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to include tax in the calculation of terms discount. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="TrmsDiscInclFrt">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to include freight in the calculation of terms discount. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="TrmsDiscInclOthChg">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to include other charges in the calculation of terms discount. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CalcTrmsDiscForCrDrNotes">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to calculate terms discount for credit and debit notes. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PrtCrDrNotesForDisc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to print credit/debit notes for settlement discount. (A - Always, R - As required, N - Never). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="A=Always;R=As required;N=Never;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="A"/>
				<xsd:enumeration value="R"/>
				<xsd:enumeration value="N"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AllowMultInvPerCrDrNote">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to allow multiple invoices per credit/debit settlement note. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed. (Option updated with '1' when Y, '0' when N).]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="TermsDiscount">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="TrmsDiscInclTax"/>
				<xsd:element ref="TrmsDiscInclFrt"/>
				<xsd:element ref="TrmsDiscInclOthChg"/>
				<xsd:element ref="CalcTrmsDiscForCrDrNotes"/>
				<xsd:element ref="PrtCrDrNotesForDisc"/>
				<xsd:element ref="AllowMultInvPerCrDrNote"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DefSalesTaxCode">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the default tax code to which the tax amount must be posted in the AR Invoice Posting program when no tax integration is selcted..]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ReqTaxPortionAdjDisc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to request a tax portion for invoice adjustments and discounts. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefTaxCodeArAdjDisc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the default tax codes for invoice adjustments and discounts. This element only needs to be entered if 'ReqTaxPortionAdjDisc'is selected. If entered, this element may only be in the  range of A - Z.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DisbTaxPortionGlTaxCode">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to disburse the tax portion of an adjustment to the GL code against the tax code. This element only needs to selected if 'ReqTaxPortionAdjDisc' has been selected. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="Tax">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DefSalesTaxCode"/>
				<xsd:element ref="ReqTaxPortionAdjDisc"/>
				<xsd:element ref="DefTaxCodeArAdjDisc"/>
				<xsd:element ref="DisbTaxPortionGlTaxCode"/>
				<xsd:element ref="DefSalesGSTCode"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="RetainPayDet">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to retain payment details. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PurgeCustMoveBy">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to purge customer sales details by date or number. (N - Number, D - Date). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=Number;D=Date;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="D"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AveNoDaysToPayCalc">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to include for the average number of days to pay calculation. (99 implies retain indefinitely). If this number is not supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MaxNoCustMoveRecs">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the maximum number of movement records or age of transaction to retain before deletion. This element is based on either 'Date' or 'Number' from the 'PurgeCustMoveBy' element. If this number is not supplied, 99999 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99999"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetZeroInv">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain zero balance invoices. (99 implies retain indefinitely). If no value is supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetCashJnl">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain printed cash journals. (99 implies retain indefinitely). If no value is supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetPayHist">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain payment history. (99 implies retain indefinitely). If no value is supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetSlsTrnSumm">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain summary sales transactions. (99 implies retain indefinitely). If no value is supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetSlsTrnDet">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain detail sales transactions. (99 implies retain indefinitely). If no value is supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetDiscCrNote">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain discount credit notes. (99 implies retain indefinitely). If no value is supplied, 99 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetDiscDrNote">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months to retain discount debit notes. (99 implies retain indefinitely). If no value is supplied, 99 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlPerRetSlsDistr">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of GL periods to retain AR sales distribution. This only applies if GL integration for the AR module is linked to GL in summary or detail. (99 implies retain indefinitely). If no value is supplied, 0 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="0"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlPerRetPayDistr">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of GL periods to retain AR payment distribution. This only applies if GL integration for the AR module is linked to GL in summary or detail. (99 implies retain indefinitely). If no value is supplied, 0 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="0"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="History">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="RetainPayDet"/>
				<xsd:element ref="PurgeCustMoveBy"/>
				<xsd:element ref="AveNoDaysToPayCalc"/>
				<xsd:element ref="MaxNoCustMoveRecs"/>
				<xsd:element ref="MthsRetZeroInv"/>
				<xsd:element ref="MthsRetCashJnl"/>
				<xsd:element ref="MthsRetPayHist"/>
				<xsd:element ref="MthsRetSlsTrnSumm"/>
				<xsd:element ref="MthsRetSlsTrnDet"/>
				<xsd:element ref="MthsRetDiscCrNote"/>
				<xsd:element ref="MthsRetDiscDrNote"/>
				<xsd:element ref="GlPerRetSlsDistr"/>
				<xsd:element ref="GlPerRetPayDistr"/>
				<xsd:element ref="MthsRetArPayRunNum"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="InvNumMethod">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the invoice numbering method. (C - Company, B - Branch). If no value is supplied then a default value of C will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="C=Company;B=Branch;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="B"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AllowDupInv">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to allow the same invoice number for different customers. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
