              <xsd:element name="Text_Branch" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Cost_Center" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Group" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Location" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
