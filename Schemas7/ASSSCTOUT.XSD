<!--
     Schema for the Asset Capex Types Setup Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="RecordsInvalid" type="xsd:boolean"/>
	<xsd:element name="RecordsRead" type="xsd:boolean"/>
	<xsd:complexType name="StatusOfItemsType">
		<xsd:sequence>
			<xsd:element ref="RecordsRead"/>
			<xsd:element ref="RecordsInvalid"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="itemType">
		<xsd:sequence>
			<xsd:element name="key" type="keyType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="keyType">
		<xsd:sequence>
			<xsd:element ref="capextype"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="setupassetcapextypes">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="item" type="itemType"/>
				<xsd:element name="StatusOfItems" type="StatusOfItemsType"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="capextype" type="xsd:string"/>
</xsd:schema>
