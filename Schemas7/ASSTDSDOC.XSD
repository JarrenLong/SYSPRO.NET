			<xsd:documentation>This is the top element for all data required for Asset acqusition. </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element name="Item" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Mandatory element.This is the top element for all data required for the transaction. This element can occur multiple times as the business object caters for the posting new acquisitions and additional acquisitions to assets.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType mixed="false">
						<xsd:sequence>
							<xsd:element ref="Asset"/>
							<xsd:element ref="AssetQty" minOccurs="0"/>
							<xsd:element ref="DisposalDate" minOccurs="0"/>
							<xsd:element ref="DisposalReason"/>
							<xsd:element ref="DisposalComments"/>
							<xsd:element ref="BvAssetCost"/>
							<xsd:element ref="BvSoldValue" minOccurs="0"/>
							<xsd:element ref="BvDisposalCost" minOccurs="0"/>
							<xsd:element ref="BvDepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="BvDepnThisYear" minOccurs="0"/>
							<xsd:element ref="BvAccumDepn" minOccurs="0"/>
							<xsd:element ref="BvCalcDepn" minOccurs="0"/>
							<xsd:element ref="TaxAssetCost"/>
							<xsd:element ref="TaxSoldValue" minOccurs="0"/>
							<xsd:element ref="TaxDisposalCost" minOccurs="0"/>
							<xsd:element ref="TaxDepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="TaxDepnThisYear" minOccurs="0"/>
							<xsd:element ref="TaxAccumDepn" minOccurs="0"/>
							<xsd:element ref="TaxCalcDepn" minOccurs="0"/>
							<xsd:element ref="Alt1AssetCost"/>
							<xsd:element ref="Alt1SoldValue" minOccurs="0"/>
							<xsd:element ref="Alt1DisposalCost" minOccurs="0"/>
							<xsd:element ref="Alt1DepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="Alt1DepnThisYear" minOccurs="0"/>
							<xsd:element ref="Alt1AccumDepn" minOccurs="0"/>
							<xsd:element ref="Alt1CalcDepn" minOccurs="0"/>
							<xsd:element ref="Alt2AssetCost"/>
							<xsd:element ref="Alt2SoldValue" minOccurs="0"/>
							<xsd:element ref="Alt2DisposalCost" minOccurs="0"/>
							<xsd:element ref="Alt2DepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="Alt2DepnThisYear" minOccurs="0"/>
							<xsd:element ref="Alt2AccumDepn" minOccurs="0"/>
							<xsd:element ref="Alt2CalcDepn" minOccurs="0"/>
							<xsd:element ref="Alt3AssetCost"/>
							<xsd:element ref="Alt3SoldValue" minOccurs="0"/>
							<xsd:element ref="Alt3DisposalCost" minOccurs="0"/>
							<xsd:element ref="Alt3DepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="Alt3DepnThisYear" minOccurs="0"/>
							<xsd:element ref="Alt3AccumDepn" minOccurs="0"/>
							<xsd:element ref="Alt3CalcDepn" minOccurs="0"/>
							<xsd:element ref="Alt4AssetCost"/>
							<xsd:element ref="Alt4SoldValue" minOccurs="0"/>
							<xsd:element ref="Alt4DisposalCost" minOccurs="0"/>
							<xsd:element ref="Alt4DepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="Alt4DepnThisYear" minOccurs="0"/>
							<xsd:element ref="Alt4AccumDepn" minOccurs="0"/>
							<xsd:element ref="Alt4CalcDepn" minOccurs="0"/>
							<xsd:element ref="Alt5AssetCost"/>
							<xsd:element ref="Alt5SoldValue" minOccurs="0"/>
							<xsd:element ref="Alt5DisposalCost" minOccurs="0"/>
							<xsd:element ref="Alt5DepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="Alt5DepnThisYear" minOccurs="0"/>
							<xsd:element ref="Alt5AccumDepn" minOccurs="0"/>
							<xsd:element ref="Alt5CalcDepn" minOccurs="0"/>
							<xsd:element ref="Alt6AssetCost"/>
							<xsd:element ref="Alt6SoldValue" minOccurs="0"/>
							<xsd:element ref="Alt6DisposalCost" minOccurs="0"/>
							<xsd:element ref="Alt6DepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="Alt6DepnThisYear" minOccurs="0"/>
							<xsd:element ref="Alt6AccumDepn" minOccurs="0"/>
							<xsd:element ref="Alt6CalcDepn" minOccurs="0"/>
							<xsd:element ref="Alt7AssetCost"/>
							<xsd:element ref="Alt7SoldValue" minOccurs="0"/>
							<xsd:element ref="Alt7DisposalCost" minOccurs="0"/>
							<xsd:element ref="Alt7DepnThisPeriod" minOccurs="0"/>
							<xsd:element ref="Alt7DepnThisYear" minOccurs="0"/>
							<xsd:element ref="Alt7AccumDepn" minOccurs="0"/>
							<xsd:element ref="Alt7CalcDepn" minOccurs="0"/>
							<xsd:element ref="Alt8AssetCost"/>
							<xsd:element ref="Alt8SoldValue" minOccurs="0"/>
							<xsd:element ref="Alt8DisposalCost" minOccurs="0"/>
