								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:whiteSpace value="preserve"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="Alt5AssetCost" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>This is the new revalued Alternate 5 value of the asset. The revaluation transaction amount will be calculated as the difference between old asset value and revalued asset value. </xsd:documentation>
