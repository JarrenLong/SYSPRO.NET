<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--      Schema for the Work Center Move Matrix Maintenance Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="Item">		<xsd:annotation>			<xsd:documentation>This element contains information to maintain a Work Center Move Matrix. NOTE: The information held within the key element is mandatory for all functions. Information held outside the key element is optional for Add/Update and is not required for Delete.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="MovementTime"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Key">		<xsd:annotation>			<xsd:documentation>This element contains the key information required to maintain a cost Center.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="ThisWorkCentre"/>				<xsd:element ref="NextWorkCentre"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="MovementTime" type="xsd:byte">		<xsd:annotation>			<xsd:documentation>The Movement time is the Movement time (in days)between the work centers.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="NextWorkCentre" type="xsd:string">		<xsd:annotation>			<xsd:documentation>Next work center specifies the Work Center that recieves</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="SetupBomWorkCenterMoveMatrix">		<xsd:annotation>			<xsd:documentation>The root element.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="ThisWorkCentre" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This work center specifies the Work Center that does the move. </xsd:documentation>		</xsd:annotation>	</xsd:element></xsd:schema>