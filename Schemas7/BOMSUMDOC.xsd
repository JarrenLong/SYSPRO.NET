<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2016 SYSPRO Ltd.--><!--      Schema for the BOM Employee Time Unit of Measure Maintenance Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="ConvFactor" type="xsd:decimal">		<xsd:annotation>			<xsd:documentation>This element contains information about the conversion factor to use to convert the employee rate to the work center rate (M - Multiply and D - Divide ).It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="EmpRateTimeUom" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the time unit of measure applicable to the employee rate. It is part of the key. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Item">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="ConvFactor"/>				<xsd:element ref="MulDiv"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Key">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="EmpRateTimeUom"/>				<xsd:element ref="WorkTimeUom"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="MulDiv" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the rate that must used to convert the employee rate time unit of measure into the work center time unit of measure. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="SetupBOMEmployeeTimeUnitofMeasure">		<xsd:annotation>			<xsd:documentation>This is the root node, it is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="WorkTimeUom" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the time unit of measure applicable to the work center. It is part of the key. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element></xsd:schema>