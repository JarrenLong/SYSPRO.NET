<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2016 SYSPRO Ltd.--><!--      Schema for the Detailed Operator List Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="Filter">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Group" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Group">		<xsd:annotation>			<xsd:documentation>The group code ADMGRP-CODE</xsd:documentation>			<xsd:appinfo source="screenItem=Y;browse=AdmOperGroup;A=All;L=List;R=Range;S=Single"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="required">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="L"/>						<xsd:enumeration value="R"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="required"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="Option">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="ShowBrowseOnly" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Query">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Option" minOccurs="0"/>				<xsd:element ref="Filter" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="ShowBrowseOnly">		<xsd:annotation>			<xsd:documentation>Flag to select whether or not to show the programs with Browse only access. The default value is "N" and the other valid value is "Y"</xsd:documentation>			<xsd:appinfo source="screenItem=Y;N=No;Y=Yes"/>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="N"/>				<xsd:enumeration value="Y"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element></xsd:schema>