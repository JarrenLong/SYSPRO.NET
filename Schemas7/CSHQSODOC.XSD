<!--
     Schema for the Query of the Cash Book Setup Options business object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="CashBookOptions">
    <xsd:annotation>
      <xsd:documentation>This is the root element.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="false">
      <xsd:sequence>
        <xsd:element ref="General" minOccurs="0"/>
        <xsd:element ref="History" minOccurs="0"/>
        <xsd:element ref="Tax" minOccurs="0"/>
        <xsd:element ref="PeriodCurrentYear" minOccurs="0"/>
        <xsd:element ref="PeriodPreviousYear" minOccurs="0"/>
        <xsd:element ref="CurrentMonth" minOccurs="0"/>
        <xsd:element ref="Information" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ForeignCurrCashBookReq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Foreign cash book currency required]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PermEntReqCb">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Permanent entries required]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AskNotPerDetDistrEntry">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Ask notation per detail distribution entry]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DispBankBalInRecapScreen">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Display bank balance in deposits recap screen]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AutoBankReconOfDeposits">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Automatic bank reconciliation of deposits by]]></xsd:documentation>
      <xsd:appinfo source="N=Value only;Y=Value, date and reference;D=Value and date"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="D"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="General">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ForeignCurrCashBookReq"/>
        <xsd:element ref="PermEntReqCb"/>
        <xsd:element ref="AskNotPerDetDistrEntry"/>
        <xsd:element ref="DispBankBalInRecapScreen"/>
        <xsd:element ref="AutoBankReconOfDeposits"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MthsRetCbJnls">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Number of days to retain cash book journals]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MthsRetReconTrans">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Number of days to retain reconciled transactions]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="History">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="MthsRetCbJnls"/>
        <xsd:element ref="MthsRetReconTrans"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TaxDistrMethod">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Tax distribution method]]></xsd:documentation>
      <xsd:appinfo source="N=Not required;D=Detail;S=Summary;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="D"/>
        <xsd:enumeration value="S"/>
