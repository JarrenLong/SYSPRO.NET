    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="TaxDistrReqFor">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Tax distribution required for]]></xsd:documentation>
      <xsd:appinfo source="B=Both deposits and withdrawals;D=Deposits;W=Withdrawals;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="B"/>
        <xsd:enumeration value="D"/>
        <xsd:enumeration value="W"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DefTaxCodeCshDep">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Default tax codes for deposits(Must be in the range of A-Z)]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DefTaxCodeCshWith">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Default tax codes for withdrawals]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Tax">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TaxDistrMethod"/>
        <xsd:element ref="TaxDistrReqFor"/>
        <xsd:element ref="DefTaxCodeCshDep"/>
        <xsd:element ref="DefTaxCodeCshWith"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
