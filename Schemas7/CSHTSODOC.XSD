<!-- 
     Schema for the Cash Book Setup Transaction Posting Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="CashBookOptions">
		<xsd:annotation>
			<xsd:documentation>This is the root element.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="General" minOccurs="0"/>
				<xsd:element ref="History" minOccurs="0"/>
				<xsd:element ref="Tax" minOccurs="0"/>
				<xsd:element ref="PeriodCurrentYear" minOccurs="0"/>
				<xsd:element ref="PeriodPreviousYear" minOccurs="0"/>
				<xsd:element ref="CurrentMonth" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ForeignCurrCashBookReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether Foreign cash book currency is required. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PermEntReqCb">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether Permanent entries are required. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AskNotPerDetDistrEntry">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Ask if additional references are required for individual ledger distributions in Cash Book per entry. (Y - Yes, N - No). If no value is supplied then a default value of Y will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="Y=Yes;N=No"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="Y"/>
				<xsd:enumeration value="N"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DispBankBalInRecapScreen">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether the bank balances in the deposits recap screen (CSHP10) should be displayed. (Y - Yes, N - No). If no value is supplied then a default value of Y will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="Y=Yes;N=No;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="Y"/>
				<xsd:enumeration value="N"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AutoBankReconOfDeposits">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether the automatic bank reconciliation of deposits should include the matching of the date and/or reference. (V - Value only, R - Value date and reference, D - Value and date). If no value is supplied then a default value of V will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="V=Value only;R=Value, date and reference;D=Value and date"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="V"/>
				<xsd:enumeration value="R"/>
				<xsd:enumeration value="D"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="General">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ForeignCurrCashBookReq"/>
				<xsd:element ref="PermEntReqCb"/>
				<xsd:element ref="AskNotPerDetDistrEntry"/>
				<xsd:element ref="DispBankBalInRecapScreen"/>
				<xsd:element ref="AutoBankReconOfDeposits"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MthsRetCbJnls">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months the cash book journals must be retained. If this number is not supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetReconTrans">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months the cash book reconciled transaction must be retained. If this number is not supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MthsRetTaxDistrCb">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the number of months the cash book tax distribution details must be retained. The format is 2 characters, numeric. (99 implies retain indefinitely). If no value is supplied, 99 will be assumed. This element may only be displayed/changed when you enable the option EC VAT system required.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="History">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MthsRetCbJnls"/>
				<xsd:element ref="MthsRetReconTrans"/>
				<xsd:element ref="MthsRetTaxDistrCb"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="TaxDistrMethod">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the output Tax distribution method. (N - Not required, D - Detail, S - Summary). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=Not required;D=Detail;S=Summary;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="D"/>
				<xsd:enumeration value="S"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="TaxDistrReqFor">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates which transactions the Tax distribution is required for. (B - Both deposits and withdrawals, D - Deposits, W - Withdrawals). If no value is supplied then a default value of B will be assumed. This element is not required when 'TaxDistrMethod' is set to N.]]></xsd:documentation>
			<xsd:appinfo source="B=Both deposits and withdrawals;D=Deposits;W=Withdrawals;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="B"/>
				<xsd:enumeration value="D"/>
				<xsd:enumeration value="W"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefTaxCodeCshDep">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the default tax codes for deposit transactions. This element only needs to be entered if 'TaxDistrReqFor' contains either B or D. This element is not validated if 'TaxDistrMethod' is set to N.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DefTaxCodeCshWith">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the default tax codes for withdrawal transactions. This element only needs to be entered if 'TaxDistrReqFor' contains either B or W. This element is not validated if 'TaxDistrMethod' is set to N.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CalcNotEquEnterTax">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates whether to display a warning or an error message if the calculated and entered tax amounts differ. (W - Warning, E - Error).  If no value is supplied then a default value of W will be  assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="Tax">
		<xsd:annotation>
			<xsd:documentation>When using Descriptive Tax Codes, a descriptive tax code must be provided.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="TaxDistrMethod"/>
				<xsd:element ref="TaxDistrReqFor"/>
				<xsd:element ref="DefTaxCodeCshDep"/>
				<xsd:element ref="DefTaxCodeCshWith"/>
				<xsd:element ref="CalcNotEquEnterTax"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CbCurrDatePeriod1">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 1 month end date. This element is optional. The format is CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CbCurrFlagPeriod1">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 1 status flag. (C - Closed, O - Open). If no value is supplied then the default value of C will be assumed. If the 'CbCurrentMonth' is modified this element will be reset to C unless this is the current period, then it will be set to O. Only the current period and the two previous periods may be set to O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CbCurrDatePeriod2">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 2 month end date. This element is optional. The format is CCYY-MM-DD. This date supplied may not precede 'CbCurrDatePeriod1'.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CbCurrFlagPeriod2">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 2 status flag. (C - Closed, O - Open). If no value is supplied then the default value of C will be assumed. If the 'CbCurrentMonth' is modified this element will be reset to C unless this is the current period, then it will be set to O. Only the current period and the previous period may be set to O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="CbCurrDatePeriod3">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 3 month end date. This element is optional. The format is CCYY-MM-DD. This date supplied may not precede 'CbCurrDatePeriod2'.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
