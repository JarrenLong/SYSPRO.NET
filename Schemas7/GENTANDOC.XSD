<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--     Schema for GL Analysis Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="PostGlAnalysis">    <xsd:complexType>      <xsd:sequence>        <xsd:element name="Item">          <xsd:complexType>            <xsd:sequence>              <xsd:element name="LedgerCode">                <xsd:annotation>                  <xsd:documentation>This is the ledger code that you are entering analysis for. A ledger code must be provided. </xsd:documentation>                </xsd:annotation>                <xsd:simpleType>                  <xsd:restriction base="xsd:string">                    <xsd:whiteSpace value="preserve"/>                  </xsd:restriction>                </xsd:simpleType>              </xsd:element>              <xsd:element name="AnalysisNumber" minOccurs="0">                <xsd:annotation>                  <xsd:documentation>If you are editing an analysis entry then you must supply the analysis entry number for the entry you wish to maintain.</xsd:documentation>                </xsd:annotation>                <xsd:simpleType>                  <xsd:restriction base="xsd:string">                    <xsd:whiteSpace value="preserve"/>                  </xsd:restriction>                </xsd:simpleType>              </xsd:element>              <xsd:element name="GlYear" minOccurs="0">                <xsd:annotation>                  <xsd:documentation>Enter the GL year for the GL analysis. If no GL year is provided then the current GL year will be used. </xsd:documentation>                </xsd:annotation>                <xsd:simpleType>                  <xsd:restriction base="xsd:string">                    <xsd:whiteSpace value="preserve"/>                  </xsd:restriction>                </xsd:simpleType>              </xsd:element>              <xsd:element name="GlPeriod" minOccurs="0">                <xsd:annotation>                  <xsd:documentation>Enter the GL period for the GL analysis. If no GL period is provided then the current GL period will be used. </xsd:documentation>                </xsd:annotation>                <xsd:simpleType>                  <xsd:restriction base="xsd:string">                    <xsd:whiteSpace value="preserve"/>                  </xsd:restriction>                </xsd:simpleType>              </xsd:element>              <xsd:element name="Amount">                <xsd:annotation>                  <xsd:documentation>This is the total amount for the GL entry. You must supply an amount for the GL transactions to balance back to.</xsd:documentation>                </xsd:annotation>                <xsd:simpleType>                  <xsd:restriction base="xsd:string">                    <xsd:whiteSpace value="preserve"/>                  </xsd:restriction>                </xsd:simpleType>              </xsd:element>              <xsd:element name="AnalysisEntry">                <xsd:complexType>                  <xsd:sequence>                    <xsd:element name="AnalysisCode1" minOccurs="0">                      <xsd:annotation>                        <xsd:documentation>This is if you have an analysis code type 1 for the the GL category. </xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                    <xsd:element name="AnalysisCode2" minOccurs="0">                      <xsd:annotation>                        <xsd:documentation>This is if you have an analysis code type 2 for the the GL category. </xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                    <xsd:element name="AnalysisCode3" minOccurs="0">                      <xsd:annotation>                        <xsd:documentation>This is if you have an analysis code type 3 for the the GL category. </xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                    <xsd:element name="AnalysisCode4" minOccurs="0">                      <xsd:annotation>                        <xsd:documentation>This is if you have an analysis code type 4 for the the GL category. </xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                    <xsd:element name="AnalysisCode5" minOccurs="0">                      <xsd:annotation>                        <xsd:documentation>This is if you have an analysis code type 5 for the the GL category. </xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                    <xsd:element name="StartDate" minOccurs="0">                      <xsd:annotation>                        <xsd:documentation>You can specify a date range for the analysis entry. You cannot enter a start date without specifying an end date. The end date may not be prior to the start date.</xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                    <xsd:element name="EndDate" minOccurs="0">                      <xsd:annotation>                        <xsd:documentation>You can specify a date range for the analysis entry. You cannot enter an end date without specifying a start date. The end date may not be prior to the start date.</xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                    <xsd:element name="EntryAmount">                      <xsd:annotation>                        <xsd:documentation>Every entry line must have an entry amount. The entry amounts for all the analysis entries must add up to the 'Amount' supplied. </xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                    <xsd:element name="Comment" minOccurs="0">                      <xsd:annotation>                        <xsd:documentation>You can optionally supply a comment for the analysis entry </xsd:documentation>                      </xsd:annotation>                      <xsd:simpleType>                        <xsd:restriction base="xsd:string">                          <xsd:whiteSpace value="preserve"/>                        </xsd:restriction>                      </xsd:simpleType>                    </xsd:element>                  </xsd:sequence>                </xsd:complexType>              </xsd:element>            </xsd:sequence>          </xsd:complexType>        </xsd:element>      </xsd:sequence>    </xsd:complexType>  </xsd:element></xsd:schema>