<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--     Schema for GL Validate Ledger Code Business Object--><xsd:schema elementFormDefault="qualified"  xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="postgljournal">    <xsd:complexType>      <xsd:sequence>        <xsd:element name="Item">          <xsd:complexType>            <xsd:sequence>              <xsd:element name="Transaction">                <xsd:complexType>                  <xsd:sequence>                    <xsd:element name="Key">                      <xsd:complexType>                        <xsd:sequence>                          <xsd:element name="GlYear" type="xsd:short"/>                          <xsd:element name="GlPeriod" type="xsd:byte"/>                          <xsd:element name="GlJournal" type="xsd:byte"/>                        </xsd:sequence>                      </xsd:complexType>                    </xsd:element>                    <xsd:element name="JournalDate" type="xsd:int"/>                    <xsd:element name="NumOfEntries" type="xsd:byte"/>                    <xsd:element name="DebitAmount" type="xsd:decimal"/>                    <xsd:element name="CreditAmount" type="xsd:decimal"/>                    <xsd:element name="JnlPostingType" type="xsd:string"/>                    <xsd:element name="Jnlstatus" type="xsd:string"/>                    <xsd:element name="Currency">                      <xsd:complexType/>                    </xsd:element>                    <xsd:element name="Authorised" type="xsd:string"/>                  </xsd:sequence>                </xsd:complexType>              </xsd:element>            </xsd:sequence>          </xsd:complexType>        </xsd:element>        <xsd:element name="StatusOfItems">          <xsd:complexType>            <xsd:sequence>              <xsd:element name="JournalsProcessed" type="xsd:boolean"/>              <xsd:element name="PostingsProcessed" type="xsd:boolean"/>              <xsd:element name="PostingsInvalid">                <xsd:complexType/>              </xsd:element>            </xsd:sequence>          </xsd:complexType>        </xsd:element>      </xsd:sequence>    </xsd:complexType>  </xsd:element></xsd:schema>