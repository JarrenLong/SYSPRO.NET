		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="GlCurrentYear"/>
				<xsd:element ref="GlCurrentMonth"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CommitmentsOffClearData">
