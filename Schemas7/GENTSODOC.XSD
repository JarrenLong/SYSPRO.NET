<?xml version="1.0" encoding="Windows-1252"?>
<!-- Copyright 1994-2014 SYSPRO Ltd.-->
<!--
     Schema for the GL Setup Transaction Posting Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="GeneralLedgerOptions">
		<xsd:annotation>
			<xsd:documentation>This is the root element.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="General" minOccurs="0"/>
				<xsd:element ref="Budget" minOccurs="0"/>
				<xsd:element ref="History" minOccurs="0"/>
				<xsd:element ref="Periods" minOccurs="0"/>
				<xsd:element ref="CurrentPeriod" minOccurs="0"/>
				<xsd:element ref="Commitments" minOccurs="0"/>
				<xsd:element ref="Journals" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DefaultBrowseSequence">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Indicates the default browse sequence. (L - Ledger Code Sequence, D - Ledger Description Sequence). If no value is supplied then a default value of L will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="L=Ledger;D=Description;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="L"/>
				<xsd:enumeration value="D"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="RevOutProvJnls">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you require provisional journals processed using the GL Journal Entry program to be reversed out automatically at period and/or year end. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlobalTaxFileReq">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you required a single tax report for sales transactions processed in the Accounts Receivable and Sales Order modules and for transactions posted in the Accounts Payable and Cash Book modules. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ShowAccBalInGlCodeBrowse">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you would like to restrict the viewing of account balances within the Ledger Code Maintenance listview. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PwdReqForIndivGlAccs">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you would like to prevent unauthorized postings being made to individual ledger accounts. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="IntegFromSlAtGlCodeLevel">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you would like to define different levels of integration for specific ledger account codes. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AnalysisEntriesAssets">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you want to include asset accounts for analysis entries. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AnalysisEntriesLiabilities">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you want to include liability accounts for analysis entries. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AnalysisEntriesCapital">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you want to include capital accounts for analysis entries. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AnalysisEntriesRevenue">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you want to include revenue accounts for analysis entries. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="AnalysisEntriesExpense">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you want to include expense accounts for analysis entries. (N - No, Y - Yes). If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="General">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DefaultBrowseSequence"/>
				<xsd:element ref="RevOutProvJnls"/>
				<xsd:element ref="GlobalTaxFileReq"/>
				<xsd:element ref="ShowAccBalInGlCodeBrowse"/>
				<xsd:element ref="PwdReqForIndivGlAccs"/>
				<xsd:element ref="IntegFromSlAtGlCodeLevel"/>
				<xsd:element ref="AnalysisEntriesAssets"/>
				<xsd:element ref="AnalysisEntriesLiabilities"/>
				<xsd:element ref="AnalysisEntriesCapital"/>
				<xsd:element ref="AnalysisEntriesRevenue"/>
				<xsd:element ref="AnalysisEntriesExpense"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="BudgetProcAtYearEnd">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates whether to leave budget information intact after the Year end function of the GL Period End program. (L - Retain current year budgets, Z - Zeroise current year budgets, C - Copy next year budgets to current year, O - Copy with zero budgets). If no value is supplied then a default value of L will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="L=Retain current year budgets;Z=Zeroise current year budgets;C=Copy next year budgets to current year;O=copy with zero budgets;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="L"/>
				<xsd:enumeration value="Z"/>
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="VariableBudgetSpread">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates the level at which you require variable budget amounts to be spread across each period of the year. (C - by Company, G - by Group).  If no value is supplied then a default value of C will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="C=by Company;G=by Group;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="G"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ZeroiseGrpBudSpreads">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates when selecting to define variable budget spread by company if all group budget spread percentages should be set to zero. (Y - Yes, N -No).  If no value is supplied then a default value of N will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="N=No;Y=Yes;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="BudgetReqYePeriod">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates if you require an additional budget period for the year end period. (Y - Yes, N - No).  If no value is supplied then a default value of Y will be assumed.]]></xsd:documentation>
			<xsd:appinfo source="Y=Yes;N=No;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="Y"/>
				<xsd:enumeration value="N"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DescAltBud1">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Description for first alternate budget.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DescAltBud2">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Description for second alternate budget.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DescAltBud3">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Description for third alternate budget.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DescAltBud4">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Description for fourth alternate budget.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DescAltBud5">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Description for fifth alternate budget.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DescAltBud6">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Description for sixth alternate budget.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DescAltBud7">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Description for seventh alternate budget.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="DescAltBud8">
		<xsd:annotation>
			<xsd:documentation><![CDATA[Description for eighth alternate budget.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="Budget">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="BudgetProcAtYearEnd"/>
				<xsd:element ref="VariableBudgetSpread"/>
				<xsd:element ref="ZeroiseGrpBudSpreads"/>
				<xsd:element ref="BudgetReqYePeriod"/>
				<xsd:element ref="DescAltBud1"/>
				<xsd:element ref="DescAltBud2"/>
				<xsd:element ref="DescAltBud3"/>
				<xsd:element ref="DescAltBud4"/>
				<xsd:element ref="DescAltBud5"/>
				<xsd:element ref="DescAltBud6"/>
				<xsd:element ref="DescAltBud7"/>
				<xsd:element ref="DescAltBud8"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="YrsRetSummHistOfBals">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates the number of years to retain summary history of balances. This number may be from 1 to 99 inclusive. If this number is not supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="YrsRetDetTranHist">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates the number of years to retain detail transaction history. This number may be from 1 to 99 inclusive. If this number is not supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlPerRetPostedJnls">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates the number of periods to retain posted journals. This number may be from 1 to 99 inclusive. If this number is not supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlPerRetExpRecJnls">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates the number of periods to retain expired recurring journals. This number may be from 1 to 99 inclusive. If this number is not supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlPeRetPostedGrpEnt">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This indicates the number of periods to retain posted group entries. This number may be from 1 to 99 inclusive. If this number is not supplied, 1 will be assumed.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:double">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="99"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="History">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="YrsRetSummHistOfBals"/>
				<xsd:element ref="YrsRetDetTranHist"/>
				<xsd:element ref="GlPerRetPostedJnls"/>
				<xsd:element ref="GlPerRetExpRecJnls"/>
				<xsd:element ref="GlPeRetPostedGrpEnt"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GlCurrDatePeriod1">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the end date of the current period 1. This field is optional, but if supplied, must be in the format CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrFlagPeriod1">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 1 status flag. (C - Closed, O - Open). This field is optional. By default, only the current month will have a status of O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrDatePeriod2">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the end date of the current period 2.This field is optional, but if supplied, must be in the format CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrFlagPeriod2">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 2 status flag. (C - Closed, O - Open). This field is optional. By default, only the current month will have a status of O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrDatePeriod3">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the end date of the current period 3. This field is optional, but if supplied, must be in the format CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrFlagPeriod3">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 3 status flag. (C - Closed, O - Open). This field is optional. By default, only the current month will have a status of O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrDatePeriod4">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the end date of the current period 4.This field is optional, but if supplied, must be in the format CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrFlagPeriod4">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 4 status flag. (C - Closed, O - Open). This field is optional. By default, only the current month will have a status of O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrDatePeriod5">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the end date of the current period 5.This field is optional, but if supplied, must be in the format CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrFlagPeriod5">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 5 status flag. (C - Closed, O - Open). This field is optional. By default, only the current month will have a status of O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrDatePeriod6">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the end date of the current period 6.This field is optional, but if supplied, must be in the format CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrFlagPeriod6">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 6 status flag. (C - Closed, O - Open). This field is optional. By default, only the current month will have a status of O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrDatePeriod7">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the end date of the current period 7.This field is optional, but if supplied, must be in the format CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrFlagPeriod7">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the current period 7 status flag. (C - Closed, O - Open). This field is optional. By default, only the current month will have a status of O.]]></xsd:documentation>
			<xsd:appinfo source="C=Closed;O=Open;"/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="O"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="GlCurrDatePeriod8">
		<xsd:annotation>
			<xsd:documentation><![CDATA[This is the end date of the current period 8.This field is optional, but if supplied, must be in the format CCYY-MM-DD.]]></xsd:documentation>
			<xsd:appinfo/>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string"/>
		</xsd:simpleType>
	</xsd:element>
