							<xsd:element name="DescForUserDefField1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DescForUserDefField2" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AutoVoucherNumMethod" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NextAutoVoucherNum" type="xsd:string" minOccurs="0"/>
