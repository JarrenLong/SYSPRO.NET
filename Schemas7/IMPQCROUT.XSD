							<xsd:element name="DefaultSecondTierCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RequestSecondTierAmount" type="xsd:string" minOccurs="0"/>
							<xsd:element name="HstCheckIssuedRecords" type="xsd:string" minOccurs="0"/>
							<xsd:element name="HstZeroBalanceInvoices" type="xsd:string" minOccurs="0"/>
							<xsd:element name="HstExpenseJournals" type="xsd:string" minOccurs="0"/>
							<xsd:element name="HstPaymentCycleDetails" type="xsd:string" minOccurs="0"/>
							<xsd:element name="HstTaxDistributionDetails" type="xsd:string" minOccurs="0"/>
							<xsd:element name="HstExpensesDistEntries" type="xsd:string" minOccurs="0"/>
