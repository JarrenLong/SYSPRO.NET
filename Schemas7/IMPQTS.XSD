<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--      Schema for the Tax Return Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="EndDate" type="xsd:date">		<xsd:annotation>			<xsd:documentation>Supply the last day of the tax return period.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Filter">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Period"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="FinancialYear">		<xsd:annotation>			<xsd:documentation>Indicate the financial year for the tax return. If no year is supplied the current year will be assumed. </xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:maxLength value="4"/>				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="IncludeMasterData">		<xsd:annotation>			<xsd:documentation>Indicate whether you would like to see Supplier, Customer, Gl and Tax master data. Valid options are N-No and Y-Yes. If no value is supplied a default value of N will be assumed. </xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:maxLength value="1"/>				<xsd:whiteSpace value="preserve"/>				<xsd:enumeration value="N"/>				<xsd:enumeration value="Y"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="Option">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="FinancialYear" minOccurs="0"/>				<xsd:element ref="IncludeMasterData" minOccurs="0"/>				<xsd:element ref="StartDate" minOccurs="0"/>				<xsd:element ref="EndDate" minOccurs="0"/>				<xsd:element ref="Version" minOccurs="0"/>				<xsd:element ref="XslStylesheet" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Period">		<xsd:complexType>			<xsd:attribute name="FilterType" use="required">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="R"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="required"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="Query">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Option" minOccurs="0"/>				<xsd:element ref="Filter" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="StartDate" type="xsd:date">		<xsd:annotation>			<xsd:documentation>Supply the first day of the tax return period.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Version">		<xsd:annotation>			<xsd:documentation>Indicate the version of the tax return. If no value is supplied a default value of zero will be assumed.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:maxLength value="5"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="XslStylesheet">		<xsd:complexType/>	</xsd:element></xsd:schema>