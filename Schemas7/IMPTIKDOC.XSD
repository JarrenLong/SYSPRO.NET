  <xsd:element name="NextDiscDebitNote">
    <xsd:annotation>
      <xsd:documentation>This is the next discount debit note number to use. This only applies if numbering method is set to C - By company. The maximum size is dependent on the presentation length. If no value is supplied 1 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowSameNumForDiffCust">
    <xsd:annotation>
      <xsd:documentation>Indicates whether to allow the same invoice number for different customers. (N - No, Y - Yes). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="1"/>
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ArInvoices">
