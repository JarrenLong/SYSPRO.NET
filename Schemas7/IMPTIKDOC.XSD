  <xsd:element name="RelKeyType">
    <xsd:annotation>
      <xsd:documentation>This is the ECC release key type (A - Alphanumeric, N - Numeric). If no value is supplied then A is assumed.</xsd:documentation>
      <xsd:appinfo source="A=Alphanumeric;N=Numeric;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RelMethod">
    <xsd:annotation>
      <xsd:documentation>This is the ECC release numbering method. (M - Manual, A - Automatic). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Manual;A=Automatic;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="EccRelease">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="RelKeyType" minOccurs="0"/>
