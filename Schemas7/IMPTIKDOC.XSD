  </xsd:element>
  <xsd:element name="GtrCase">
    <xsd:annotation>
      <xsd:documentation>This is the case allowed for goods in transit reference if the key type is A - Alphanumeric (M - Mixed, U - Upper, L - Lower). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Mixed;U=Upper;L=Lower;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="U"/>
        <xsd:enumeration value="L"/>
