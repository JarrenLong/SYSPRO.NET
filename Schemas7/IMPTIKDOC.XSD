<?xml version="1.0" encoding="Windows-1252"?>
<!-- Copyright 1994-2015 SYSPRO Ltd.-->
<!-- 
     Schema for the Key Information Setup Business Object
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="SetKeyInformation">
    <xsd:annotation>
      <xsd:documentation>This is the root element.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="false">
      <xsd:sequence>
        <xsd:element ref="Customers" minOccurs="0"/>
        <xsd:element ref="Suppliers" minOccurs="0"/>
        <xsd:element ref="StockCodes" minOccurs="0"/>
        <xsd:element ref="Serials" minOccurs="0"/>
        <xsd:element ref="Lots" minOccurs="0"/>
        <xsd:element ref="BuyingGroups" minOccurs="0"/>
        <xsd:element ref="Assets" minOccurs="0"/>
        <xsd:element ref="CapexNumbers" minOccurs="0"/>
        <xsd:element ref="EccEcoNumbers" minOccurs="0"/>
        <xsd:element ref="EccRevision" minOccurs="0"/>
        <xsd:element ref="EccRelease" minOccurs="0"/>
        <xsd:element ref="ArInvoices" minOccurs="0"/>
        <xsd:element ref="ApPaymentRun" minOccurs="0"/>
        <xsd:element ref="GoodsInTransit" minOccurs="0"/>
        <xsd:element ref="SalesOrders" minOccurs="0"/>
        <xsd:element ref="DeliveryNotes" minOccurs="0"/>
        <xsd:element ref="DispatchNotes" minOccurs="0"/>
        <xsd:element ref="RmaNumbers" minOccurs="0"/>
        <xsd:element ref="PurchaseOrders" minOccurs="0"/>
        <xsd:element ref="GrnNumbers" minOccurs="0"/>
        <xsd:element ref="Requisitions" minOccurs="0"/>
        <xsd:element ref="Quotations" minOccurs="0"/>
        <xsd:element ref="Jobs" minOccurs="0"/>
        <xsd:element ref="WipInspection" minOccurs="0"/>
        <xsd:element ref="ArPaymentRun" minOccurs="0"/>
        <xsd:element ref="ArCollectionRun" minOccurs="0"/>
        <xsd:element ref="AutoVouchering" minOccurs="0"/>
        <xsd:element ref="TradePromotions" minOccurs="0"/>
        <xsd:element ref="InventoryDocs" minOccurs="0"/>
        <xsd:element ref="WipDocs" minOccurs="0"/>
        <xsd:element ref="LoadPlanning" minOccurs="0"/>
        <xsd:element ref="EdiTransmission" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CusKeyType">
    <xsd:annotation>
      <xsd:documentation>This is the customer key type (A - Alphanumeric, N - Numeric). If no value is supplied then A is assumed.</xsd:documentation>
      <xsd:appinfo source="A=Alphanumeric;N=Numeric;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CusCase">
    <xsd:annotation>
      <xsd:documentation>This is the case allowed for customers if the key type is A - Alphanumeric (M - Mixed, U - Upper, L - Lower). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Mixed;U=Upper;L=Lower;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="U"/>
        <xsd:enumeration value="L"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CusZero">
    <xsd:annotation>
      <xsd:documentation>This indicates whether to suppress leading zeros if the numbering method is numeric. (N - No, Y - Yes). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CusPresentationLength">
    <xsd:annotation>
      <xsd:documentation>This is the maximum presentation length allowed for customers. If no value is supplied then a length of 7 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="15"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CusMethod">
    <xsd:annotation>
      <xsd:documentation>This is the customer numbering method. (M - Manual, A - Automatic, S - Scripted). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Manual;A=Automatic;S=Scripted;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="S"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextCustomer">
    <xsd:annotation>
      <xsd:documentation>This is the next customer number to use. This element only applies if the numbering method is A - Automatic. The maximum size is dependent on the presentation length. If no value is supplied 1 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Customers">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CusKeyType" minOccurs="0"/>
        <xsd:element ref="CusCase" minOccurs="0"/>
        <xsd:element ref="CusZero" minOccurs="0"/>
        <xsd:element ref="CusPresentationLength" minOccurs="0"/>
        <xsd:element ref="CusMethod" minOccurs="0"/>
        <xsd:element ref="NextCustomer" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SupKeyType">
    <xsd:annotation>
      <xsd:documentation>This is the supplier key type (A - Alphanumeric, N - Numeric). If no value is supplied then A is assumed.</xsd:documentation>
      <xsd:appinfo source="A=Alphanumeric;N=Numeric;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SupCase">
    <xsd:annotation>
      <xsd:documentation>This is the case allowed for suppliers if the key type is A - Alphanumeric (M - Mixed, U - Upper, L - Lower). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Mixed;U=Upper;L=Lower;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="U"/>
        <xsd:enumeration value="L"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SupZero">
    <xsd:annotation>
      <xsd:documentation>This indicates whether to suppress leading zeros if the numbering method is numeric. (N - No, Y - Yes). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SupPresentationLength">
    <xsd:annotation>
      <xsd:documentation>This is the maximum presentation length allowed for suppliers. If no value is supplied then a length of 7 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="15"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SupMethod">
    <xsd:annotation>
      <xsd:documentation>This is the supplier numbering method. (M - Manual, A - Automatic, S - Scripted). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Manual;A=Automatic;S=Scripted;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="S"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextSupplier">
    <xsd:annotation>
      <xsd:documentation>This is the next supplier number to use. This element only applies if the numbering method is A - Automatic. The maximum size is dependent on the presentation length. If no value is supplied 1 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Suppliers">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="SupKeyType" minOccurs="0"/>
        <xsd:element ref="SupCase" minOccurs="0"/>
        <xsd:element ref="SupZero" minOccurs="0"/>
        <xsd:element ref="SupPresentationLength" minOccurs="0"/>
        <xsd:element ref="SupMethod" minOccurs="0"/>
        <xsd:element ref="NextSupplier" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="StkKeyType">
    <xsd:annotation>
      <xsd:documentation>This is the stock code key type (A - Alphanumeric, N - Numeric). If no value is supplied then A is assumed.</xsd:documentation>
      <xsd:appinfo source="A=Alphanumeric;N=Numeric;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StkCase">
    <xsd:annotation>
      <xsd:documentation>This is the case allowed for stock codes if the key type is A - Alphanumeric (M - Mixed, U - Upper, L - Lower). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Mixed;U=Upper;L=Lower;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="U"/>
        <xsd:enumeration value="L"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StkZero">
    <xsd:annotation>
      <xsd:documentation>This indicates whether to suppress leading zeros if the numbering method is numeric. (N - No, Y - Yes). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StkPresentationLength">
    <xsd:annotation>
      <xsd:documentation>This is the maximum presentation length allowed for stock codes. If no value is supplied then a length of 15 is assumed if key type is N - Numeric and 30 if the key type is A - Alphanumeric.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="30"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StkMethod">
    <xsd:annotation>
      <xsd:documentation>This is the stock code numbering method. (M - Manual, A - Automatic, S - Scripted). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Manual;A=Automatic;S=Scripted;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="S"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextStockCode">
    <xsd:annotation>
      <xsd:documentation>This is the next stock code number to use. This element only applies if the numbering method is set to A - Automatic. The maximum size is dependent on the presentation length. If no value is supplied 1 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StockCodes">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="StkKeyType" minOccurs="0"/>
        <xsd:element ref="StkCase" minOccurs="0"/>
        <xsd:element ref="StkZero" minOccurs="0"/>
        <xsd:element ref="StkPresentationLength" minOccurs="0"/>
        <xsd:element ref="StkMethod" minOccurs="0"/>
        <xsd:element ref="NextStockCode" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SerKeyType">
    <xsd:annotation>
      <xsd:documentation>This is the serial key type (A - Alphanumeric, N - Numeric). If no value is supplied A is assumed.</xsd:documentation>
      <xsd:appinfo source="A=Alphanumeric;N=Numeric;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SerCase">
    <xsd:annotation>
      <xsd:documentation>This is the case allowed for serials if the key type is A - Alphanumeric (M - Mixed, U - Upper, L - Lower). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Mixed;U=Upper;L=Lower;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="U"/>
        <xsd:enumeration value="L"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SerZero">
    <xsd:annotation>
      <xsd:documentation>This indicates whether to suppress leading zeros if the numbering method is numeric. (N - No, Y - Yes). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SerPresentationLength">
    <xsd:annotation>
      <xsd:documentation>This is the maximum presentation length allowed for serials. If the key type is N - Numeric then a maximum of 15 is allowed and if it is A - Alphanumeric then a maximum of 50 is allowed. If no value is supplied then a length of 15 is assumed if key type is N - Numeric and 20 if the key type is A - Alphanumeric.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="50"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ManSerialNoTracking">
    <xsd:annotation>
      <xsd:documentation>This is the manual serial tracking suffix to use. (N - None, S - By stock code, C - By company). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=None;S=By stock code;C=By company;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="S"/>
        <xsd:enumeration value="C"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ManSerialNoTrackPrefix">
    <xsd:annotation>
      <xsd:documentation>This is the manual serial number tracking prefix. The format is 35 characters, alphanumeric. This element only applies if the suffix to use is C - Company.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:minLength value="0"/>
        <xsd:maxLength value="35"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ManSerialNoTrackSuffix">
    <xsd:annotation>
      <xsd:documentation>This is the manual serial number tracking next suffix. The format is 15 characters, alphanumeric but can only contain spaces or numeric characters. This element only applies if the suffix to use is C - Company.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:minLength value="0"/>
        <xsd:maxLength value="15"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Serials">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="SerKeyType" minOccurs="0"/>
        <xsd:element ref="SerCase" minOccurs="0"/>
        <xsd:element ref="SerZero" minOccurs="0"/>
        <xsd:element ref="SerPresentationLength" minOccurs="0"/>
        <xsd:element ref="ManSerialNoTracking" minOccurs="0"/>
        <xsd:element ref="ManSerialNoTrackPrefix" minOccurs="0"/>
        <xsd:element ref="ManSerialNoTrackSuffix" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="LotKeyType">
    <xsd:annotation>
      <xsd:documentation>This is the lot key type (A - Alphanumeric, N - Numeric). If no value is supplied then A is assumed.</xsd:documentation>
      <xsd:appinfo source="A=Alphanumeric;N=Numeric;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LotCase">
    <xsd:annotation>
      <xsd:documentation>This is the case allowed for lots if the key type is A - Alphanumeric (M - Mixed, U - Upper, L - Lower). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Mixed;U=Upper;L=Lower;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="U"/>
        <xsd:enumeration value="L"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LotZero">
    <xsd:annotation>
      <xsd:documentation>This indicates whether to suppress leading zeros if the numbering method is numeric. (N - No, Y - Yes). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LotPresentationLength">
    <xsd:annotation>
      <xsd:documentation>This is the maximum presentation length allowed for lots. If the key type is N - Numeric then a maximum of 15 is allowed and if it is A - Alphanumeric then a maximum of 50 is allowed. If no value is supplied then a length of 10 is assumed.assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="50"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LotMethod">
    <xsd:annotation>
      <xsd:documentation>This is the lot numbering method. (M - Manual, A - Automatic). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="N=Manual;Y=Automatic;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextLot">
    <xsd:annotation>
      <xsd:documentation>This is the next lot number to use. This element only applies if the numbering method is set to A - Automatic. The maximum size is dependent on the presentation length. If no value is supplied 1 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LotEqualToJob">
    <xsd:annotation>
      <xsd:documentation>Lot number equal to job number (N-No, Y-Yes). If no value is supplied then Y is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Lots">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="LotKeyType" minOccurs="0"/>
        <xsd:element ref="LotCase" minOccurs="0"/>
        <xsd:element ref="LotZero" minOccurs="0"/>
        <xsd:element ref="LotPresentationLength" minOccurs="0"/>
        <xsd:element ref="LotMethod" minOccurs="0"/>
        <xsd:element ref="NextLot" minOccurs="0"/>
        <xsd:element ref="LotEqualToJob" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="BgrKeyType">
    <xsd:annotation>
      <xsd:documentation>This is the buying group key type (A - Alphanumeric, N - Numeric). If no value is supplied then A is assumed.</xsd:documentation>
      <xsd:appinfo source="A=Alphanumeric;N=Numeric;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="BgrZero">
    <xsd:annotation>
      <xsd:documentation>This indicates whether to suppress leading zeros if the numbering method is numeric. (N - No, Y - Yes). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="BuyingGroups">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="BgrKeyType" minOccurs="0"/>
        <xsd:element ref="BgrZero" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AstKeyType">
    <xsd:annotation>
      <xsd:documentation>This is the asset key type (A - Alphanumeric, N - Numeric). If no value is supplied then A is assumed.</xsd:documentation>
      <xsd:appinfo source="A=Alphanumeric;N=Numeric;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AstCase">
    <xsd:annotation>
      <xsd:documentation>This is the case allowed for assets if the key type is A - Alphanumeric (M - Mixed, U - Upper, L - Lower). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Mixed;U=Upper;L=Lower;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="U"/>
        <xsd:enumeration value="L"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AstZero">
    <xsd:annotation>
      <xsd:documentation>This indicates whether to suppress leading zeros if the numbering method is numeric. (N - No, Y - Yes). If no value is supplied then N is assumed.</xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AstPresentationLength">
    <xsd:annotation>
      <xsd:documentation>This is the maximum presentation length allowed for assets. If the key type is N - Numeric then a maximum of 15 is allowed and if it is A - Alphanumeric then a maximum of 30 is allowed. If no value is supplied then a length of 15 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="15"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AstMethod">
    <xsd:annotation>
      <xsd:documentation>This is the asset numbering method. (M - Manual, A - Automatic). If no value is supplied then M is assumed.</xsd:documentation>
      <xsd:appinfo source="M=Manual;A=Automatic;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextAsset">
    <xsd:annotation>
      <xsd:documentation>This is the next asset number to use. This element only applies if the numbering method is A - Automatic. The maximum size is dependent on the presentation length. If no value is supplied 1 is assumed.</xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
