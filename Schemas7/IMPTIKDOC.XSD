      <xsd:sequence>
        <xsd:element ref="AriKeyType" minOccurs="0"/>
        <xsd:element ref="AriCase" minOccurs="0"/>
        <xsd:element ref="AriZero" minOccurs="0"/>
        <xsd:element ref="AriPresentationLength" minOccurs="0"/>
        <xsd:element ref="AriPrefixLength" minOccurs="0"/>
        <xsd:element ref="AriMethod" minOccurs="0"/>
        <xsd:element ref="NextInvoice" minOccurs="0"/>
