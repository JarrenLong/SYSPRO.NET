                    <xsd:element name="WhsAlloc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WhsAllocUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedWhsAlloc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsAlloc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsAllocUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSlsAlloc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WhsBackOrd" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WhsBackOrdUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedWhsBackOrd" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsBackOrd" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsBackOrdUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSlsBackOrd" type="xsd:string" />
                    <xsd:element name="WhsBinShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WhsBinShipUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedWhsBinShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsBinShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsBinShipUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSlsBinShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WhsLotShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WhsLotShipUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedWhsLotShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsLotShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsLotShipUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSlsLotShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WhsSerShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WhsSerShipUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedWhsSerShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsSerShip" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SlsSerShipUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSlsSerShip" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>