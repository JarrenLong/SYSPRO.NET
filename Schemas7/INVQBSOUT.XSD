<!-- 
     Schema for the Inventory to Sales Balance Query Business Object
-->
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" id="InvToSalesBalanceQuery" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="InvToSalesBalanceQuery">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="DetailOrSummary" type="xsd:string" minOccurs="0"/>
