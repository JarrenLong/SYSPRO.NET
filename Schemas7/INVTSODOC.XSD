  <xsd:element name="DeplRemFromDefBin">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates if negative on hand, whether to deplete the remainder from default bin. (N - No, Y - Yes). If no value is supplied then the default of N will be assumed. If neither 'AutoDeplLotsInSO' nor 'AutoDeplLotsInPOS' have been set to Y, this element must contain N.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AutoDeplLotsDate">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the method by which to automatically deplete the lots by. (R - Receipt date, E - Expiry date). If no value is supplied then the default value of R will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Receipt date=;E=Expiry date;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="R"/>
        <xsd:enumeration value="E"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Depletion">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="AutoDeplLotsInSO"/>
        <xsd:element ref="AutoDeplLotsInPOS"/>
        <xsd:element ref="DeplRemFromDefBin"/>
        <xsd:element ref="AutoDeplLotsDate"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MultBinBrowseDefSeq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the type of multiple bin default sequence to use. (B - Bin, R - Receipt date). If no value is supplied then the default value of B will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="B=Bin;R=Receipt date;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="B"/>
        <xsd:enumeration value="R"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DisplayPriceQtyInvBrowse">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to display the price and quantities in the stock code browse flag. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="FilterStockCodesOpAccess">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to filter stock codes in browse based on operator access list. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DefWhForDispQty">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates which type of default warehouse to use in the browse when displaying quantities. (S - Standard, O - Operator default, C - Company default, W - Warehouse to use, D - Distribution warehouse to use). The default value is S (which will translate to spaces).]]></xsd:documentation>
      <xsd:appinfo source="S=Standard;O=Operator default;C=Company default;W=Warehouse to use;D=Distribution warehouse to use;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="S"/>
        <xsd:enumeration value="O"/>
        <xsd:enumeration value="C"/>
        <xsd:enumeration value="W"/>
        <xsd:enumeration value="D"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DefAtpPresentation">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the default ATP presentation to use. (R - Regular, C - Cumulative). If no value is supplied then the default value of R will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="R=Regular;C=Cumulative;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="R"/>
        <xsd:enumeration value="C"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ShowWhValsForMultComps">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to show warehouse values for multiple companies. (N - No, Y - Yes). The default value is N. If Y is supplied then 'InvQueryCompanyCode' must have at least 1 value supplied.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Browse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="MultBinBrowseDefSeq"/>
        <xsd:element ref="DisplayPriceQtyInvBrowse"/>
        <xsd:element ref="FilterStockCodesOpAccess"/>
        <xsd:element ref="DefWhForDispQty"/>
        <xsd:element ref="DefAtpPresentation"/>
        <xsd:element ref="ShowWhValsForMultComps"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="InvQueryCompanyCode">
    <xsd:annotation>
      <xsd:documentation><![CDATA[These are the selected company codes. These are only applicable if 'ShowWhValsForMultComps' is set to Y. The values supplied must be valid SYSPRO company codes.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="InvQueryCompanyName">
    <xsd:annotation>
      <xsd:documentation><![CDATA[These are the company names for the corresponding company codes entered in 'InvQueryCompanyCode'. The format is 50 characters, alphanumeric per company code, each separated by a comma.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Companies">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="InvQueryCompanyCode"/>
        <xsd:element ref="InvQueryCompanyName"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NextStockRecDoc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next stock receipt document. If no value is supplied then 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextIssueDoc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next issue document. If no value is supplied then 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextTrfOutDoc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next transfer out document. If no value is supplied then 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextInspectionDoc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next inspection print document. If no value is supplied then 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextRejectionDoc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next rejection print document. If no value is supplied then 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextScrappedDoc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next scrapped print document. If no value is supplied then 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StockCodeNumMethod">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the stock code numbering method to be used. (M - Manual, A - Automatic, S - Scripted). If no value is supplied then the default value of M will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="M=Manual;A=Automatic;S=Scripted;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="S"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextStockCode">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next Stock Code to use. This element is only to be supplied if 'StockCodeNumMethod' is set to A.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="GitReference">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the goods in transit reference to use. (M - Manual, A - By company, W - By warehouse, P - By warehouse with prefix). If no value is supplied then the default value of M will be assumed. May not be P if IM5-GTR-KEY is numeric. May not be A or W if IM5-GTR-KEY is alphanumeric.]]></xsd:documentation>
      <xsd:appinfo source="M=Manual;A=By company;W=By warehouse;P=By warehouse - with prefix;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="W"/>
        <xsd:enumeration value="P"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextGitReference">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next goods in transit reference number.This element may only be supplied if 'GitReference' is not set to A.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ManSerialNoTracking">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the manual serial tracking suffix to use. (N - None, S - By stock code, C - By company). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=None;S=By stock code;C=By company;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="S"/>
        <xsd:enumeration value="C"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ManSerialNoTrackPrefix">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the manual serial number tracking prefix. This may only be supplied if 'ManSerialNoTracking' is set to C.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ManSerialNoTrackSuffix">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the manual serial number tracking next prefix. This may only be supplied if 'ManSerialNoTracking' is C.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowExistInNonStkTable">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the stock code can be added if it exists in the non-stocked table. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Numbering">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="NextStockRecDoc"/>
        <xsd:element ref="NextIssueDoc"/>
        <xsd:element ref="NextTrfOutDoc"/>
        <xsd:element ref="NextInspectionDoc"/>
        <xsd:element ref="NextRejectionDoc"/>
        <xsd:element ref="NextScrappedDoc"/>
        <xsd:element ref="StockCodeNumMethod"/>
        <xsd:element ref="NextStockCode"/>
        <xsd:element ref="GitReference"/>
        <xsd:element ref="NextGitReference"/>
        <xsd:element ref="ManSerialNoTracking"/>
        <xsd:element ref="ManSerialNoTrackPrefix"/>
        <xsd:element ref="ManSerialNoTrackSuffix"/>
        <xsd:element ref="AllowExistInNonStkTable"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DoNotPromptForPrice">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the configurator option to not prompt for prices when running from the sales orders or quotes. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AcceptZeroPrice">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the configurator option to accept zero prices when running from the sales orders or quotes. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. This may only be set to Y if 'DoNotPromptForPrice' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CreateJobForExistParent">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the configurator option to create a job through sales orders using existing selection criteria generated stock codes. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="OverrideExistParentDesc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the configuration option to override stock description when using an existing stock code to create a job. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. May only be set to Y if 'CreateJobForExistParent' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DoNotUpdWhCostExistParent">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the configurator option not to update the warehouse cost for an existing parent.(N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. May only be set to Y if  'CreateJobForExistParent' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CostsForBoItemsInBom">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the costs to use for bought out items when creating a Bill of Material. (I - Inventory, B - BOM). If no value is supplied then the default value of I will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="I=Inventory;B=BOM;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="I"/>
        <xsd:enumeration value="B"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="UseProductConfiguratorWeb">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the configurator option to use the web based product configurator. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ProductConfiguratorWebPath">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the configurator web path. The format is 255 characters, alphanumeric. This may only be supplied if 'UseProductConfiguratorWeb' is Y.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DontAllowSpacesInGenStk">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the configurator option not to allow spaces in the generated parent stock code.(N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Configurator">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DoNotPromptForPrice"/>
        <xsd:element ref="AcceptZeroPrice"/>
        <xsd:element ref="CreateJobForExistParent"/>
        <xsd:element ref="OverrideExistParentDesc"/>
        <xsd:element ref="DoNotUpdWhCostExistParent"/>
        <xsd:element ref="CostsForBoItemsInBom"/>
        <xsd:element ref="UseProductConfiguratorWeb"/>
        <xsd:element ref="ProductConfiguratorWebPath"/>
        <xsd:element ref="DontAllowSpacesInGenStk"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
