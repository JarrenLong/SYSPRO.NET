  <xsd:element name="MthsRetInvJnls">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of months to retain the printed inventory journals. This element may not be zeros. A value of 99 will retain them indefinitely. Valid values are 1 - 60 or 99 only.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MthsRetInvDocs">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of months to retain the inventory documents. A value of 0 will purge them when printed, 99 will mean they never get purged. Valid values are 0 - 60 or 99 only.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="GlPerRetGitEntries">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of GL periods to retain the inventory GIT entries. If no value is supplied, a minimum value of 3 will be assumed. A value of 99 will retain them indefinitely. If the Inventory integrated with G/L is set to No or Report only, then this element will be set to 3 as a default. Valid values are 3 - 60 or 99 only.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DayRetBucketsZeroQty">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of days to retain the FIFO buckets with a zero quantity. If no value is supplied then the default value of 9999 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="9999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PurgeMoveRecs">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the method by which to purge the inventory movement records. (N - By number, D - By date). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=By number;D=By date;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="D"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MaxNoRecsRet">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the maximum number of movement records to retain (if 'PurgeMoveRecs' is set to N) or the maximum age of transactions before they are deleted. (if 'PurgeMoveRecs' is set to D). If no value is supplied then the default value of 99999 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RetLast12MthOpenStockBal">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to retain the last 12 months opening stock balance quantities. (Y - Yes, N - No). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RetHistOfPrcChg">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to retain the history of price changes. (Option updated with 0 when N, 1 when Y). If no value is supplied then the default of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DaysRetPriceChgHist">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of days to retain the price history. This element only applies if 'RetHistOfPrcChg' is set to Y. If applicable, the maximum value of 9999 will apply. If 'RetHistOfPrcChg' is set to N, then the default value of 0 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0"/>
        <xsd:maxInclusive value="9999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="HistRetExprInMonths">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the history retentions are to be expressed in months. (Option updated with 0 when N, 1 when Y). If no value is supplied then the default of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
