<?xml version="1.0" encoding="Windows-1252"?>
<!-- Copyright 1994-2014 SYSPRO Ltd.-->
<!-- 
     Schema for the Inventory Setup Transaction Posting Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="InventoryOptions">
    <xsd:annotation>
      <xsd:documentation>This is the root element.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="false">
      <xsd:sequence>
        <xsd:element ref="General" minOccurs="0"/>
        <xsd:element ref="DependentOnWarehouseList" minOccurs="0"/>
        <xsd:element ref="DependentOnWhRange" minOccurs="0"/>
        <xsd:element ref="Options" minOccurs="0"/>
        <xsd:element ref="DefineOptions" minOccurs="0"/>
        <xsd:element ref="DelOptions" minOccurs="0"/>
        <xsd:element ref="History" minOccurs="0"/>
        <xsd:element ref="PeriodCurrentYear" minOccurs="0"/>
        <xsd:element ref="PeriodPreviousYear" minOccurs="0"/>
        <xsd:element ref="UserDefined" minOccurs="0"/>
        <xsd:element ref="Documents" minOccurs="0"/>
        <xsd:element ref="CurrentMonth" minOccurs="0"/>
        <xsd:element ref="Depletion" minOccurs="0"/>
        <xsd:element ref="Browse" minOccurs="0"/>
        <xsd:element ref="Companies" minOccurs="0"/>
        <xsd:element ref="Numbering" minOccurs="0"/>
        <xsd:element ref="Configurator" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FifoValuation">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether FIFO valuation is required. (Option updated with 'F' when Y, 'N' when N). If no value is supplied then the default value of N will be assumed. This may not be selected if LIFO costing is installed and must be selected if FIFO costing is installed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MultipleBins">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether Multiple bins are required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. This may not be selected if LIFO costing is installed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="InvAmendJnlsReq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the stock code ammendment journals are required. (N - No, Y - Yes, A - Yes with additions). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;A=Yes, with additions;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="A"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RoundWipAllocs">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the Inventory/WIP component rounding flag to the maximum number of decimals. (S - 5 up/4 down, U - Round Up, D - Round Down). If no value is supplied then the default value of S will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="S=5-up, 4-down;U=Round up;D=Round down;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="S"/>
        <xsd:enumeration value="U"/>
        <xsd:enumeration value="D"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ABPercentageBreak">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the A/B Percentage break for ABC analysis. If no value is supplied then the default value of 80 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="BCPercentageBreak">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the B/C Percentage break for ABC analysis. If no value is supplied then the default value of 95 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AcceptCostVarPerc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the maximum acceptable cost variance percentage.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0.00"/>
        <xsd:maxInclusive value="999.99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RejectImpRecCostVarExc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to apply cost variance checking and reject lines when importing purchase orders and job receipts. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="UseFullGit">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to use the full goods in transit transfers facility. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. All warehouse transfers should be cleared before switching this option on.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="UseSctDesciption">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to use the sales order description for stock items originating from SCT. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="GitUseAbsoluteDays">
    <xsd:annotation>
      <xsd:documentation><![CDATA[ Indicates whether to use absolute days when applying lead time to transfer supplied items. Only valid when the full goods in transit transfers facility is switched on. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CostingPerWh">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the costing per warehouse should apply. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. This option is not allowed if LIFO costing is installed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ApplyWhBomCost">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether warehouse BOM costs should apply. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CostingMethod">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the costing method to be used. (A - Average, S - Standard, L - Last Cost, F - FIFO, l - LIFO). If no value is supplied then the default value of A will be assumed. FIFO may not be selected if cost uom other than stocked required.]]></xsd:documentation>
      <xsd:appinfo source="A=Average;S=Standard;L=Last Cost;F=FIFO;l=LIFO;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="S"/>
        <xsd:enumeration value="L"/>
        <xsd:enumeration value="F"/>
        <xsd:enumeration value="l"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DoNotCalcAveCostNegRec">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether NOT to recalculate the average cost on negative receipts. (N - Recalculate, Y - Do Not Recalculate). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ActualCosting">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether actual costing is to apply. (N - None, L - Lot Traceable, S - Batch Serial). If no value is supplied then the default value of N will be assumed. B may not be selected if stock codes set to record serials during order entry exist. Set <RunActualCostingConversion> to Y if this value is changed to either L or S to automatically run the conversion program.]]></xsd:documentation>
      <xsd:appinfo source="N=None;L=Lot traceable;S=Batch serial;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="L"/>
        <xsd:enumeration value="S"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AutoDeplCostBuckets">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether depletion of cost buckets must be automatic. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RecordInvMmBySerialLot">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to record the inventory movements by serial or lot. (Option updated with 1 when Y, 0 when N). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StockOnHandAllowNeg">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether Stock on hand is allowed to go negative. (N - No, Y - Yes, L - Dependent on warehouse list, R - Dependent on warehouse range, W - Dependent on warehouse). If no value is supplied then the default value of Y will be assumed. This may only be set to N if FIFO or LIFO costing is installed.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;L=Dependent on w/h list;R=Dependent on w/h range;W=Dependent on warehouse;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="L"/>
        <xsd:enumeration value="R"/>
        <xsd:enumeration value="W"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="General">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="FifoValuation"/>
        <xsd:element ref="MultipleBins"/>
        <xsd:element ref="InvAmendJnlsReq"/>
        <xsd:element ref="RoundWipAllocs"/>
        <xsd:element ref="ABPercentageBreak"/>
        <xsd:element ref="BCPercentageBreak"/>
        <xsd:element ref="AcceptCostVarPerc"/>
        <xsd:element ref="RejectImpRecCostVarExc"/>
        <xsd:element ref="UseFullGit"/>
        <xsd:element ref="UseSctDesciption"/>
        <xsd:element ref="GitUseAbsoluteDays"/>
        <xsd:element ref="CostingPerWh"/>
        <xsd:element ref="ApplyWhBomCost"/>
        <xsd:element ref="CostingMethod"/>
        <xsd:element ref="DoNotCalcAveCostNegRec"/>
        <xsd:element ref="ActualCosting"/>
        <xsd:element ref="AutoDeplCostBuckets"/>
        <xsd:element ref="RecordInvMmBySerialLot"/>
        <xsd:element ref="StockOnHandAllowNeg"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NegQtyInSelWhList">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether a negative quantity is allowed in selected warehouses. (A - Allowed, D - Disallowed). If no value is supplied then the default value of A will be assumed. This only applies if the element 'NegQtyInSelWh' is set to L (Dependent on warehouse list).]]></xsd:documentation>
      <xsd:appinfo source="A=Allowed;D=Disallowed"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="D"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Warehouse">
    <xsd:annotation>
      <xsd:documentation><![CDATA[These are the warehouse codes where the stock is allowed to go negative. A maximum of 10 warehouse codes are allowed. Only applies if 'NegQtyInSelWh' is set to L (Dependent on warehouse list). The format is alphanumeric per warehouse code, each separated by a comma. The warehouse codes supplied must be valid SYSPRO warehouses.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DependentOnWarehouseList">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="NegQtyInSelWhList"/>
        <xsd:element ref="Warehouse"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="NegQtyInSelWh">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether a negative quantity is allowed in selected warehouses. (A - Allowed, D - Disallowed). If no value is supplied then the default value of A will be assumed. This only applies if the element 'NegQtyInSelWh' is set to R (Dependent on warehouse range.]]></xsd:documentation>
      <xsd:appinfo source="A=Allowed;D=Disallowed"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="D"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="WhSelection">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the type of range of warehouses where the stock is allowed to go negative. (A - All, R - Range, S - Single). If no value is supplied then the default value of A will be assumed. This only applies if 'NegQtyInSelWh' is set to R (Dependent on  warehouse range).]]></xsd:documentation>
      <xsd:appinfo source="A=All;R=Range;S=Single;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="R"/>
        <xsd:enumeration value="S"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="FromWarehouse">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This entry only applies and is mandatory if 'WhSelection' has been set to R (Range) or S (Single). This contains the warehouse FROM where the stock is allowed to go negative.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ToWarehouse">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This entry only applies and is mandatory if 'WhSelection' has been set to R (Range). This contains the warehouse TO where the stock is allowed to go negative.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DependentOnWhRange">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="NegQtyInSelWh"/>
        <xsd:element ref="WhSelection"/>
        <xsd:element ref="FromWarehouse"/>
        <xsd:element ref="ToWarehouse"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AgedInvValReq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the aged inventory valuation is required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="UpdLastCostIfStdCost">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the last cost must be updated when using standard costing. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="InclInvNmCostInLastCost">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to include non-merchandise costs in last cost. (Y - Yes, N - No). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SerialTrackReq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the serial tracking system is required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SameSerialMultStockCodes">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the same serial is to be used for mutiple stock codes. (N - No, Y - Yes). This element only applies if 'SerialTrackReq' is set to Y. If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RetSerialInfoAfterNegRec">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether all serial information after negative receipts is to be retained. (N - No, Y - Yes). This element only applies if 'SerialTrackReq' is set to Y. If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ApplyCostMultInvRec">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether cost multiplier must apply when processing receipts. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SuppStockCodeIchgReq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the supplier/stock code interchange system is required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MultBatchSerialRecAllowed">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether mutiple batch serial receipts are allowed. (Y - Yes, N - No). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowBlankListPrcCode">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether blank price list codes are allowed. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ValidateProdClass">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the product class must be validated. (Option updated with 1 when Y, 0 when N). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowBackflushOfBoItems">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether backflushing of bought out items is allowed. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RecordMoveForBinTrf">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether movements for bin transfers should be recorded. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowCreateBins">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether creation of bins (when processing inventory transactions) is allowed. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ApplyOpAccesCtlDestWh">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to apply operator access control to destination warehouses on transfers OUT. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="InvCostUomOtherThanStocked">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether inventory cost is to be held in a unit of measure other than stocked. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. This may only be set to N if FIFO, LIFO or ABC is installed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NegQtyOnHandInInvVal">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the negative quantity on hand is to be included in the inventory valuation. (N - No, Y -Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodes">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to request the ledger codes for inventory receipts and adjustments. (N - No, Y - Yes, L - List). If Y or L is selected, at least 1 of the 'DefineOptions' elements must be entered. If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;L=List;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="L"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DisallowSingleWhPerEnd">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to disallow a single warehouse selection for the month-end function. (Option updated with 0 when N, 1 when Y). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SetUpStockCodeDelOpts">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to set up stock code deletion options. (N - No, Y - Yes). If any of the 'DelOptions' elements have been selected, then this element should contain Y. If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PrintFifoJnls">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to print the FIFO journals. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. Only applies if 'FifoValuation' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="BalLotsMostRecFifoBucket">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to balance lots from the most recent FIFO bucket. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. This only applies if 'FifoValuation' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="UseZeroCostBucketRets">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to use zero FIFO buckets when processing returns. (Option updated with 0 when N, 1 when Y). If no value is supplied then the default value of N will be assumed. This only applies if 'FifoValuation' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RecalcCostFromFifoBalFunc">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to recalculate the cost from FIFO when the period-end balance function is run. (Option updated with 0 when N, 1 when Y). If no value is supplied then the default value of N will be assumed. This only applies if 'FifoValuation' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RecalcCostFromFifoChgWhCost">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to recalculate the current cost field to average of FIFO bins on hand when warehouse costing method changes from FIFO. (Option updated with 0 when N, 1 when Y). If no value is supplied then the default value of N will be assumed. This only applies if 'FifoValuation' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StockTakeVariance">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the type of stock take variance to use. (P - Percentage, V - Variance). If no value is supplied then the default value of P will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="P=Percentage;V=Variance;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="P"/>
        <xsd:enumeration value="V"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="StockTakeVarMaxPerm">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the maximum permitted stock take variance percentage.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="-999999999999.9"/>
        <xsd:maxInclusive value="999999999999.99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="UomDefault">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the default unit of measure. If no value is supplied then the default value of EA will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Options">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="AgedInvValReq"/>
        <xsd:element ref="UpdLastCostIfStdCost"/>
        <xsd:element ref="InclInvNmCostInLastCost"/>
        <xsd:element ref="SerialTrackReq"/>
        <xsd:element ref="SameSerialMultStockCodes"/>
        <xsd:element ref="RetSerialInfoAfterNegRec"/>
        <xsd:element ref="ApplyCostMultInvRec"/>
        <xsd:element ref="SuppStockCodeIchgReq"/>
        <xsd:element ref="MultBatchSerialRecAllowed"/>
        <xsd:element ref="AllowBlankListPrcCode"/>
        <xsd:element ref="ValidateProdClass"/>
        <xsd:element ref="AllowBackflushOfBoItems"/>
        <xsd:element ref="RecordMoveForBinTrf"/>
        <xsd:element ref="AllowCreateBins"/>
        <xsd:element ref="ApplyOpAccesCtlDestWh"/>
        <xsd:element ref="InvCostUomOtherThanStocked"/>
        <xsd:element ref="NegQtyOnHandInInvVal"/>
        <xsd:element ref="ReqLedgerCodes"/>
        <xsd:element ref="DisallowSingleWhPerEnd"/>
        <xsd:element ref="SetUpStockCodeDelOpts"/>
        <xsd:element ref="PrintFifoJnls"/>
        <xsd:element ref="BalLotsMostRecFifoBucket"/>
        <xsd:element ref="UseZeroCostBucketRets"/>
        <xsd:element ref="RecalcCostFromFifoBalFunc"/>
        <xsd:element ref="RecalcCostFromFifoChgWhCost"/>
        <xsd:element ref="StockTakeVariance"/>
        <xsd:element ref="StockTakeVarMaxPerm"/>
        <xsd:element ref="UomDefault"/>
        <xsd:element name="RoundToStockDecimals" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation><![CDATA[Indicates whether quantities converted to stocking unit of measure must be rounded to the number of decimals specified against the stock code. Default is 'N'.]]></xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="N"/>
              <xsd:enumeration value="Y"/>
              <xsd:enumeration value=" "/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="MaxDecimalsAllowedInQtys" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation><![CDATA[Indicates the maximum number of decimals allowed for quantities. This is only applied if not rounding to stock code decimals ('RoundToStockDecimals' option is not set to 'Y').
Valid values are 0 - 6. Default is '6'.]]></xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:minLength value="0"/>
              <xsd:maxLength value="1"/>
              <xsd:whiteSpace value="preserve"/>
              <xsd:enumeration value="0"/>
              <xsd:enumeration value="1"/>
              <xsd:enumeration value="2"/>
              <xsd:enumeration value="3"/>
              <xsd:enumeration value="4"/>
              <xsd:enumeration value="5"/>
              <xsd:enumeration value="6"/>
              <xsd:enumeration value=" "/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="AllowTransfSerialStockTake" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation><![CDATA[This option allows you to transfer serial numbers for stock items that are currently in stock take. Default is 'N']]></xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="N"/>
              <xsd:enumeration value="Y"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesReceipts">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for receipts. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesAdjusts">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for adjustments. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesIssues">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for issues. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesCostMods">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for cost modifications. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesCostChgs">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for cost changes. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesPhysical">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for physical. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesTransfers">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for stock transfers. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="POReceipts">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether ledger codes are to be requested for purchase order receipts. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y. If this is set to N, the next 3 elements ('ReqLedgerCodesDebits', 'ReqLedgerCodesCredits' and 'ReqLedgerCodesNonMerch') must be set to N as well.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesDebits">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for debits (non stocked). (N - No, Y - Yes). Y may only be supplied if both 'ReqLedgerCodes' and 'POReceipts' are set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesCredits">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for debits credits. (N - No, Y - Yes). Y may only be supplied if both 'ReqLedgerCodes' and 'POReceipts' are set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqLedgerCodesNonMerch">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the ledger code is to be requested for non-merchandised. (N - No, Y - Yes). Y may only be supplied if both 'ReqLedgerCodes' and 'POReceipts' are set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowChgGlCodeInBackflush">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the change of ledger code in backflushing. (Y - Yes, N - No). Y may only be supplied if 'ReqLedgerCodes' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DefineOptions">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ReqLedgerCodesReceipts"/>
        <xsd:element ref="ReqLedgerCodesAdjusts"/>
        <xsd:element ref="ReqLedgerCodesIssues"/>
        <xsd:element ref="ReqLedgerCodesCostMods"/>
        <xsd:element ref="ReqLedgerCodesCostChgs"/>
        <xsd:element ref="ReqLedgerCodesPhysical"/>
        <xsd:element ref="ReqLedgerCodesTransfers"/>
        <xsd:element ref="POReceipts"/>
        <xsd:element ref="ReqLedgerCodesDebits"/>
        <xsd:element ref="ReqLedgerCodesCredits"/>
        <xsd:element ref="ReqLedgerCodesNonMerch"/>
        <xsd:element ref="AllowChgGlCodeInBackflush"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DelStockCodeConfig">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the deletion of stock codes used in a configuration. (Option updated with 0 when N, 1 when Y). Y may only be supplied if 'SetUpStockCodeDelOpts' has been set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DelStockCodeCustXref">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the deletion of stock codes if it has a customer/stock code cross reference record against it. (Option updated with 0 when N, 1 when Y). Y may only be supplied if 'SetUpStockCodeDelOpts' has been set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DelStockCodeSuppXref">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the deletion of stock codes if it has a supplier/stock code cross reference record against it. (Option updated with 0 when N, 1 when Y). Y may only be supplied if 'SetUpStockCodeDelOpts' has been set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DelStockCodeEccDrawReg">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the deletion of stock codes if an ECC drawing number exists against it. (Option updated with 0 when N, 1 when Y). Y may only be supplied if 'SetUpStockCodeDelOpts' has been set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DelStockCodeBillLandCost">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the deletion of stock codes if it is part of a LCT bill. (Option updated with 0 when N, 1 when Y). Y may only be supplied if 'SetUpStockCodeDelOpts' has been set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DelStockCodeTempSoPoDet">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the deletion of a temp stock code if purchase order or sales order details exist against it. (Option updated with 0 when N, 1 when Y). Y may only be supplied if 'SetUpStockCodeDelOpts' has been set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DelStockCodeOnHold">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the deletion of stock codes if on hold. (Option updated with 0 when N, 1 when Y). Y may only be supplied if 'SetUpStockCodeDelOpts' has been set to Y.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DelOptions">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DelStockCodeConfig"/>
        <xsd:element ref="DelStockCodeCustXref"/>
        <xsd:element ref="DelStockCodeSuppXref"/>
        <xsd:element ref="DelStockCodeEccDrawReg"/>
        <xsd:element ref="DelStockCodeBillLandCost"/>
        <xsd:element ref="DelStockCodeTempSoPoDet"/>
        <xsd:element ref="DelStockCodeOnHold"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SalesHistQtyIncl">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This shows how the sales history quantities are to be represented. (B - Both sales and issues, S - Sales, I - Issues). If no value is supplied then the default value of B will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="B=Sales and issues;S=Sales;I=Issues;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="B"/>
        <xsd:enumeration value="S"/>
