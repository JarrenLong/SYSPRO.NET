<!-- 
     Schema for the Lot Traceability Setup Transaction Posting Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="lottraceabilityoptions">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Item" type="xsd:string" minOccurs="0" />
        <xsd:element name="UpdateSummary" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="OptionsChanged" type="xsd:string" minOccurs="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>