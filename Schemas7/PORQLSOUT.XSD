<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--      Schema for the LCT Shipment Tracking Query Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="Back" type="xsd:string"/>	<xsd:element name="DataType" type="xsd:string"/>	<xsd:element name="DepartureDate">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Value" minOccurs="0"/>				<xsd:element ref="DataType" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Fwd" type="xsd:string"/>	<xsd:element name="Header" type="xsd:string"/>	<xsd:element name="HeaderDetails">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Header" minOccurs="0" maxOccurs="unbounded"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="LCTShipmentTrackingReportQueryListing">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Title" minOccurs="0"/>				<xsd:element ref="Row" maxOccurs="unbounded"/>				<xsd:element ref="NextPrevKey"/>				<xsd:element ref="HeaderDetails"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="NextKey" type="xsd:string"/>	<xsd:element name="NextPrevKey">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="PrevKey" minOccurs="0"/>				<xsd:element ref="NextKey" minOccurs="0"/>				<xsd:element ref="Fwd" minOccurs="0"/>				<xsd:element ref="Back" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="PrevKey" type="xsd:string"/>	<xsd:element name="Row">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Vessel"/>				<xsd:element ref="DepartureDate"/>				<xsd:element ref="VesselName"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Title" type="xsd:string"/>	<xsd:element name="Value" type="xsd:string"/>	<xsd:element name="Vessel">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Value" minOccurs="0"/>				<xsd:element ref="DataType" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="VesselName">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Value" minOccurs="0"/>				<xsd:element ref="DataType" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element></xsd:schema>