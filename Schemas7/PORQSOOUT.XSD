						<xsd:sequence>
							<xsd:element name="ReqNumMethod" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SendConfOfRecToOrig" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UpdCapexPoMaint" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CheckMaxValueAtLevel" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CheckMaxValueAtTime" type="xsd:string" minOccurs="0"/>
							<xsd:element name="MinCapexReqLnValue" type="xsd:string" minOccurs="0"/>
