							<xsd:element name="AutoPromptBillNsLines" type="xsd:string" minOccurs="0"/>
							<xsd:element name="InclOtherCostsInLandedCost" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AutoPromptExchRate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseFixedExchRate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AllowZeroPriceLines" type="xsd:string" minOccurs="0"/>
							<xsd:element name="InclLandedCostInLastCost" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ConvertQtyToStockedUom" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UpdatePoDueDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AutoSetSoRecQtyEqual" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AutoSetSoRecQtyLess" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AutoSetSoRecQtyGreater" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Numbering">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="PONumMethod" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NextPONum" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NextGrnNum" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NextReqNum" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CurrentMonth">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="PoCurrentYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PoCurrentMonth" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AccPeriodsPerYear" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
