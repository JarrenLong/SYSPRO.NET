							<xsd:element name="RecordApprovals" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ApprovalLevelChecking" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RequisitionBudgetsReq" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RequisitionBudgetsLevel" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RequisitionBudgetsTerms" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
