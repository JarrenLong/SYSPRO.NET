<!-- 
     Schema for the Purchase Order Setup Query Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="PurchaseOrderOptions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Company">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="General">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="DefPriceMethodPo" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseAltUomPOQty" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AllowMntCompPo" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseAltDocPrice" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RetainPoHdrWithoutLines" type="xsd:string" minOccurs="0"/>
							<xsd:element name="InclPONmCostInLastCost" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ShipInstrDefMsg" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LiableForScrap" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Options">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="InsertPOAddText" type="xsd:string" minOccurs="0"/>
							<xsd:element name="InsertDangerGdsTextInPO" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SetPoStatusAfterMaint" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PlusLessReqPo" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RestrictOrdToSingleWh" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AllowMntDelAddr" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AllowMntExchRate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ValidateBuyerPoCreated" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ManPoWhenBpoExists" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DefaultTaxStatus" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="History">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="MaintHistOfReceipts" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AllowIncomplShipArchive" type="xsd:string" minOccurs="0"/>
