							<xsd:element name="DaysRetGrnJnl" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DaysRetGrnPrVar" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DaysRetCompBpoContr" type="xsd:string" minOccurs="0"/>
							<xsd:element name="HistRetExprInMonths" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="GRN">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="GrnSuspReq" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GrnNumMethod" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AutoGrnForPoLine" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AutoGrnForPoLineRecAll" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AutoGrnForShipment" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CheckDuplGrnNum" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AllowNegRecGrnMatched" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Details">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="DefFreightChgCurr" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DefMiscChgCurr" type="xsd:string" minOccurs="0"/>
							<xsd:element name="InclFreightMiscInXml" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DaysEarlyLateSupply" type="xsd:string" minOccurs="0"/>
							<xsd:element name="QtyOverUnderPerc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AcceptCostPercVarExpCost" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AcceptCostPercVarWhCost" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Requisition">
