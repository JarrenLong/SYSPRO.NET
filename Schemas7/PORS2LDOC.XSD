<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2015 SYSPRO Ltd.--><!--     Schema for the LCT Route Maintenance Setup Business Object --><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="Description" type="xsd:string">    <xsd:annotation>      <xsd:documentation>This element contains information about the Route Description. It is mandatory.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Item">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Key"/>        <xsd:element ref="Description"/>        <xsd:element ref="DaysBeforeEta" minOccurs="0"/>        <xsd:element ref="DemurrageDays" minOccurs="0"/>        <xsd:element ref="DaysAfterEta" minOccurs="0"/>        <xsd:element ref="eSignature" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Key">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Route"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="DaysBeforeEta" type="xsd:decimal">    <xsd:annotation>      <xsd:documentation>This element contains information about the number of days taken for the shipment before ETA It is optional.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="DemurrageDays" type="xsd:decimal">    <xsd:annotation>      <xsd:documentation>This element contains information about the number of days taken for demurrage. It is optional.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="DaysAfterEta" type="xsd:decimal">    <xsd:annotation>      <xsd:documentation>This element contains information about the number of days taken for the shipment after ETA. It is optional.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="eSignature"  type="xsd:string">    <xsd:annotation>      <xsd:documentation>GUID for eSignatures</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="SetupLctRoute">    <xsd:annotation>      <xsd:documentation>This is the root node, it is mandatory.</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Item"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Route" type="xsd:string">    <xsd:annotation>      <xsd:documentation>This element contains information about the Route. It is the key. It is mandatory.</xsd:documentation>    </xsd:annotation>  </xsd:element></xsd:schema>