<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2015 SYSPRO Ltd.--><!--     Schema for the LCT Customs Exchange Maintenance Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="Item">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="ExchangeRate" minOccurs="0"/>				<xsd:element ref="MulDiv"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Key">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Currency"/>				<xsd:element ref="EffectiveDate" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="EffectiveDate" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Effective Date.It is part of the key. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Currency" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Currency. It is part of the key. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="ExchangeRate" type="xsd:double">		<xsd:annotation>			<xsd:documentation>This element contains information about the Exchange Rate. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="MulDiv">		<xsd:annotation>			<xsd:documentation>This element contains information about the Conversion Factor Code. It is mandatory. M - Multiply (Default) or D - Divide.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="M"/>				<xsd:enumeration value="D"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="SetupCustomsExchange">		<xsd:annotation>			<xsd:documentation>This is the root node, it is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element></xsd:schema>