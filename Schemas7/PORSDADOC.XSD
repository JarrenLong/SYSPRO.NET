<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2015 SYSPRO Ltd.--><!--     Schema for the PO Multiple Delivery Address Maintenance Setup Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="SetupPoMultiAddress">		<xsd:annotation>			<xsd:documentation>This is the root node, it is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Item">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="DeliveryName" minOccurs="0"/>				<xsd:element ref="DeliveryAddr1" minOccurs="0"/>				<xsd:element ref="DeliveryAddr2" minOccurs="0"/>				<xsd:element ref="DeliveryAddr3" minOccurs="0"/>				<xsd:element ref="DeliveryAddr3Loc" minOccurs="0"/>				<xsd:element ref="DeliveryAddr4" minOccurs="0"/>				<xsd:element ref="DeliveryAddr5" minOccurs="0"/>				<xsd:element ref="DeliveryPostCode" minOccurs="0"/>				<xsd:element ref="DeliveryGpsLat" minOccurs="0"/>				<xsd:element ref="DeliveryGpsLong" minOccurs="0"/>				<xsd:element ref="LanguageCode" minOccurs="0"/>				<xsd:element ref="ShippingLocation" minOccurs="0"/>				<xsd:element ref="DeliveryTerms" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Key">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="DeliveryAddrCode"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="DeliveryAddrCode" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Delivery Address Code. It is the key. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryName" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Name of the person to which the delivery must go. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryAddr1" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the 1st Address Line - Building. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryAddr2" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the 2nd Address Line - Street. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryAddr3" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the 3rd Address Line - City. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryAddr3Loc" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the 3rd Address Line - Locality. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryAddr4" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the 4th Address Line - State. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryAddr5" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the 5th Address Line - Country. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryPostCode" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Delivery Address Postal Code It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryGpsLat" type="xsd:decimal">		<xsd:annotation>			<xsd:documentation>This element contains information about the Delivery Adress Latitude. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryGpsLong" type="xsd:decimal">		<xsd:annotation>			<xsd:documentation>This element contains information about the Delivery Address Longitude. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="LanguageCode" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Language Code. It is optional. Note - This can only be entered when Multi-language for document printing is on. </xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="ShippingLocation" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Shipping Location. It is optional. Note - This can only be entered when EC VAT system is required or Capture delievery terms for non-EC members is on.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DeliveryTerms" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Delivery Terms. It is optional. Note - This can only be entered when EC VAT system is required or Capture delievery terms for non-EC members is on.</xsd:documentation>		</xsd:annotation>	</xsd:element></xsd:schema>