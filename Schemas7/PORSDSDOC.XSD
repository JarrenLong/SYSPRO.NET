<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2015 SYSPRO Ltd.--><!--     Schema for the PO Order Discounts Maintenance Setup Business Object --><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="Description" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Discount Code Description. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Item">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="Description"/>				<xsd:element ref="DiscountPct1" minOccurs="0"/>				<xsd:element ref="DiscountPct2" minOccurs="0"/>				<xsd:element ref="DiscountPct3" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Key">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="DiscountCode"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="DiscountPct1" type="xsd:decimal">		<xsd:annotation>			<xsd:documentation>This element contains information about the 1st Discount Percent. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DiscountPct2" type="xsd:decimal">		<xsd:annotation>			<xsd:documentation>This element contains information about the 2nd Discount Percent. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="DiscountPct3" type="xsd:decimal">		<xsd:annotation>			<xsd:documentation>This element contains information about the 3rd Discount Percent. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="SetupPoDiscount">		<xsd:annotation>			<xsd:documentation>This is the root node, it is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="DiscountCode" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the Discount Code. It is the key. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element></xsd:schema>