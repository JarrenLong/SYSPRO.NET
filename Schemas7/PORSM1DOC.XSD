<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2016 SYSPRO Ltd.--><!--     Schema for the Requisition User Maintenance Setup Business Object --><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="Item">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="UserName"/>				<xsd:element ref="Email" minOccurs="0"/>				<xsd:element ref="Password" minOccurs="0"/>				<xsd:element ref="CanAddReqn"/>				<xsd:element ref="CanAddReqnDetails"/>				<xsd:element ref="CanChgReqnDetails"/>				<xsd:element ref="CanApproveReqn"/>				<xsd:element ref="CanCreateOrder"/>				<xsd:element ref="NextUser"/>				<xsd:element ref="UserForPOrder"/>				<xsd:element ref="MaxLineValue" minOccurs="0"/>				<xsd:element ref="MaxApproveValue" minOccurs="0"/>				<xsd:element ref="MandatoryChkReqd"/>				<xsd:element ref="MaintainReqnFlag"/>				<xsd:element ref="InclPlanning"/>				<xsd:element ref="OnHoldFlag"/>				<xsd:element ref="ForwardToUser"/>				<xsd:element ref="SoReviewFlag"/>				<xsd:element ref="AuthWarehouse" minOccurs="0"/>				<xsd:element ref="RequisitionGroup"/>				<xsd:element ref="eSignature"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Key">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="UserCode"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="UserName" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the uiser name. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Email" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the email address. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Password" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the users password. It is optional</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="CanAddReqn" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the users ability to add a requisition (Y-Yes [Default] or N-No). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="CanAddReqnDetails" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the users ability to add requisition details (Y-Yes or N-No  [Default]). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="CanChgReqnDetails" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the users ability to change requisition details (Y-Yes or N-No  [Default]). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="CanApproveReqn" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the users ability to approve requisitions (Y-Yes or N-No  [Default]). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="CanCreateOrder" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the users ability to create a purchase order (Y-Yes or N-No  [Default]). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="NextUser" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the default next user for routing. It is optional. It cannot be the same as the user being added/changed.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="UserForPOrder" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the user to forward to for purchase order creation. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="MandatoryChkReqd" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information regarding requisitions created by this user being ticked automatically for mandatory users (Y-Yes [Default] or N-No). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="MaintainReqnFlag" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information stating if the user can maintain requisitions where the current holder is not the originator (Y-Yes [Default] or N-No). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="InclPlanning" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information stating if the user is included in the mrp calculation (Y-Yes or N-No  [Default]). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="OnHoldFlag" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about whether the user is on hold or not (Y-Yes or N-No  [Default]). It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="ForwardToUser" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the user to which requistions need to be forwarded to if the one being added or changed is on hold. It is mandatory when the user is on hold.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="SoReviewFlag" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the users ability to review sales orders (Y-Yes or N-No [Default]). It is mandatory. </xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="AuthWarehouse" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the authorized warehouse. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="RequisitionGroup" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the requisition group. It is mandatory when requisition budgets are required.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="MaxLineValue" type="xsd:decimal">		<xsd:annotation>			<xsd:documentation>This element contains information about the Maximum requistion line value. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="MaxApproveValue" type="xsd:decimal">		<xsd:annotation>			<xsd:documentation>This element contains information about the maximum requistion approval value. It is optional.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="SetupRequisitionUser">		<xsd:annotation>			<xsd:documentation>This is the root node, it is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="UserCode" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the User Code. It is the key. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="eSignature" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element contains information about the GUID supplied for esignature transactions. It is mandatory.</xsd:documentation>		</xsd:annotation>	</xsd:element></xsd:schema>