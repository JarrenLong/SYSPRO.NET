<xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="RmaLine">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Parameters">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="IgnoreWarnings" type="xsd:string"/>
              <xsd:element name="ApplyIfEntireDocumentValid" type="xsd:string"/>
              <xsd:element name="ValidateOnly" type="xsd:string"/>
              <xsd:element name="ShipFromDefaultBin" type="xsd:string"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
