<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="SetupSalesTargetSpreads">
    <xsd:annotation>
      <xsd:documentation>This is the root element</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Item">
          <xsd:annotation>
            <xsd:documentation>This element contains all the relevant information to maintain sales targets spread sheet</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Key">
                <xsd:annotation>
                  <xsd:documentation>This element contains the Key elements to maintain a sales targets spread sheet</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="TargetType" default="S">
                      <xsd:annotation>
                        <xsd:documentation>This element contains the target type you wish to maintain. (C) Customer, (P) Product class and (S) Sales history summary.  Default value is S.</xsd:documentation>
                      </xsd:annotation>
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:enumeration value="C"/>
                          <xsd:enumeration value="P"/>
                          <xsd:enumeration value="S"/>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="TargetSumSeq" type="xsd:string" minOccurs="0">
                      <xsd:annotation>
                        <xsd:documentation>This element contains the Sales history summary sequence type.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="SalesSpread1" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>This element contains the Sales targets % spread for month 1.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="ProfitSpread1" type="xsd:string" minOccurs="0">
