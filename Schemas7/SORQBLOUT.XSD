                    <xsd:element name="WarehouseDescription" type="xsd:string" minOccurs="0" />
                    <xsd:element name="OrdShipQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="OrdShipQtyUnits" type="xsd:string" minOccurs="0"/>
