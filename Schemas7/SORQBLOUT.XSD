                    <xsd:element name="OrdResvQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="OrdResvQtyUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedOrdResvQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BinShipQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="BinShipQtyUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedBinShipQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="BinResvQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="BinResvQtyUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedBinResvQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LotShipQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="LotShipQtyUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedLotShipQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LotResvQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="LotResvQtyUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedLotResvQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SerShipQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="SerShipQtyUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSerShipQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SerResvQty" type="xsd:string" minOccurs="0" />
                    <xsd:element name="SerResvQtyUnits" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EditedSerResvQty" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>