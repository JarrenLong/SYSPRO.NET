        <xsd:enumeration value="2,000"/>
        <xsd:enumeration value="4,000"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Customer">
    <xsd:simpleType>
      <xsd:restriction base="xsd:byte">
        <xsd:enumeration value="0000000001"/>
        <xsd:enumeration value="0000000002"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DeliveryRouteFilterType" type="xsd:string"/>
  <xsd:element name="DeliveryRouteFilterValueF">
    <xsd:complexType/>
  </xsd:element>
  <xsd:element name="DeliveryRouteFilterValueT">
    <xsd:complexType/>
  </xsd:element>
  <xsd:element name="Filter">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DeliveryRouteFilterType"/>
        <xsd:element ref="DeliveryRouteFilterValueF"/>
        <xsd:element ref="DeliveryRouteFilterValueT"/>
        <xsd:element ref="SalesOrderFilterType"/>
        <xsd:element ref="SalesOrderFilterValueF"/>
        <xsd:element ref="SalesOrderFilterValueT"/>
        <xsd:element ref="NumberOfDaysBeforeTodayFilterType"/>
        <xsd:element ref="NumberOfDaysBeforeTodayFilterValue"/>
        <xsd:element ref="NumberOfDaysAfterTodayFilterType"/>
        <xsd:element ref="NumberOfDaysAfterTodayFilterValue"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="InclZeroAvailabeToShip" type="xsd:string"/>
  <xsd:element name="IncludeCompleteSalesOrders" type="xsd:string"/>
  <xsd:element name="IncludeCompletedSaleOrderLines" type="xsd:string"/>
  <xsd:element name="IncludeSalesOrdersInSuspense" type="xsd:string"/>
  <xsd:element name="Item">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="Route"/>
        <xsd:element ref="SalesOrder"/>
        <xsd:element ref="SalesOrderLine"/>
