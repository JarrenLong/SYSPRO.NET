<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="AvailableToShip">
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="1,000"/>
        <xsd:enumeration value="1,250"/>
        <xsd:enumeration value="10,000"/>
        <xsd:enumeration value="12,000"/>
        <xsd:enumeration value="15,000"/>
        <xsd:enumeration value="2,000"/>
        <xsd:enumeration value="3,000"/>
        <xsd:enumeration value="5,000"/>
        <xsd:enumeration value="6,000"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="BackOrderQuantity">
