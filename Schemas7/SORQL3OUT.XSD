  <xsd:element name="Option">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="IncludeCompletedSaleOrderLines"/>
        <xsd:element ref="IncludeSalesOrdersInSuspense"/>
        <xsd:element ref="IncludeCompleteSalesOrders"/>
        <xsd:element ref="InclZeroAvailabeToShip"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Options">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="Option"/>
        <xsd:element ref="Filter"/>
