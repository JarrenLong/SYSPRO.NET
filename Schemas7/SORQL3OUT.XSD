        <xsd:element ref="ShipDate"/>
        <xsd:element ref="RequestDate"/>
        <xsd:element ref="Customer"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Mass">
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="0,000000"/>
        <xsd:enumeration value="0,050000"/>
