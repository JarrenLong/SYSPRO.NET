<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2012 SYSPRO Ltd.--><!--      This is an example XML instance to demonstrate     use of the SO Allocation of Reserved Stock Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="BinLocation">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="BinPieces">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="BinQuantity">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="BinUnits">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Bins">    <xsd:annotation>      <xsd:documentation>This element is optional. Must be supplied if multiple bins are in use and auto depletion is not set or is to be ignored.</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="BinLocation" minOccurs="0"/>        <xsd:element ref="BinQuantity" minOccurs="0"/>        <xsd:element ref="BinUnits" minOccurs="0"/>        <xsd:element ref="BinPieces" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Customer">    <xsd:annotation>      <xsd:documentation>This element is optional. If supplied, it must be the customer for the sales order being allocated.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Item">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Customer" minOccurs="0"/>        <xsd:element ref="SalesOrder"/>        <xsd:element ref="SalesOrderLine"/>        <xsd:element ref="StockCode" minOccurs="0"/>        <xsd:element ref="Warehouse" minOccurs="0"/>        <xsd:element ref="Quantity"/>        <xsd:element ref="UnitOfMeasure" minOccurs="0"/>        <xsd:element ref="Units" minOccurs="0"/>        <xsd:element ref="Pieces" minOccurs="0"/>        <xsd:element ref="Lot" minOccurs="0"/>        <xsd:element ref="Serials" minOccurs="0"/>        <xsd:element ref="Bins" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Lot">    <xsd:annotation>      <xsd:documentation>This element is optional. Must be supplied if sales order line if for a lot-traceable item and auto depletion is not set or is to be ignored.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Pieces">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="PostSorAllocateReserved">    <xsd:annotation>      <xsd:documentation>This is the root element.</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Item"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Quantity">    <xsd:annotation>      <xsd:documentation>This element is mandatory and must not exceed the quantity in reserve against the line.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="SalesOrder">    <xsd:annotation>      <xsd:documentation>This element is mandatory.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="SalesOrderLine">    <xsd:annotation>      <xsd:documentation>This element is mandatory and it must refer to a stocked merchandise line. No kits are allowed.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="SerialNumber">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="SerialPieces">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="SerialQuantity">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="SerialUnits">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Serials">    <xsd:annotation>      <xsd:documentation>This element is optional. Must be supplied if sales order line if for a serialized item. </xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="SerialNumber" minOccurs="0"/>        <xsd:element ref="SerialQuantity" minOccurs="0"/>        <xsd:element ref="SerialUnits" minOccurs="0"/>        <xsd:element ref="SerialPieces" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="StockCode">    <xsd:annotation>      <xsd:documentation>This element is optional. If supplied, it must be the stock code for the sales order line being allocated.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="UnitOfMeasure">    <xsd:annotation>      <xsd:documentation>This element is optional. If supplied indicates the unit of measure of the quantities entered and must be either the order unit of measure or the stocking unit of measure for the stock code on the line. If not supplied, it defaults to the order unit of measure and the quantities will be treated as such.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Units">    <xsd:annotation>      <xsd:documentation>This element is optional.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Warehouse">    <xsd:annotation>      <xsd:documentation>This element is optional. If supplied, it must be the warehouse for the sales order line being allocated.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element></xsd:schema>