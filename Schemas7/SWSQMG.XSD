<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2012 SYSPRO Ltd.--><!--     Schema for the Query Message Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="IncludeCompletedMessages">    <xsd:annotation>      <xsd:documentation>Indicates if the completed messages should be returned.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="N"/>        <xsd:enumeration value="Y"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Key">    <xsd:annotation>      <xsd:documentation>Key Node</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Type"/>        <xsd:element ref="OperatorRole"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="OperatorRole" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Must contain either the OperatorCode or RoleId.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Option">    <xsd:annotation>      <xsd:documentation>Option Node</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence minOccurs="0">        <xsd:element ref="IncludeCompletedMessages" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Query">    <xsd:annotation>      <xsd:documentation>Query root element</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Key"/>        <xsd:element ref="Option" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Type">    <xsd:annotation>      <xsd:documentation>The type of message, O-Operator, R-Role.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="O"/>        <xsd:enumeration value="R"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element></xsd:schema>