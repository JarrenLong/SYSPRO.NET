<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2012 SYSPRO Ltd.--><!--     Schema for the Workflow Security Query Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="CompanyId" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Company Code of the worflow to be queried</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Key">    <xsd:annotation>      <xsd:documentation>Key Node</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:all>        <xsd:element ref="Workflow" minOccurs="0"/>        <xsd:element ref="CompanyId" minOccurs="0"/>        <xsd:element ref="Operation" minOccurs="0"/>        <xsd:element ref="Operator" minOccurs="0"/>        <xsd:element ref="enetGuid" minOccurs="0"/>        <xsd:element ref="SecType"/>        <xsd:element name="RoleId" type="xsd:string" minOccurs="0">          <xsd:annotation>            <xsd:documentation>The roleid of the specified operator</xsd:documentation>          </xsd:annotation>        </xsd:element>      </xsd:all>    </xsd:complexType>  </xsd:element>  <xsd:element name="Operation" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Name of the Operation for the specified workflow</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Operator" type="xsd:string">    <xsd:annotation>      <xsd:documentation>SYSPRO Operator code</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Query">    <xsd:annotation>      <xsd:documentation>Root Node</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Key"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="SecType">    <xsd:annotation>      <xsd:documentation>The type of security information to be returned</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="W"/>        <xsd:enumeration value="SO"/>        <xsd:enumeration value="UO"/>        <xsd:enumeration value="SE"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Workflow" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Name of the workflow to be queried</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="enetGuid" type="xsd:string">    <xsd:annotation>      <xsd:documentation>e.net Guid of operator</xsd:documentation>    </xsd:annotation>  </xsd:element></xsd:schema>