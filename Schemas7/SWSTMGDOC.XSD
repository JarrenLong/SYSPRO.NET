<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--     Schema for the Workflow Message Post Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="ActionId" type="xsd:byte">    <xsd:annotation>      <xsd:documentation>The ActionId from the SwsActionDetail table.  Optional. </xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="ActioningOperator" type="xsd:string">    <xsd:annotation>      <xsd:documentation>The operator actioning the task</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Category">    <xsd:annotation>      <xsd:documentation>Indicates the message category, S-SYSPRO, W-Workflow.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="S"/>        <xsd:enumeration value="W"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="DateCreated" type="xsd:date">    <xsd:annotation>      <xsd:documentation>Must contain a date the message must be effective from. CCYY-MM-DD</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="ExpiryDate" type="xsd:date">    <xsd:annotation>      <xsd:documentation>Must contain a date the message must be effective until. CCYY-MM-DD. Zero date does not expire.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="ExpiryTime" type="xsd:time">    <xsd:annotation>      <xsd:documentation>Must contain a time the message must be effective until. HH:MM:SS.FFF. Zero time does not expire.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Message">    <xsd:annotation>      <xsd:documentation>The message</xsd:documentation>    </xsd:annotation>    <xsd:complexType mixed="true">      <xsd:sequence>        <xsd:element ref="Type"/>        <xsd:element ref="OperatorRole"/>        <xsd:element ref="DateCreated"/>        <xsd:element ref="TimeCreated"/>        <xsd:element name="MessageKey" type="xsd:string" minOccurs="0">          <xsd:annotation>            <xsd:documentation>This is an optional field that must only be supplied on an Update or Delete function.  It should contain the full key as provided from a previous post.</xsd:documentation>          </xsd:annotation>        </xsd:element>        <xsd:element ref="ExpiryDate" minOccurs="0"/>        <xsd:element ref="ExpiryTime" minOccurs="0"/>        <xsd:element ref="Category" minOccurs="0"/>        <xsd:element ref="Status" minOccurs="0"/>        <xsd:element ref="ActionId" minOccurs="0"/>        <xsd:element ref="Subject" minOccurs="0"/>        <xsd:element ref="Message" minOccurs="0"/>        <xsd:element ref="Priority" minOccurs="0"/>        <xsd:element ref="ActioningOperator" minOccurs="0"/>        <xsd:element ref="WorkflowName" minOccurs="0"/>        <xsd:element ref="WorkflowKey" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="OperatorRole" type="xsd:string">    <xsd:annotation>      <xsd:documentation>Must contain either the OperatorCode, NetworkUserName, or RoleId.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="PostMessages">    <xsd:annotation>      <xsd:documentation>PostMessages root element</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Message" maxOccurs="unbounded"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Priority" type="xsd:byte">    <xsd:annotation>      <xsd:documentation>The priority of the message</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Status">    <xsd:annotation>      <xsd:documentation>Indicates the status of the message. 1-ActionRequired,2-BeingActioned,99-ActionCompleted.</xsd:documentation>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:enumeration value="1"/>        <xsd:enumeration value="2"/>        <xsd:enumeration value="99"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Subject" type="xsd:string">    <xsd:annotation>      <xsd:documentation>The subject of the message.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="TimeCreated" type="xsd:time">    <xsd:annotation>      <xsd:documentation>Must contain a time the message must be effective from. HH:MM:SS.FFF</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="Type" type="xsd:string">    <xsd:annotation>      <xsd:documentation>The type of message, O-Operator, R-Role.</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="WorkflowKey" type="xsd:byte">    <xsd:annotation>      <xsd:documentation>The keyid of the workflow</xsd:documentation>    </xsd:annotation>  </xsd:element>  <xsd:element name="WorkflowName" type="xsd:string">    <xsd:annotation>      <xsd:documentation>The rest address or workflow name.</xsd:documentation>    </xsd:annotation>  </xsd:element></xsd:schema>