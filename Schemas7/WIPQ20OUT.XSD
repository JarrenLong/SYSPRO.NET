                                <xsd:element name="LabCostToDate" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="MatCostToDate" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="LabValueIssues" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="MatValueIssues" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="CurrentWIPValue" type="xsd:double" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
