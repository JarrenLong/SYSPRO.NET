<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--      Schema for the WIP Job Classification Maintenance Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="ClassJobPrefix">		<xsd:annotation>			<xsd:documentation>This element contains information about the WIP Job Classification, Job number prefix.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="ClassJobSuffix">		<xsd:annotation>			<xsd:documentation>This element contains information about the WIP Job Classification, Job number suffix.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="ClassDescription">		<xsd:annotation>			<xsd:documentation>This element contains information about the WIP Job Classification description. It is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="Item">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="ClassDescription"/>				<xsd:element ref="ClassJobPrefix" minOccurs="0"/>				<xsd:element ref="ClassJobSuffix" minOccurs="0"/>				<xsd:element ref="WipInspectPrefix" minOccurs="0"/>				<xsd:element ref="NextInspReference" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="JobClassification">		<xsd:annotation>			<xsd:documentation>This is the key to the WIP Job Classification being maintained. This is mandatory. If this WIP Job Classification is being added, it must not pre-exist. If this WIP Job Classification is being changed or deleted, it must be a valid Syspro WIP Job Classification.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="Key">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="JobClassification"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="NextInspReference">		<xsd:annotation>			<xsd:documentation>This element contains information about the WIP Job Classification, WIP Inspection reference suffix. </xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="SetupJobClassification">		<xsd:annotation>			<xsd:documentation>This is the root node, it is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="WipInspectPrefix">		<xsd:annotation>			<xsd:documentation>This element contains information about the WIP Job Classification, WIP Inspection reference prefix. </xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element></xsd:schema>