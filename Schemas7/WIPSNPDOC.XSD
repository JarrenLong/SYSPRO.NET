<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--      Schema for the WIP Labor Scrap And Non-productive Code Maintenance Business Object--><xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="Description">		<xsd:annotation>			<xsd:documentation>This element contains information about the WIP Labor Scrap And Non-productive Code description. It is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="ExpenseGlCode">		<xsd:annotation>			<xsd:documentation>This element contains information about the GL expense account for non-productive codes only</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="Item">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Key"/>				<xsd:element ref="Description"/>				<xsd:element ref="ExpenseGlCode" minOccurs="0"/>				<xsd:element name="LedgerCodePassword" minOccurs="0">					<xsd:annotation>						<xsd:documentation>This element contains information about the GL LedgerCode password.</xsd:documentation>					</xsd:annotation>					<xsd:simpleType>						<xsd:restriction base="xsd:string">							<xsd:whiteSpace value="preserve"/>						</xsd:restriction>					</xsd:simpleType>				</xsd:element>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Key">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="RecordFlag"/>				<xsd:element ref="NonProdScrap"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="NonProdScrap">		<xsd:annotation>			<xsd:documentation>This element contains information about the WIP Labor Scrap And Non-productive Code Reason/Non-Productive code. It is part of the key. It is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="RecordFlag">		<xsd:annotation>			<xsd:documentation>This element contains information about the WIP Labor Scrap And Non-productive Code type (R - Reason code for scrap and N - Non-productive code). It is part of the key. It is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:whiteSpace value="preserve"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="SetupScrapAndNonProductiveCodes">		<xsd:annotation>			<xsd:documentation>This is the root node, it is mandatory.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Item"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element></xsd:schema>