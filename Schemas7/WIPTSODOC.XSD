  <xsd:element name="AllowSubsLotsSerialsParent">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow substitution for lots/serials for parent parts. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowSubsLotsSerialsComp">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow substitution for lots/serials for component parts. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqCompToParentLotTrack">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether component to parent lot tracking is required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ReqCompToParentSerialTrack">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether component to parent serial tracking is required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LotsAndSerials">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ReserveLotsForParent"/>
        <xsd:element ref="ReserveSerialsForParent"/>
        <xsd:element ref="ReserveLotsForAlloc"/>
        <xsd:element ref="ReserveSerialsForAlloc"/>
        <xsd:element ref="AllowSubsLotsSerialsParent"/>
        <xsd:element ref="AllowSubsLotsSerialsComp"/>
        <xsd:element ref="ReqCompToParentLotTrack"/>
        <xsd:element ref="ReqCompToParentSerialTrack"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ChkPostThresholdAgainst">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates how to check the posting threshold when adding/issuing allocations. (1 - Anticipated value 1, 2 - Anticipated value 2, 3 - Anticipated value 3). If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="1=Anticipated value 1;2=Anticipated value 2;3=Anticipated value 3;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="1"/>
        <xsd:enumeration value="2"/>
        <xsd:enumeration value="3"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AutoExpandHierTreeview">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to automatically expand hierarchy treeviews in maintenance and posting. (Y - Yes, N - N - No). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ShowTreeviewAsListview">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to show treeviews as listviews. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PrtOnlineDelNoteBoRelease">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to print online delivery notes when releasing the quantity from back order. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ProjectsAndContracts">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ChkPostThresholdAgainst"/>
        <xsd:element ref="AutoExpandHierTreeview"/>
        <xsd:element ref="ShowTreeviewAsListview"/>
        <xsd:element ref="PrtOnlineDelNoteBoRelease"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DemandRevLink" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation><![CDATA[ Indicates whether to call the Inventory Demand Review program when doing receipts (N - No, Y - Yes).  If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
    </xsd:annotation>
  </xsd:element>
</xsd:schema>
