<?xml version="1.0" encoding="Windows-1252"?>
<!-- Copyright 1994-2016 SYSPRO Ltd.-->
<!-- 
     Schema for the Work in Progress Setup Transaction Posting Business Object
-->
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="WorkInProgressOptions">
    <xsd:annotation>
      <xsd:documentation>This is the root element.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="false">
      <xsd:sequence>
        <xsd:element ref="General1" minOccurs="0"/>
        <xsd:element ref="General2" minOccurs="0"/>
        <xsd:element ref="History" minOccurs="0"/>
        <xsd:element ref="JobCreation" minOccurs="0"/>
        <xsd:element ref="LabourPosting" minOccurs="0"/>
        <xsd:element ref="PeriodCurrentYear" minOccurs="0"/>
        <xsd:element ref="PeriodPreviousYear" minOccurs="0"/>
        <xsd:element ref="CurrentMonth" minOccurs="0"/>
        <xsd:element ref="Numbering" minOccurs="0"/>
        <xsd:element ref="LotsAndSerials" minOccurs="0"/>
        <xsd:element ref="ProjectsAndContracts" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="WipLoadLevelReq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether Work in Progress load levelling is required. (Y - Yes, N - No). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowChgToOperationTimes">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether Work in Progress load levelling is allowed to change operation times. (Y - Yes, N - No). If no value is supplied then the default value of Y will be assumed. Setting this option to N will prevent changes from being made to expected times required for operations on jobs, whereas setting this option to Y will allow changes to be made.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowOpToEndEarlier">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow the operation to end earlier than prior operation. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AutoClearVariance">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the automatically cleared variance is a percentage or value. (V - Value, P - Percentage). If no value is supplied then the default value of V will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="V=Value;P=Percentage;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="V"/>
        <xsd:enumeration value="P"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AutoClearWipVarLessThan">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Automatically clear any WIP variance smaller than this percentage variance when marking a job as complete in IMP010 if costing is set to standard.This amount only applies if 'AutoClearVariance' is set to P.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0.00"/>
        <xsd:maxInclusive value="999.99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AutoClearWipVarLessThanValue">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Automatically clear any WIP variance smaller than this variance amount when marking a job as complete in IMP010 if costing is set to standard. This amount only applies if 'AutoClearVariance' is set to P.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0.00"/>
        <xsd:maxInclusive value="999999999999.99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ApplyAutoVarAllCostMethods">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to apply automatic variance for all cost methods. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="FloorStockWh">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Contains the floor stock warehouse to use. This element is optional.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AdvancedSchedulingReq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether advanced scheduling is required for APS. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LinkSoDirectlyToAps">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to link the Sales Orders directly to APS. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LinkWipDirectlyToAps">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to link Work in Progress directly to APS. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="WipInspReq">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether Work in Progress inspection is required. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CaptureSerialsEndWipInsp">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether serials are to be captured at the end of Work in Progress inspection. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CaptureLotsEndWipInsp">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether lots are to be captured at the end of Work in Progress inspection. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="General1">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="WipLoadLevelReq"/>
        <xsd:element ref="AllowChgToOperationTimes"/>
        <xsd:element ref="AllowOpToEndEarlier"/>
        <xsd:element ref="AutoClearVariance"/>
        <xsd:element ref="AutoClearWipVarLessThan"/>
        <xsd:element ref="AutoClearWipVarLessThanValue"/>
        <xsd:element ref="ApplyAutoVarAllCostMethods"/>
        <xsd:element ref="FloorStockWh"/>
        <xsd:element ref="AdvancedSchedulingReq"/>
        <xsd:element ref="LinkSoDirectlyToAps"/>
        <xsd:element ref="LinkWipDirectlyToAps"/>
        <xsd:element ref="WipInspReq"/>
        <xsd:element ref="CaptureSerialsEndWipInsp"/>
        <xsd:element ref="CaptureLotsEndWipInsp"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CalcGrossProfitPartBill">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether gross profits are to be calculated and checked in part billings. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LowProfitPercForGrossProfit">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the lowest acceptable profit percentage allowed when checking the gross profit. This element is optional.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0.00"/>
        <xsd:maxInclusive value="999.99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="HighProfitPercForGrossProfit">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the highest acceptable profit percentage allowed when checking the gross profit. This element is optional.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0.00"/>
        <xsd:maxInclusive value="999.99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LabServChg">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the labor service charge is to be billed. (N - Not required, R - Required, P - Prompt operator). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=Not required;R=Required,P=Prompt operator;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="R"/>
        <xsd:enumeration value="P"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DefLabServChg">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the default labor charge code to be used. This field is optional and is only enterable if 'LabServChg' is not set to N. If supplied, it must be a valid Sales Order Service Charge record.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PlaceMultBinsOnHold">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to place multiple bins on hold when receipting a job. (N - No, Y - Automatic, P - Prompt operator). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Automatic,P=Prompt operator;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="P"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PrintKitIssDocOnline">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to print the kit-issue document in-line. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowKitsWithShortages">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow kits with shortages to be issued. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AllowSubsInKitIssues">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow substitution in kit issues. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RoundMatIssToPanSize">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to round material allocations up to pan size in kit issues. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="WipInspDocSel">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the Work in Progress inspection document selection. (N - No document, B - Batch only, O - On line and batch). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No document;B=Batch only;O=On line and batch;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="B"/>
        <xsd:enumeration value="O"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PromptForOperation">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to prompt for operation number when issuing to a previously non-existant material allocation. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="UseManufUom">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to allow units of measure other than stocking units of measure in manufacturing. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="RetainSoLink">
    <xsd:annotation>
      <xsd:documentation><![CDATA[ Indicates whether to retain the link between the sales order and job for completed jobs (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.
  ]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="General2">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CalcGrossProfitPartBill"/>
        <xsd:element ref="LowProfitPercForGrossProfit"/>
        <xsd:element ref="HighProfitPercForGrossProfit"/>
        <xsd:element ref="LabServChg"/>
        <xsd:element ref="DefLabServChg"/>
        <xsd:element ref="PlaceMultBinsOnHold"/>
        <xsd:element ref="PrintKitIssDocOnline"/>
        <xsd:element ref="AllowKitsWithShortages"/>
        <xsd:element ref="AllowSubsInKitIssues"/>
        <xsd:element ref="RoundMatIssToPanSize"/>
        <xsd:element ref="WipInspDocSel"/>
        <xsd:element ref="PromptForOperation"/>
        <xsd:element ref="UseManufUom"/>
        <xsd:element ref="RetainSoLink"/>
        <xsd:element ref="DemandRevLink"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PermRetScrapAnal">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to permanently retain scrap analysis. (Y - Yes, N - No). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PermRetNonProdAnal">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to permanently retain non-productive analysis. (Y - Yes, N - No). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Y=Yes;N=No;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MthsRetLaborJnls">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of months to retain labor posting journals. Entered values must be between 1 and 60 inclusively or 99 only. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MthsRetPartBillJnls">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of months to retain part billing journals. Entered values must be between 1 and 60 inclusively or 99 only. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="GlPerRetLaborDistr">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of GL periods to retain the labor distribution entries. Entered values must be less than 61 or 99 only. (99 implies retaining them indefinitely). This field is optional.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="GlPerRetPartBillDistr">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of GL periods to retain the part billing distribution entries. Entered values must be less than 61 or 99 only. (99 implies retaining them  indefinitely). This field is optional.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="UseDaysAsRetMethod">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to use days as a retention method for completed jobs. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DaysRetJobs">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the number of days to retain jobs before deletion (purge). This field is optional and only applies if 'UseDaysAsRetMethod' is set to Y.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0"/>
        <xsd:maxInclusive value="9999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ArchiveCompletedJobs">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to archive completed jobs before purging. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ArchiveJobTrans">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to archive posting records when archiving jobs. (N - No, Y - Yes). If no value is supplied then the default value of N will be assumed. May only be set to Y if 'ArchiveCompletedJobs' is set to Y, else N will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="N=No;Y=Yes;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="N"/>
        <xsd:enumeration value="Y"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MaxJobsPerArchive">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the maximum number of jobs per archive file. This field only applies if 'ArchiveCompletedJobs' is set to Y, else all 9's will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="0"/>
        <xsd:maxInclusive value="9999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="History">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="PermRetScrapAnal"/>
        <xsd:element ref="PermRetNonProdAnal"/>
        <xsd:element ref="MthsRetLaborJnls"/>
        <xsd:element ref="MthsRetPartBillJnls"/>
        <xsd:element ref="GlPerRetLaborDistr"/>
        <xsd:element ref="GlPerRetPartBillDistr"/>
        <xsd:element ref="UseDaysAsRetMethod"/>
        <xsd:element ref="DaysRetJobs"/>
        <xsd:element ref="ArchiveCompletedJobs"/>
        <xsd:element ref="ArchiveJobTrans"/>
        <xsd:element ref="MaxJobsPerArchive"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TrfFromBom">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether the transfer from Bill of Materials must be at BOM or inventory cost. (S - BOM cost, I - Inventory cost). If no value is supplied then the default value of S will be assumed.]]></xsd:documentation>
