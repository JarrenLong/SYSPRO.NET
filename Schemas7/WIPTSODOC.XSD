  <xsd:element name="PeriodPreviousYear">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="WipPrevDatePeriod1"/>
        <xsd:element ref="WipPrevFlagPeriod1"/>
        <xsd:element ref="WipPrevDatePeriod2"/>
        <xsd:element ref="WipPrevFlagPeriod2"/>
        <xsd:element ref="WipPrevDatePeriod3"/>
        <xsd:element ref="WipPrevFlagPeriod3"/>
        <xsd:element ref="WipPrevDatePeriod4"/>
        <xsd:element ref="WipPrevFlagPeriod4"/>
        <xsd:element ref="WipPrevDatePeriod5"/>
        <xsd:element ref="WipPrevFlagPeriod5"/>
        <xsd:element ref="WipPrevDatePeriod6"/>
        <xsd:element ref="WipPrevFlagPeriod6"/>
        <xsd:element ref="WipPrevDatePeriod7"/>
        <xsd:element ref="WipPrevFlagPeriod7"/>
        <xsd:element ref="WipPrevDatePeriod8"/>
        <xsd:element ref="WipPrevFlagPeriod8"/>
        <xsd:element ref="WipPrevDatePeriod9"/>
        <xsd:element ref="WipPrevFlagPeriod9"/>
        <xsd:element ref="WipPrevDatePeriod10"/>
        <xsd:element ref="WipPrevFlagPeriod10"/>
        <xsd:element ref="WipPrevDatePeriod11"/>
        <xsd:element ref="WipPrevFlagPeriod11"/>
        <xsd:element ref="WipPrevDatePeriod12"/>
        <xsd:element ref="WipPrevFlagPeriod12"/>
        <xsd:element ref="WipPrevDatePeriod13"/>
        <xsd:element ref="WipPrevFlagPeriod13"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="WipCurrentYear">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the calendar year in which your current financial year ends. This element may only be altered if there are no records already in the Work in Progress Job Detail file.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="9999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="WipCurrentMonth">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the calendar month in which your company is running in relation to the start of your financial year. This element may only be altered if there are no records already in the Work in Progress Job Detail file.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="99"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CurrentMonth">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="WipCurrentYear"/>
        <xsd:element ref="WipCurrentMonth"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="JobNumMethod">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the job numbering method to be used. (M - Manual, A - Automatic, C - By job classification, S - Scripted). If no value is supplied then the default value of M will be assumed. This element may not be set to A if the job key type is alphanumeric. This element may only be set to C if 'JobClassReq' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="M=Manual;A=Automatic;C=By job classification;S=Scripted;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="C"/>
        <xsd:enumeration value="S"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="WipInspNumMethod">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates the Work in Progress inspection numbering method to be used. (M - Manual, A - Automatic, C - By Job Classification). If no value is supplied then the default value of M will be assumed. This element may not be set to A if the WIP inspection reference key type is alphanumeric. This element may not be set to C unless 'JobClassReq' is set to Y.]]></xsd:documentation>
      <xsd:appinfo source="M=Manual;A=Automatic;C=By Job Classification"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="M"/>
        <xsd:enumeration value="A"/>
        <xsd:enumeration value="C"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextJobNum">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next job number to use. This element is only to be supplied if the 'WipInspNumMethod' is set to A. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextKitIssueNum">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next kit issue document number to use. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextWipInspNumber">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next Work in Progress inspection number to use. This element is only to be supplied if the 'WipInspNumMethod' is set to A. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextWipInspDocNum">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next inspection document number to use. This element is only to be supplied if the 'WipInspReq' is set to Y and 'PromptForOperation' is not set to N. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NextScrapDocNum">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the next scrap document number to use. This element is only to be supplied if the 'WipInspReq' is set to Y and 'PromptForOperation' is not set to N. If no value is supplied then the default value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="999999999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="OpNumberIncrement">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the Work in Progress operation number increment. If no value is supplied or the value supplied exceeds 100 then a value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="9999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="FirstOpNumber">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is first Work in Progress operation number. If no value is supplied or the value supplied exceeds 101 then a value of 1 will be assumed.]]></xsd:documentation>
      <xsd:appinfo/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:double">
        <xsd:minInclusive value="1"/>
        <xsd:maxInclusive value="9999"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="OpRenumber">
    <xsd:annotation>
      <xsd:documentation><![CDATA[This is the Work in Progress operation renumbering. (Y - No renumbering of started operations, S - No renumbering after job started, C - No renumbering after job confirmed, N - No renumbering after job added). If no value is supplied then the default value of Y will be assumed.]]></xsd:documentation>
      <xsd:appinfo source="Y=No renumbering of started operations;S=No renumbering after job started;C=No renumbering after job confirmed;N=No renumbering after job added;"/>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Y"/>
        <xsd:enumeration value="S"/>
        <xsd:enumeration value="C"/>
        <xsd:enumeration value="N"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="Numbering">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="JobNumMethod"/>
        <xsd:element ref="WipInspNumMethod"/>
        <xsd:element ref="NextJobNum"/>
        <xsd:element ref="NextKitIssueNum"/>
        <xsd:element ref="NextWipInspNumber"/>
        <xsd:element ref="NextWipInspDocNum"/>
        <xsd:element ref="NextScrapDocNum"/>
        <xsd:element ref="OpNumberIncrement"/>
        <xsd:element ref="FirstOpNumber"/>
        <xsd:element ref="OpRenumber"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ReserveLotsForParent">
    <xsd:annotation>
      <xsd:documentation><![CDATA[Indicates whether to reserve lots for parent on job confirmation. (N - No, Y - Yes, P - Prompt operator). If no value is supplied then the default value of N will be assumed.]]></xsd:documentation>
