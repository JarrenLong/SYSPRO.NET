										<xsd:element name="AbcQuantity" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Edited_AbcQuantity" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AbcValue" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Edited_AbcValue" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ElementTotal" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="AbcQuantityTotal" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Edited_AbcQuantityTotal" type="xsd:string" minOccurs="0"/>
													<xsd:element name="AbcValueTotal" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Edited_AbcValueTotal" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="IncidenceCodeSummary" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="PreProductionSummary" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Edited_PreProductSummary" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ManufacturingSummary" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Edited_ManufacturingSummary" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SalesSummary" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SalesSummary" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ReportTotal" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Edited_ReportTotal" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
