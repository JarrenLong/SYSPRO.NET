										<xsd:element name="TransactionDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CalculationMethod" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Edited_CalculationMethod" type="xsd:string" minOccurs="0"/>
										<xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
										<xsd:element name="RouteNumber" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LedgerPeriod" type="xsd:string" minOccurs="0"/>
										<xsd:element name="GroupCode" type="xsd:string" minOccurs="0"/>
