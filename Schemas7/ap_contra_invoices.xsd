<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--     Schema for the AP Contra Invoices Business Object--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="CheckRegister">		<xsd:annotation>			<xsd:documentation>This element is used to indicate the check register selection. The default is A and the other valid options are R and S.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;A=All;R=Range;S=Single"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="optional" default="A">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="R"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="Filter">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="MonthSelection" minOccurs="0"/>				<xsd:element ref="YearSelection" minOccurs="0"/>				<xsd:element ref="CheckRegister" minOccurs="0"/>				<xsd:element ref="SupplierSelection" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="MonthSelection">		<xsd:annotation>			<xsd:documentation>This element is used to indicate the ledger period selection.The default is A and the other valid values are R and S.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;A=All;S=Single;R=Range"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="optional" default="A">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="R"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="Option">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Reprint" minOccurs="0"/>				<xsd:element ref="XslStylesheet" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Query">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Option"/>				<xsd:element ref="Filter"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Reprint">		<xsd:annotation>			<xsd:documentation>This element is used to indicate the reprint register report option.The default is N and the other valid option is Y.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;N=No;Y=Yes"/>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="N"/>				<xsd:enumeration value="Y"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="SupplierSelection">		<xsd:annotation>			<xsd:documentation>This element is used to indicate the check register selection. The default is A and the other valid options are R and S.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;browse=ApSupplier;A=All;S=Single;R=Range"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="optional" default="A">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="R"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="XslStylesheet">		<xsd:annotation>			<xsd:documentation>This is the style sheet to be applied.</xsd:documentation>		</xsd:annotation>		<xsd:complexType/>	</xsd:element>	<xsd:element name="YearSelection">		<xsd:annotation>			<xsd:documentation>This element is used to indicate the ledger period selection.The default is A and the other valid value is S.</xsd:documentation>			<xsd:appinfo source="screenItem=Y;A=All;S=Single"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="optional" default="A">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string"/>		</xsd:complexType>	</xsd:element></xsd:schema>