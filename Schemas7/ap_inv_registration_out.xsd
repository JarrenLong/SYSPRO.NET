<!-- 
     Schema for the AP Invoice Registration Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="APInvoiceRegistration" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="APInvoiceRegistration" msdata:IsDataSet="false" msdata:UseCurrentLocale="true">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ReportSequence" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_ReportSequence" type="xsd:string" minOccurs="0"/>
							<xsd:element name="InvoiceStatus" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_InvoiceStatus" type="xsd:string" minOccurs="0"/>
							<xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_TransactionType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NewPagePerSupplier" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_NewPagePerSupplier" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PrintRelatedPurchaseOrders" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PrintRelatedPurchaseOrders" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PrintNotes" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_PrintNotes" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReturnNotesInBlock" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SupplierFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_SupplierFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SupplierFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_BranchFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BranchFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SupplierClassFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_SupplierClassFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SupplierClassFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BuyerFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_BuyerFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BuyerFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AuthorizingPersonFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_AuthorizingPersonFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AuthorizingPersonFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DepartmentFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_DepartmentFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DepartmentFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="InvoiceFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_InvoiceFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="InvoiceFilterValue" type="xsd:string" minOccurs="0"/>
							<xsd:element name="FollowUpDateFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_FollowUpDateFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="FollowUpDateFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ApInvoiceRegistrationItems">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ApInvoiceRegistrationItem" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="InvoiceRegistrationHeader" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="Buyer" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierName" type="xsd:string" minOccurs="0"/>
													<xsd:element name="APSMST_Description" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierClass" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierClassDescription" type="xsd:string" minOccurs="0"/>
													<xsd:element name="TBLAPC_Description" type="xsd:string" minOccurs="0"/>
													<xsd:element name="AuthorizingPerson" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierOnHold" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Translated_SupplierOnHold" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="InvoiceDetails" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="Invoice" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
													<xsd:element name="BranchDescription" type="xsd:string" minOccurs="0"/>
													<xsd:element name="APSBRN_Description" type="xsd:string" minOccurs="0"/>
													<xsd:element name="InvoiceAmount" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_InvoiceAmount" type="xsd:string" minOccurs="0"/>
													<xsd:element name="InvoiceDiscount" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_InvoiceDiscount" type="xsd:string" minOccurs="0"/>
													<xsd:element name="InvoiceDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="DueDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Department" type="xsd:string" minOccurs="0"/>
													<xsd:element name="DateSent" type="xsd:string" minOccurs="0"/>
													<xsd:element name="FollowUpDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Status" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ApproveDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ReasonCancelled" type="xsd:string" minOccurs="0"/>
													<xsd:element name="CancelledDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="PurgeDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ReasonReturned" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ReturnDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ReasonModified" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ModifyDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="InvoiceDetailsEntryDate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Operator" type="xsd:string" minOccurs="0"/>
													<xsd:element name="OperatorDescription" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ADMOPR_Description" type="xsd:string" minOccurs="0"/>
													<xsd:element name="RelatedPurchaseOrder" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Translated_ShortTransactionType" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Notes" minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Text" type="xsd:string" nillable="true"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
