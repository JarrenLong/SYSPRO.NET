													<xsd:element name="SupplierTotalsPrev1" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited-SupplierTotalsPrev1" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev2" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev2" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev3" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited-SupplierTotalsPrev3" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev4" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev4" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev5" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev5" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev6" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev6" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev7" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev7" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev8" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev8" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev9" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev9" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev10" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev10" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierTotalsPrev11" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierTotalsPrev11" type="xsd:string" minOccurs="0"/>
													<xsd:element name="SupplierPrevYearPurchases" type="xsd:double" minOccurs="0"/>
													<xsd:element name="Edited_SupplierPrevYearPurchases" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CompanyTotals">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="SupplierTotals" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="SupplierTotalsCurrent" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalsCurent" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev1" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalsPrev1" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev2" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalsPrev2" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev3" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalsPrev3" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev4" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalsPrev4" type="xsd:string" minOccurs="0"/>
