										<xsd:element name="Edited_SupplierTotalsPrev6" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev7" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalsPrev7" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev8" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalPrev8" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev9" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalPrev9" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev10" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalPrev10" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierTotalsPrev11" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierTotalsPrev11" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SupplierPrevYearPurchases" type="xsd:double" minOccurs="0"/>
										<xsd:element name="Edited_SupplierPrevYearPurchases" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TranslatedText">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Text_Supplier" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Text_Short_name" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Text_User_defined" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Text_Yes" type="xsd:string" minOccurs="0"/>
