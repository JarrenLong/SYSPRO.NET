<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="ARCollectorRunQuery" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="ARCollectorRunQuery">
    <xsd:complexType>
      <xsd:sequence minOccurs="0">
        <xsd:element name="SystemInformation" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportSequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportSequence" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeCaptured" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeCaptured" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeCancelled" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeCancelled" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludePaymentInProcess" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludePaymentInProcess" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeVerified" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeVerified" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeClosed" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludeClosed" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludePDChecks" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_IncludePDChecks" type="xsd:string" minOccurs="0"/>
              <xsd:element name="InclInvoiceAttachedToPD" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_InclInvoiceAttachedToPD" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CollectorRunNoFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CollectorRunNoFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CollectorRunNoFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BankFilterType" type="xsd:string" minOccurs="0"/>
