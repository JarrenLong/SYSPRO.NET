              <xsd:element name="BankFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GeographicAreaFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_GeographicAreaFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GeographicAreaFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SalespersonFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_SalespersonFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SalespersonFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CurrencyFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CurrencyFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CurrencyFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CollectorRuns">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CollectorRun" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="CollectorRunHeader" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="CollectorRunNumber" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PaymentCollector" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PaymentCollectorName" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="DateCollected" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ReceiptDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Status" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BankDescription" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CurrencyDescription" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BatchCashTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_BatchCashTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BatchCheckTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_BatchCheckTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BatchOtherTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_BatchOtherTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BatchPDCheckTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_BatchPDCheckTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BatchTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_BatchTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="GeographicArea" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="GeographicAreaDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Salesperson" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SalespersonName" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="CollectorRunDetail" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="LineNumber" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PaymentCollector" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PaymentCollectorName" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="DateCollected" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ReceiptDate" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CustomerName" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="InvoiceNumber" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="InvoiceAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_InvoiceAmount" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CashTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_CashTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CheckTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_CheckTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="OtherTotal" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_OtherTotal" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="GeographicArea" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="GeographicAreaDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Salesperson" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SalespersonName" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="DocumentType" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="PDChecks" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="PDCheck" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="CollectorRunNumber" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CustomerName" minOccurs="0"/>
                                <xsd:element name="AvailableDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Check" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Amount" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_Amount" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Narration" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Invoices" minOccurs="0">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="Invoice" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                          <xsd:sequence>
                                            <xsd:element name="CollectorRunNumber" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="CustomerName" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Check" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="AvailableDate" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Invoice_Id" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="DocumentType" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Amount" type="xsd:double" minOccurs="0"/>
                                            <xsd:element name="Edited_Amount" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="Allocation" type="xsd:string" minOccurs="0"/>
                                          </xsd:sequence>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
