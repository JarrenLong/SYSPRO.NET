							<xsd:element name="CustName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CustAdd1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CustAdd2" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CustAdd3" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CustAdd4" type="xsd:string" minOccurs="0"/>
