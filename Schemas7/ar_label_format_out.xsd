							<xsd:element name="AreaName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="TaxExemptNo" type="xsd:string" minOccurs="0"/>
							<xsd:element name="PrcCatPrcCodeTbl" type="xsd:string" minOccurs="0"/>
