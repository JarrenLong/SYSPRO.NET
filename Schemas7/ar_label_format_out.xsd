							<xsd:element name="ShipAdd1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ShipAdd2" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ShipAdd3" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ShipAdd4" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ShipAdd5" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ShipPCod" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Contact" type="xsd:string" minOccurs="0"/>
