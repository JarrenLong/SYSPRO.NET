											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="PositionOfMessage" type="xsd:string" minOccurs="0"/>
													<xsd:element name="ContentsOfMessage" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
