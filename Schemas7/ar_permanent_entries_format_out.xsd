<!-- 
     Schema for the AR Permanent Entries Format 
-->
<xsd:schema id="ArPermanentEntriesFormatReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="ArPermanentEntriesFormatReport">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="PrintMethod" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="PrintFormat">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Preferences" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="FormatType" type="xsd:string" minOccurs="0"/>
										<xsd:element name="FormatTypeDesc" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_FormatTypeDesc" type="xsd:string" minOccurs="0"/>
										<xsd:element name="NoOfLinesOnPage" type="xsd:string" minOccurs="0"/>
										<xsd:element name="HighestStartColumn" type="xsd:string" minOccurs="0"/>
										<xsd:element name="PrintDateFormat" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_PrintDateFormat" minOccurs="0"/>
										<xsd:element name="LengthOfNotesLine" type="xsd:string" minOccurs="0"/>
										<xsd:element name="MaxNoOfNotesLines" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="FormatDetails" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="InvMessagePosition" type="xsd:string" minOccurs="0"/>
										<xsd:element name="InvMessage" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SystemDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TimeDocPrinted" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CompanyAddressLine1" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CompanyAddressLine2" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CompanyAddressLine3" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CompanyTaxRegNo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TaxExemptionNo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CompanyRegNo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerName" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerAddressLine1" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerAddressLine2" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerAddressLine3" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerAddressLine4" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerAddressLine5" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerPostalZipCode" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CustomerTaxNo" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ShipToAddressLine1" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ShipToAddressLine2" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ShipToAddressLine3" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ShipToAddressLine4" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ShipToAddressLine5" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ShipToAddressPostalZipCode" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Contact" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
										<xsd:element name="CurrencyDesc" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Salesperson" type="xsd:string" minOccurs="0"/>
										<xsd:element name="InvoiceDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="InvoiceNumber" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Branch" type="xsd:string" minOccurs="0"/>
										<xsd:element name="InvoiceTerms" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
										<xsd:element name="EntryDesc" type="xsd:string" minOccurs="0"/>
										<xsd:element name="ProductClass" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SalesTaxCode" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AmountWhichTaxPayable" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SalesTaxPercentage" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SalesTaxAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="InvAmountIncludingTax" type="xsd:string" minOccurs="0"/>
										<xsd:element name="InvoiceAmount" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TotalCashDiscOffered" type="xsd:string" minOccurs="0"/>
										<xsd:element name="FirstNotesLine" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TemplateLocation" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_TemplateLocation" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TemplateName" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DateTemplateLastAnalysed" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TimeTemplateLastAnalysed" type="xsd:string" minOccurs="0"/>
										<xsd:element name="HeaderOption" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_HeaderOption" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DetailOption" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_DetailOption" type="xsd:string" minOccurs="0"/>
										<xsd:element name="MaxNoOfDetailLines" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TotalOption" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_TotalOption" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AttachDocument" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_AttachDocument" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AttachmentLocation" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_AttachmentLocation" type="xsd:string" minOccurs="0"/>
										<xsd:element name="AttachName" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SuppressLeadingspacesInNumeric" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Translated_SuppressLeadingspacesInNumeric" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SrsDocumentIndex" type="xsd:string" minOccurs="0"/>
										<xsd:element name="TextFields" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="TextField" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="PositionOfMessage" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ContentsOfMessage" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
