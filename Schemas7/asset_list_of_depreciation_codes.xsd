<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2010 SYSPRO Ltd.--><!--     Schema List for the Asset List Of Depreciation Codes--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:element name="CurrentFinancialMonth" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element is used to indicate the current financial month</xsd:documentation>			<xsd:appinfo source="screenItem=Y"/>		</xsd:annotation>	</xsd:element>	<xsd:element name="CurrentFinancialYear" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This element is used to indicate the current financial year</xsd:documentation>			<xsd:appinfo source="screenItem=Y"/>		</xsd:annotation>	</xsd:element>	<xsd:element name="DepreciationCodeSelection">		<xsd:annotation>			<xsd:documentation>This sequence is used to indicate the depreciation code selection.The default is A and the other valid values are R and S</xsd:documentation>			<xsd:appinfo source="screenItem=Y;browse=AssetDepreciation;A=All;R=Range;S=Single"/>		</xsd:annotation>		<xsd:complexType>			<xsd:attribute name="FilterType" use="optional" default="A">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="A"/>						<xsd:enumeration value="R"/>						<xsd:enumeration value="S"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:attribute>			<xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="Filter">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="DepreciationCodeSelection" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Option">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="CurrentFinancialMonth" minOccurs="0"/>				<xsd:element ref="CurrentFinancialYear" minOccurs="0"/>				<xsd:element ref="XslStylesheet" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Query">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Option"/>				<xsd:element ref="Filter"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="XslStylesheet" type="xsd:string">		<xsd:annotation>			<xsd:documentation>This is the xml style sheet to be applied</xsd:documentation>		</xsd:annotation>	</xsd:element></xsd:schema>