<!-- 
     Schema for the Assets List of Revaluations Business Object
-->
<xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="Query">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="FromPeriod" type="xsd:string"/>
							<xsd:element name="ToPeriod" type="xsd:string"/>
							<xsd:element name="AssetFilterType" type="xsd:string"/>
							<xsd:element name="Translated_AssetFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="AssetFilterValue" type="xsd:string"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ListofTransfers">
					<xsd:complexType>
						<xsd:sequence minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="Assets">
								<xsd:complexType>
									<xsd:sequence minOccurs="0" maxOccurs="unbounded">
										<xsd:element name="Asset">
											<xsd:complexType>
												<xsd:sequence minOccurs="0" maxOccurs="unbounded">
													<xsd:element name="AssetDetail">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="AssetCode" type="xsd:string"/>
																<xsd:element name="AssetDescription" type="xsd:string"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="Transfers">
														<xsd:complexType>
															<xsd:sequence minOccurs="0" maxOccurs="unbounded">
																<xsd:element name="Transfer">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="EntryDate" type="xsd:string"/>
																			<xsd:element name="FromCompany" type="xsd:string"/>
																			<xsd:element name="ToCompany" type="xsd:string"/>
																			<xsd:element name="FromBranch" type="xsd:string"/>
																			<xsd:element name="FromBranchName" type="xsd:string"/>
																			<xsd:element name="ToBranch" type="xsd:string"/>
																			<xsd:element name="ToBranchName" type="xsd:string"/>
																			<xsd:element name="FromGroup" type="xsd:string"/>
																			<xsd:element name="FromGroupDescription" type="xsd:string"/>
																			<xsd:element name="ToGroupAfter" type="xsd:string"/>
																			<xsd:element name="ToGroupDescription" type="xsd:string"/>
																			<xsd:element name="FromCostCentre" type="xsd:string"/>
																			<xsd:element name="FromCostCentreDescription" type="xsd:string"/>
																			<xsd:element name="ToCostCentreAfter" type="xsd:string"/>
																			<xsd:element name="ToCostCentreDescription" type="xsd:string"/>
																			<xsd:element name="Narration" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="DepreciationValue" type="xsd:double"/>
																			<xsd:element name="TransferValue" type="xsd:double"/>
																			<xsd:element name="Edited_DepreciationValue" type="xsd:string" minOccurs="0"/>
																			<xsd:element name="Edited_TransferValue" type="xsd:string" minOccurs="0"/>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="TotalTransfer">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="DepreciationValue" type="xsd:double"/>
																<xsd:element name="TransferValue" type="xsd:double"/>
																<xsd:element name="Edited_DepreciationValue" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Edited_TransferValue" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="CompanyAssetTotal">
								<xsd:complexType>
									<xsd:sequence minOccurs="0" maxOccurs="unbounded">
										<xsd:element name="CompanyTotalTransfer">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="DepreciationValue" type="xsd:double"/>
													<xsd:element name="TransferValue" type="xsd:double"/>
													<xsd:element name="Edited_DepreciationValue" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Edited_TransferValue" type="xsd:string" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
