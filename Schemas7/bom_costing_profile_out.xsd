                    <xsd:element name="Edited_Value2" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="Type" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Type" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Level" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Component" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Operation" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WorkCenter" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CoProduct" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PartCategory" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_PartCategory" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Route" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UnitOfMeasure" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_TotalQty" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TotalTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_TotalTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Source" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Source" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Value" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_Value" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SourceCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CompSource" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_CompSource" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CompValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_CompValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CompSourceCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CumulativeValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_CumulativeValue" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BaseCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_BaseCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ComparativeCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_ComparativeCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="QuantityPer" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_QuantityPer" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MaterialPerc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_MaterialPerc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MaterialCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_MaterialCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MaterialCost2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_MaterialCost2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LaborPerc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_LaborPerc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LaborCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_LaborCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LaborCost2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_LaborCost2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SubcontractPerc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_SubcontractPerc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SubcontractCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_SubcontractCost" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SubcontractCost2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Edited_SubcontractCost2" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
