<!--
     Schema for the BOM Multi-Level Trial Kitting Report
-->
<xsd:schema id="BomMultiTrialKit" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="BomMultiTrialKit" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ReportType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ShortageLinesOnly" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_ShortageLinesOnly" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AltStockOnShortage" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_AltStockOnShortage" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NewPagePerSupplier" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_NewPagePerSupplier" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="BomReportTypes">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="HeaderInfo" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="StockToMake" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="StockToMakeDesc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Route" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="QtyToMake" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_QtyToMake" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DueDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CustomerLeadTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ManufacturingLeadTime" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="MultiLevelList" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ComponentItem" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Level" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Component" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ComponentDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="QtyAvailable" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_QtyAvailable" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="GrossQty" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_GrossQty" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="NetQty" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_NetQty" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ShortageQty" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_ShortageQty" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="UOM" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PartCategory" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_PartCategory" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CLT" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="AdditionalInfo" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="AltStockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AltWarehouse" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AltQtyAvailable" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_AltQtyAvailable" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AltUom" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="MadeInList" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ComponentItem" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Component" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ComponentDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ExplodedQty" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_ExplodedQty" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="GrossQtyReqd" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_GrossQtyReqd" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="NetQtyReqd" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_NetQtyReqd" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="UOM" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="PartCategory" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_PartCategory" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="MLT" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CLT" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="BuyingRule" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_BuyingRule" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EBQ" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Edited_EBQ" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="UnitCost" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LeadTime" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="AdditionalInfo" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="AltStockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AltWarehouse" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AltQtyAvailable" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_AltQtyAvailable" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AltUom" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="BoughtOutList" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="SupplierDetail" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierName" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="SupplierOnHold" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Translated_SupplierOnHold" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="ComponentItem" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Component" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ComponentDesc" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ExplodedQty" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_ExplodedQty" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="GrossQtyReqd" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_GrossQtyReqd" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="NetQtyReqd" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_NetQtyReqd" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="UOM" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PartCategory" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_PartCategory" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="MLT" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CLT" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="UnitCost" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="LeadTime" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="BuyingRule" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_BuyingRule" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EBQ" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Edited_EBQ" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AdditionalInfo" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="AltStockCode" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="AltWarehouse" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="AltQtyAvailable" type="xsd:double" minOccurs="0"/>
                                      <xsd:element name="Edited_AltQtyAvailable" type="xsd:string" minOccurs="0"/>
                                      <xsd:element name="AltUom" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
