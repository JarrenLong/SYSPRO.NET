<!-- 
     Schema for the Cash Book Cash Projection Business Object
-->
<xsd:schema id="CbCashProjectionReport" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="CbCashProjectionReport" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ProjectionDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BankFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_BankFilterType" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BankFilterValue" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="BankItem" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Bank" type="xsd:string" minOccurs="0"/>
							<xsd:element name="APSBNK_Description" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="TBLCUR_Description" type="xsd:string" minOccurs="0"/>
							<xsd:element name="StatementBalance" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_StatementBalance" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OutstandingDeposits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_OutstandingDeposits" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OutstandingWithdrawals" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_OutstandingWithdrawals" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CurrentBalance" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_CurrentBalance" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalEquivalent" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_LocalEquivalent" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ForeignBalanceBelowZero" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalBalanceBelowZero" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OverFlowError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_OverFlowError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="BalanceError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_BalanceError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ConversionError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_ConversionError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CurrencyNotFoundError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_CurrencyNotFoundError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="WrongCurrencyError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_WrongCurrencyError" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CompanyTotal">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="StatementBalance" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_StatementBalance" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OutstandingDeposits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_OutstandingDeposits" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OutstandingWithdrawals" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_OutstandingWithdrawals" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CurrentBalance" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_CurrentBalance" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalEquivalent" type="xsd:double" minOccurs="0"/>
							<xsd:element name="Edited_LocalEquivalent" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OverFlowError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Translated_OverFlowError" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyTotalBalanceBelowZero" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
