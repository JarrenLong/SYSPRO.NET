<!-- 
     Schema for the Delivery Route Load List Business Object
-->
<xsd:schema id="RouteCodeDetail" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xsd:element name="RouteCodeDetail" msdata:IsDataSet="true" msdata:Locale="en-US">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="SystemInformation">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Role" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
							<xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
							<xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
							<xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
							<xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>							
							<xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
							<xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="QueryOptions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="RouteCode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="RouteDescription" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LimitingFactor" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Transport" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DeliveryDate" type="xsd:string" minOccurs="0"/>
							<xsd:element name="DeliverySeq" type="xsd:string" minOccurs="0"/>
							<xsd:element name="IncludeSalesorderLines" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Routes">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Route" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="DeliveryDate" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DeliverySeq" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Volume" type="xsd:string" minOccurs="0"/>
										<xsd:element name="VolumeUsed" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Mass" type="xsd:string" minOccurs="0"/>
										<xsd:element name="MassUsed" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LoadCompleteFlag" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Status" type="xsd:string" minOccurs="0"/>
										<xsd:element name="LoadNumber" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Driver" type="xsd:string" minOccurs="0"/>
										<xsd:element name="DriverName" type="xsd:string" minOccurs="0"/>
										<xsd:element name="SalesOrders" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="SalesOrderItem" minOccurs="0" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CustomerName" type="xsd:string" minOccurs="0"/>
																<xsd:element name="CustomerPoNumber" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ShipAddress1" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ShipAddress2" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ShipAddress3" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ShipAddress4" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ShipAddress5" type="xsd:string" minOccurs="0"/>
																<xsd:element name="ShipPostalCode" type="xsd:string" minOccurs="0"/>
																<xsd:element name="SalesOrder" type="xsd:string" minOccurs="0"/>
																<xsd:element name="SalesOrderLine" type="xsd:string" minOccurs="0"/>
																<xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
																<xsd:element name="StockCodeDesc" type="xsd:string" minOccurs="0"/>
																<xsd:element name="Quantity" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LineVolume" type="xsd:string" minOccurs="0"/>
																<xsd:element name="LineMass" type="xsd:string" minOccurs="0"/>
																<xsd:element name="RouteDistance" type="xsd:string" minOccurs="0"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
