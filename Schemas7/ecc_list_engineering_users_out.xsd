<!-- 
     Schema for the ECC Engineering Users Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="EngineeringUsers" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="EngineeringUsers" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="PrintAuthorities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintAuthorities" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UserFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UserFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_UserFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="EccUsers">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="EccUser" type="xsd:string" minOccurs="0"/>
              <xsd:element name="EccUserName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Administrator" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Administrator" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Operator" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Operator" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintAuthority" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="StatusCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ECCSTA_Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AuthorityType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AuthorityCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Authority" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_Authority" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
