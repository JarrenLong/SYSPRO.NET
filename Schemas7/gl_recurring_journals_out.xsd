<!--
     Schema for the G/L Recurring Journal Entry Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="GLRecurringJournals" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="GLRecurringJournals" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ValidateJournalsForPosting" type="xsd:string" minOccurs="0"/>
              <xsd:element name="AuthorizedJournals" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RecurringJournalCodeSelectionFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RecurringJournalCodeSelectionFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="GLJournalItem">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="GLJournalHeader" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="AuditReportType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RecurringJournal" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PostingFrequency" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NextPostingPeriod" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NextPostingYear" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ExpiryPeriod" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ExpiryYear" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SourceCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Notation" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="GLJournalDetails" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="GLJournalDetail" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="EntryNumber" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LedgerCode" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LedgerCodeDescription" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="EntryType" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LineType" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LineTypeDesc" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CreditAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="DebitAmount" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="Comment" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Validation" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="GLJournalTotal" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="LastJournalCreatedMsg" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="LastJournalCreated" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="DebitAmount" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="CreditAmount" type="xsd:double" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
