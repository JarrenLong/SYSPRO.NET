                    <xsd:element name="InvWhControl_GtrCtlAccl" type="xsd:string"/>
                    <xsd:element name="InvWhControl_GtrPrefix" type="xsd:string"/>
                    <xsd:element name="InvWhControl_GtrNextNo" type="xsd:string"/>
                    <xsd:element name="InvWhControl_Branch" type="xsd:string"/>
                    <xsd:element name="InvWhControl_Area" type="xsd:string"/>
                    <xsd:element name="InvWhControl_FaxContact" type="xsd:string"/>
                    <xsd:element name="InvWhControl_Fax" type="xsd:string"/>
                    <xsd:element name="InvWhControl_FaxDocIwsFlag" type="xsd:string"/>
                    <xsd:element name="InvWhControl_GitAdjAcc" type="xsd:string"/>
                    <xsd:element name="InvWhControl_WhForComp" type="xsd:string"/>
                    <xsd:element name="InvWhControl_Route" type="xsd:string"/>
                    <xsd:element name="InvWhControl_InclPlanning" type="xsd:string"/>
