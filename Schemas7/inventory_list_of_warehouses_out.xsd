                    <xsd:element name="InvWhControl_PoNextNumber" type="xsd:string"/>
                    <xsd:element name="InvWhControl_GrnPrefix" type="xsd:string"/>
                    <xsd:element name="InvWhControl_GrnNextNumber" type="xsd:string"/>
                    <xsd:element name="InvWhControl_WipInspectGlCode" type="xsd:string"/>
                    <xsd:element name="InvWhControl_WipControlGlCode" type="xsd:string"/>
                    <xsd:element name="InvWhControl_WipVarCtlGlCode" type="xsd:string"/>
                    <xsd:element name="InvWhControl_WipAutoVarGlCode" type="xsd:string"/>
