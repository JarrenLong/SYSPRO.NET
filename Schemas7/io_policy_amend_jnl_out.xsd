<!-- 
     Schema for the IO Policy Amendment Journal Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="PolicyAmendJnlReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="PolicyAmendJnlReport" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Reprint" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="PrintPolicyAdditions" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="PrintPolicyChanges" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="PrintPolicyDeletions" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DeleteJournalFile" type="xsd:string" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="AmendmentDetails">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="AmendmentDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="JournalDate" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="JournalTime" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Operator" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="ChangeFlag" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="StockCode" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="SkuPolicyId" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PolicyEffectiveDate" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PolicyReference" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="SourceOfPolicyCreation" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="SelectionSet" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="TargetServiceLevelPercentage" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="ServiceLevelMeasure" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="MaxQuantitySafetyStock" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="ReviewFrequencyInDays" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="CodeForSupplyType" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="TypeOfPolicy" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="LevelType" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="FixedSafetyStockDays" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="FixedSafetyStockQuantity" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="AlgorithmToCalculateSafetyStock" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="MaxDaysCoverForSafetyStock" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="LeadTimeUsedToCalcForecastAccuracy" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PolicyChangeDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="PolicyChanged" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="PolicyChangeName" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="PolicyChangedOldValue" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="PolicyChangedNewValue" type="xsd:string" minOccurs="0"></xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="AmendmentTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="TotalAdditions" type="xsd:double" minOccurs="0"></xsd:element>
              <xsd:element name="TotalUpdates" type="xsd:double" minOccurs="0"></xsd:element>
              <xsd:element name="TotalDeletions" type="xsd:double" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
