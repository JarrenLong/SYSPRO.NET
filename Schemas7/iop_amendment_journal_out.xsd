                    <xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Release" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Warehouse" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ChildCollection" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ChildInstance" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="IOPCOL_ChildDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Calendar" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UnitOfMeasure" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="QuantityDecimals" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WarehouseConsolidation" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_WarehouseConsolidation" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ForecastAlgorithm" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_ForecastAlgorithm" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="OverrideDefaultAlgorithmSettings" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_OverrideDefAlgSettings" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NumberOfMonthsForCompetition" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MeasureToUseForCompetition" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_MeasureToUseForCompetition" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NumberOfMonthsForMean" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NumberOfMonthsForMedian" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NumberOfMonthsForAverage" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SmoothingCoefficient" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TrendCoefficient" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SixPeriodWeightingMinusOneandTwo" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SixPeriodWeightingMinusThreeandFour" type="xsd:string" minOccurs="0"/>
