                    <xsd:element name="TwelvePeriodWeightingMinusOneTwoandThree" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TwelvePeriodWeightingMinusFourFiveandSix" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TwelvePeriodWeightingMinusSevenEightandNine" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TwelvePeriodWeightingMinusTenElevenandTwelve" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HoltWintersAdditiveOptimizeCoefficients" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_HoltWintersAdditiveOptCoefficients" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HoltWintersAdditiveLevelCoefficient" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HoltWintersAdditiveTrendCoefficient" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HoltWintersAdditiveSeasonalCoefficient" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HoltWintersMultiplicativeOptimizeCoefficients" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_HoltWintersMultiOptCoefficients" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HoltWintersMultiplicativeLevelCoefficient" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HoltWintersMultiplicativeTrendCoefficient" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="HoltWintersMultiplicativeSeasonalCoefficient" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CollectionChangeDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="CollectionChanged" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="CollectionChangeName" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_CollectionChangeName" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CollectionChangeOldValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_CollectionChangeOldValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="CollectionChangeNewValue" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_CollectionChangeNewValue" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
