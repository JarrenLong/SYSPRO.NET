              <xsd:element name="Translated_PrintSKUAdditions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintSKUDeletions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintSKUDeletions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DeleteJournalFile" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DeleteJournalFile" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="AmendmentDetails">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="AmendmentDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="JournalDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JournalTime" type="xsd:string" minOccurs="0"/>
