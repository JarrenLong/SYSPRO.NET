<!-- 
     Schema for the List of BOM Notional Parts Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="BOMNotionalParts" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="BOMNotionalParts" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"></xsd:element>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:double" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ParentPartFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_ParentPartFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="ParentPartFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="RouteFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="Translated_RouteFilterType" type="xsd:string" minOccurs="0"></xsd:element>
              <xsd:element name="RouteFilterValue" type="xsd:string" minOccurs="0"></xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="NotionalDetails">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="NotionalDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ParentPartCount" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="ParentPart" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="ParentPartDescription" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Version" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Release" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Route" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="CoProductLine" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="CoProduct" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="CoProductDescription" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="CoProductVersion" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="CoProductRelease" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="OperationOffset" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="OptionalFlag" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="QtyPer" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_QtyPer" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PctMaterialCost" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_PctMaterialCost" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PctLaborCost" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_PctLaborCost" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PctSubContractCost" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_PctSubContractCost" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PlanWeight" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_PlanWeight" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="PctQuantity" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_PctQuantity" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="MaterialLabourItems" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="MaterialLabourItem" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="ItemType" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Translated_ItemType" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="LCoProductLine" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="LOperation" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="LPctLaborCost" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_LPctLaborCost" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="MCoProductLine" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="MSequenceNum" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="MComponent" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="MComponentDescription" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="MPctMaterialCost" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_MPctMaterialCost" type="xsd:string" minOccurs="0"></xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
