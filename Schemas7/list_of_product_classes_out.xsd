                    <xsd:element name="SalProductClass_SalesBudget5" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesBudget6" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesBudget7" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesBudget8" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesBudget9" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesBudget10" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesBudget11" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesBudget12" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesBudget13" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual1" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual2" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual3" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual4" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual5" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual6" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual7" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual8" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual9" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual10" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual11" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual12" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_SalesActual13" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SalProductClass_LowProfitPct" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SalProductClass_HighProfitPct" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
