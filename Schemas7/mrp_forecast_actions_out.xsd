						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ForecastActions">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Forecast" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
										<xsd:element name="Revision" type="xsd:string" minOccurs="0"/>
