<!-- 
     Schema for the List of Supplier/Stock Code Contract Prices Business Object
-->
<xsd:schema id="SupplierStockCodeContractPrice" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="SupplierStockCodeContractPrice" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="IncludePriceDetails" type="xsd:string" minOccurs="0"/>
              <xsd:element name="IncludeExtraPriceInformation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NewPagePerSupplier" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CatalogueFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CatalogueFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SupplierFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ContractFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ContractFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReferenceFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReferenceFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="DetailsOfContracts">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ContractDetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ContractPriceHeader" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="CatalogueNumber" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LongDescription" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Decimals" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="DemonstratedLeadTime" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="LastPricePaid" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="PriceUOM" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="DateLastReceived" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="LastQtyReceived" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="PurchaseUOM" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="ContractLines" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="ContractLine" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Contract" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PurchasePrice" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="PriceUOM" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="DiscountCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="OrderMultiple" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="MinOrderQty" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="PriceValidFrom" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PriceValidTo" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PriceQuoteDate" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="QuotedLeadTime" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="ShippingInstructions" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="AnticipatedFreightCharge" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="DiscountMethod" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Discount1" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Discount2" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="Discount3" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="PriceComments" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="FreightComments" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="PriceReference" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SupplierCatalogueNum" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TranslatedText" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Text_Range" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Single" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Yes" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_No" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Plus" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_Less" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Text_All" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
