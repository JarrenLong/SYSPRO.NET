<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2014 SYSPRO Ltd.--><!--       Schema for the Sales Analysis Advanced Sales Tax SRS Report--><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="Date">    <xsd:annotation>      <xsd:documentation>Date selection for report. Enabled for detail report, and when 'Enable date selection for summary report' is selected. A=All, R=Range, S=Single.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;A=All;R=Range;S=Single;browse={date}"/>    </xsd:annotation>    <xsd:complexType>      <xsd:attribute name="FilterType" use="optional" default="A">        <xsd:simpleType>          <xsd:restriction base="xsd:string">            <xsd:enumeration value="A"/>            <xsd:enumeration value="R"/>            <xsd:enumeration value="S"/>          </xsd:restriction>        </xsd:simpleType>      </xsd:attribute>      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>    </xsd:complexType>  </xsd:element>  <xsd:element name="EnableDatesForSummary">    <xsd:annotation>      <xsd:documentation>Enable date selection for summary report. Y=Yes, N=No.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Filter">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Date"/>        <xsd:element ref="State"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="FromPeriod">    <xsd:annotation>      <xsd:documentation>Period to use when printing for a single period, or first period to print when printing a range of periods.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;01=01;02=02;03=03;04=04;05=05;06=06;07=07;08=08;09=09;10=10;11=11;12=12"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="01"/>        <xsd:enumeration value="02"/>        <xsd:enumeration value="03"/>        <xsd:enumeration value="04"/>        <xsd:enumeration value="05"/>        <xsd:enumeration value="06"/>        <xsd:enumeration value="07"/>        <xsd:enumeration value="08"/>        <xsd:enumeration value="09"/>        <xsd:enumeration value="10"/>        <xsd:enumeration value="11"/>        <xsd:enumeration value="12"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="FromYear">    <xsd:annotation>      <xsd:documentation>Year to use when printing for a single period, or first year to print when printing a range of periods.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;C=C;P1=P1;P2=P2;P3=P3;P4=P4"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="C"/>        <xsd:enumeration value="P1"/>        <xsd:enumeration value="P2"/>        <xsd:enumeration value="P3"/>        <xsd:enumeration value="P4"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Option">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="ReportType" minOccurs="0"/>        <xsd:element ref="EnableDatesForSummary" minOccurs="0"/>        <xsd:element ref="Reprint" minOccurs="0"/>        <xsd:element ref="PeriodSelection" minOccurs="0"/>        <xsd:element ref="FromPeriod" minOccurs="0"/>        <xsd:element ref="FromYear" minOccurs="0"/>        <xsd:element ref="ToPeriod" minOccurs="0"/>        <xsd:element ref="ToYear" minOccurs="0"/>        <xsd:element name="XslStylesheet" type="xsd:string" minOccurs="0">          <xsd:annotation>            <xsd:documentation>The stylesheet to be applied.</xsd:documentation>          </xsd:annotation>        </xsd:element>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="PeriodSelection">    <xsd:annotation>      <xsd:documentation>Period selection for the report. A range is allowed only for a reprint. R=Range, S=Single.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;S=Single;R=Range"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="S"/>        <xsd:enumeration value="R"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Query">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Option"/>        <xsd:element ref="Filter"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="ReportType">    <xsd:annotation>      <xsd:documentation>Report type to print. S=Summary, D=Detail.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;S=Summary;D=Detail"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="S"/>        <xsd:enumeration value="D"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Reprint">    <xsd:annotation>      <xsd:documentation>Reprint report. Y=Yes, N=No.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="Y"/>        <xsd:enumeration value="N"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="State">    <xsd:annotation>      <xsd:documentation>State selection for report. Selection enabled for reprint only. A=All, S=Single.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;A=All;S=Single"/>    </xsd:annotation>    <xsd:complexType>      <xsd:attribute name="FilterType" use="optional" default="A">        <xsd:simpleType>          <xsd:restriction base="xsd:string">            <xsd:enumeration value="A"/>            <xsd:enumeration value="S"/>          </xsd:restriction>        </xsd:simpleType>      </xsd:attribute>      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>    </xsd:complexType>  </xsd:element>  <xsd:element name="ToPeriod">    <xsd:annotation>      <xsd:documentation>Last period to print when printing a range of periods.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;01=01;02=02;03=03;04=04;05=05;06=06;07=07;08=08;09=09;10=10;11=11;12=12"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="01"/>        <xsd:enumeration value="02"/>        <xsd:enumeration value="03"/>        <xsd:enumeration value="04"/>        <xsd:enumeration value="05"/>        <xsd:enumeration value="06"/>        <xsd:enumeration value="07"/>        <xsd:enumeration value="08"/>        <xsd:enumeration value="09"/>        <xsd:enumeration value="10"/>        <xsd:enumeration value="11"/>        <xsd:enumeration value="12"/>        <xsd:enumeration value=""/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ToYear">    <xsd:annotation>      <xsd:documentation>Last year to print when printing a range of periods.</xsd:documentation>      <xsd:appinfo source="screenitem=Y;C=C;P1=P1;P2=P2;P3=P3;P4=P4"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="C"/>        <xsd:enumeration value="P1"/>        <xsd:enumeration value="P2"/>        <xsd:enumeration value="P3"/>        <xsd:enumeration value="P4"/>        <xsd:enumeration value=""/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element></xsd:schema>