<?xml version="1.0" encoding="Windows-1252"?><!-- Copyright 1994-2012 SYSPRO Ltd.--><!--       Schema for the Sales Analysis GST Report Business Object --><xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="Date">    <xsd:annotation>      <xsd:documentation>Selection of dates to be included in the report.'A' - All (Default) Select this to produce a report regardless of date.'R' - Range Select this to produce the report for a range of dates.You indicate this range in the FilterValue attribute.'S' - Single Select this to report on a single date.You indicate this date in the FilterValue attribute.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;browse={date};A=All;S=Single;R=Range"/>    </xsd:annotation>    <xsd:complexType>      <xsd:attribute name="FilterType" use="optional" default="A">        <xsd:simpleType>          <xsd:restriction base="xsd:string">            <xsd:enumeration value="A"/>            <xsd:enumeration value="S"/>            <xsd:enumeration value="R"/>          </xsd:restriction>        </xsd:simpleType>      </xsd:attribute>      <xsd:attribute name="FilterValue" type="xsd:string" use="optional"/>    </xsd:complexType>  </xsd:element>  <xsd:element name="Filter">    <xsd:annotation>      <xsd:documentation>List of filters available.</xsd:documentation>      <xsd:appinfo source="screenItem=N"/>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Date"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="MonthToPrint">    <xsd:annotation>      <xsd:documentation>This indicates the financial period (within the financial year entered), for which the report must be printed.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;01=01;02=02;03=03;04=04;05=05;06=06;07=07;08=08;09=09;10=10;11=11;12=12"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="01"/>        <xsd:enumeration value="02"/>        <xsd:enumeration value="03"/>        <xsd:enumeration value="04"/>        <xsd:enumeration value="05"/>        <xsd:enumeration value="06"/>        <xsd:enumeration value="07"/>        <xsd:enumeration value="08"/>        <xsd:enumeration value="09"/>        <xsd:enumeration value="10"/>        <xsd:enumeration value="11"/>        <xsd:enumeration value="12"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Option">    <xsd:annotation>      <xsd:documentation>List of options available.</xsd:documentation>      <xsd:appinfo source="screenItem=N"/>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="ReportType" minOccurs="0"/>        <xsd:element ref="MonthToPrint" minOccurs="0"/>        <xsd:element ref="YearToPrint" minOccurs="0"/>        <xsd:element ref="PrintRegisterSummary" minOccurs="0"/>        <xsd:element ref="Reprint" minOccurs="0"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="PrintRegisterSummary" default="N">    <xsd:annotation>      <xsd:documentation>This option is only available if you selected to print the report in Entry sequence, and this is not a reprint.Valid values are 'N' - No (default) and 'Y' - Yes.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="N"/>        <xsd:enumeration value="Y"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Query">    <xsd:annotation>      <xsd:documentation>This is the root element.</xsd:documentation>      <xsd:appinfo source="screenItem=N"/>    </xsd:annotation>    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Option"/>        <xsd:element ref="Filter"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="ReportSequence" default="E">    <xsd:annotation>      <xsd:documentation>Sequence required for the report.'E' - Entry sequence (default)Select this to produce the report in the sequence in which invoices were initially captured.'G' - Geographic area Select this to produce the report according to the geographic area in which the invoices were captured.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;E=Entry sequence;G=Geographic area"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="E"/>        <xsd:enumeration value="G"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="ReportType" default="S">    <xsd:annotation>      <xsd:documentation>Type of report required.'S' - Summary (default)Select this if you want to exclude detail invoice information from being printed on the report.Only an analysis of the tax is printed.'D' - Detail Select this if you want to print detail invoice information as well as an analysis of tax on the report.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;S=Summary;D=Detail"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="S"/>        <xsd:enumeration value="D"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="Reprint" default="N">    <xsd:annotation>      <xsd:documentation>Select this to print a report that was previously printed. Valid values are 'N' - No (default) and 'Y' - Yes.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;Y=Yes;N=No"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="N"/>        <xsd:enumeration value="Y"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element>  <xsd:element name="YearToPrint" default="C">    <xsd:annotation>      <xsd:documentation>This indicates the year for which the report must be printed.Valid values are 'C' - Current (default), 'P' - Previous or a 4-digit field indicating the current or previous AR financial year.</xsd:documentation>      <xsd:appinfo source="screenItem=Y;C=Current year;P=Previous year"/>    </xsd:annotation>    <xsd:simpleType>      <xsd:restriction base="xsd:string">        <xsd:whiteSpace value="preserve"/>        <xsd:enumeration value="C"/>        <xsd:enumeration value="P"/>      </xsd:restriction>    </xsd:simpleType>  </xsd:element></xsd:schema>