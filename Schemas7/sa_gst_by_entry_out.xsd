													<xsd:element name="GSTCode" type="xsd:string" minOccurs="0"/>
													<xsd:element name="GSTRate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="EditedGSTRate" type="xsd:string" minOccurs="0"/>
													<xsd:element name="GSTExempt" type="xsd:string" minOccurs="0"/>
													<xsd:element name="EditedGSTExempt" type="xsd:string" minOccurs="0"/>
													<xsd:element name="GSTTaxable" type="xsd:string" minOccurs="0"/>
													<xsd:element name="EditedGSTTaxable" type="xsd:string" minOccurs="0"/>
