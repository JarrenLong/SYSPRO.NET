												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="RegisterTotal" minOccurs="0" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="Source" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Translated_Source" type="xsd:string" minOccurs="0"/>
													<xsd:element name="TaxStatus" type="xsd:string" minOccurs="0"/>
													<xsd:element name="Translated_TaxStatus" type="xsd:string" minOccurs="0"/>
