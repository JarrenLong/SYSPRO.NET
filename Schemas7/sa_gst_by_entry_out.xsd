																<xsd:element name="EditedGSTTaxable" minOccurs="0"/>
																<xsd:element name="GSTExempt" minOccurs="0"/>
																<xsd:element name="EditedGSTExempt" minOccurs="0"/>
																<xsd:element name="GSTTotal" minOccurs="0"/>
