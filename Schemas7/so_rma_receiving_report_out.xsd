<!-- 
     Schema for the RMA Daily Receiving Query Business Object
-->
<xsd:schema id="RMADailyReceiving" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="RMADailyReceiving" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CustomerFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_CustomerFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CustomerFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RMAReceivedDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_RMAReceivedDateFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RMAReceivedDateFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RMANumberFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_RMANumberFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RMANumberFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RMAProblemCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_RMAProblemCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="RMAProblemCodeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_StockCodeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="StockCodeFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="RMADetails">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="RMADetail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="RMAHeaderInfo" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="Name" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="LineDetails" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="LineDetail" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Customer" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="RMANumber" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="RMAAction" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="RMAActionDesc" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_RMAActionDesc" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="StockCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Translated_Description" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="SalePerson" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Name" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="RMAuom" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Quantity" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="EditedQuantity" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ProblemCode" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="ProblemDesc" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Price" type="xsd:double" minOccurs="0"/>
                                <xsd:element name="EditedPrice" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Stockinguom" type="xsd:string" minOccurs="0"/>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="CustomerTotals" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="TotalUnits" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedTotalUnits" type="xsd:string" minOccurs="0"/>
                          <xsd:element name="TotalValue" type="xsd:double" minOccurs="0"/>
                          <xsd:element name="EditedTotalValue" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ReportTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="ReportTotalUnits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedReportTotalUnits" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportTotalValue" type="xsd:double" minOccurs="0"/>
              <xsd:element name="EditedReportTotalValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
