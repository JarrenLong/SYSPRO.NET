<!-- 
     Schema for the AP Amendment Journal Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="APAmendmentJournalReport" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="APAmendmentJournalReport" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_Reprint" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintAdditions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintAdditions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintFullAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintFullAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintChanges" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintChanges" type="xsd:string" minOccurs="0"/>
              <xsd:element name="PrintDeletions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_PrintDeletions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DeleteJournalReport" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_DeleteJournalReport" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Amendments">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Amendment" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="ChangeFlag" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Translated_ChangeFlag" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JnlDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JnlTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="JnlLine" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Supplier" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="APSMST_Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ColumnName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BeforeAmendment" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AfterAmendment" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ADMOPR_Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupAddr1" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupAddr2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupAddr3" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupAddr3Loc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupAddr4" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupAddr5" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupPostalCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SupGpsLat" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="SupGpsLong" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="RemitName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RemitAddr1" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RemitAddr2" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RemitAddr3" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RemitAddr3Loc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RemitAddr4" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RemitAddr5" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RemitPostalCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="RemitGpsLat" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="RemitGpsLong" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Report_SequencingFormula" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="FileDeleted">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Delete" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CompanyTotals">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="TotalAdditions" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotalChanges" type="xsd:string" minOccurs="0"/>
              <xsd:element name="TotalDeletions" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
