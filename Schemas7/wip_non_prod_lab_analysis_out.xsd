                                <xsd:element name="Edited_QtyScrapped" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="ValueEntered" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_ValueEntered" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="HoursEntered" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_HoursEntered" type="xsd:string" minOccurs="0"></xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="WorkCenterTotal" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="WorkCenterQtyScrapped" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_WorkCenterQtyScrapped" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="WorkCenterValueEntered" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_WorkCenterValueEntered" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="WorkCenterHoursEntered" type="xsd:string" minOccurs="0"></xsd:element>
                                <xsd:element name="Edited_WorkCenterHoursEntered" type="xsd:string" minOccurs="0"></xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="NonProdScrapTotal" minOccurs="0" maxOccurs="unbounded">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="NonProdScrapQtyScrapped" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_NonProdScrapQtyScrapped" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="NonProdScrapValueEntered" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_NonProdScrapValueEntered" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="NonProdScrapHoursEntered" type="xsd:string" minOccurs="0"></xsd:element>
                          <xsd:element name="Edited_NonProdScrapHoursEntered" type="xsd:string" minOccurs="0"></xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="ReportTotals" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="TotalQtyScrapped" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_TotalQtyScrapped" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="TotalValueEntered" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_TotalValueEntered" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="TotalHoursEntered" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Edited_TotalHoursEntered" type="xsd:string" minOccurs="0"></xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
