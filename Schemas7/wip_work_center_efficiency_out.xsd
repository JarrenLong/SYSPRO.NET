<!-- 
     Schema for the WIP Work Center Efficiency Query Business Object
-->
<xsd:schema elementFormDefault="unqualified" attributeFormDefault="unqualified" id="Query" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xsd:element name="Query" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="SystemInformation">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CssStyle" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Language" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DecFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="DateFormat" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Role" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Version" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyId" type="xsd:string" minOccurs="0"/>
              <xsd:element name="CompanyName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorCode" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorName" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorGroup" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorEmailAddress" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorLocation" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
              <xsd:element name="LocalCurrencyDesc" type="xsd:string" minOccurs="0"/>
              <xsd:element name="GlPrdPerYear" type="xsd:string" minOccurs="0"/>
              <xsd:element name="ReportDate" type="xsd:string" minOccurs="0"/>
              <xsd:element name="UseSeparatorOnReports" type="xsd:string" minOccurs="0"/>
              <xsd:element name="SeparatorToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="OperatorPrimaryRole" type="xsd:string" minOccurs="0"/>
              <xsd:element name="NoOfDigits" type="xsd:double" minOccurs="0"/>
              <xsd:element name="DecCharToUse" type="xsd:string" minOccurs="0"/>
              <xsd:element name="BOMQtyPerDecimals" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="QueryOptions">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="UnitOfMeasure" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_UnitOfMeasure" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WorkCenterTypeFilterType" type="xsd:string" minOccurs="0"/>
              <xsd:element name="WorkCenterTypeFilterValue" type="xsd:string" minOccurs="0"/>
              <xsd:element name="Translated_WorkCenterTypeFilterValue" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="WorkCenterEfficiency">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Detail" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="WorkCenter" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WorkCenterDescription" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PtdStandardTimeEarned" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_PtdStandardTimeEarned" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PtdNonProductiveTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_PtdNonProductiveTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PtdCapacityAvailable" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_PtdCapacityAvailable" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PtdLostTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_PtdLostTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PtdActualProductionTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_PtdActualProductionTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PtdPercentageProductionUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="PtdPercentageTotalUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="PtdPercentageEfficiency" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MtdStandardTimeEarned" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_MtdStandardTimeEarned" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MtdNonProductiveTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_MtdNonProductiveTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MtdCapacityAvailable" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_MtdCapacityAvailable" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MtdLostTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_MtdLostTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MtdActualProductionTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_MtdActualProductionTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MtdPercentageProductionUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MtdPercentageTotalUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="MtdPercentageEfficiency" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="YtdStandardTimeEarned" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_YtdStandardTimeEarned" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="YtdNonProductiveTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_YtdNonProductiveTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="YtdCapacityAvailable" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_YtdCapacityAvailable" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="YtdLostTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_YtdLostTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="YtdActualProductionTime" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="Edited_YtdActualProductionTime" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="YtdPercentageProductionUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="YtdPercentageTotalUtilised" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="YtdPercentageEfficiency" type="xsd:double" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
